\s5
\v 18 Alors le \w roi|strong="H4428"\w* \w dit|strong="H559" x-morph="strongMorph:TH8799"\w* à \w Doëg|strong="H1673"\w*: \w Tourne|strong="H5437" x-morph="strongMorph:TH8798"\w*-toi, et \w frappe|strong="H6293" x-morph="strongMorph:TH8798"\w* les \w sacrificateurs|strong="H3548"\w*. Et \w Doëg|strong="H1673"\w*, l'\w Édomite|strong="H130"\w*, se \w tourna|strong="H5437" x-morph="strongMorph:TH8735"\w*, et ce fut lui qui \w frappa|strong="H6293" x-morph="strongMorph:TH8799"\w* les \w sacrificateurs|strong="H3548"\w*; il fit \w mourir|strong="H4191" x-morph="strongMorph:TH8686"\w* en ce \w jour|strong="H3117"\w* quatre-\w vingt|strong="H8084"\w*-\w cinq|strong="H2568"\w* \w hommes|strong="H376"\w* \w portant|strong="H5375" x-morph="strongMorph:TH8802"\w* l'\w éphod|strong="H646"\w* de \w lin|strong="H906"\w*. 
\v 19 Saül \w frappa|strong="H5221" x-morph="strongMorph:TH8689"\w* encore du \w tranchant|strong="H6310"\w* de l'\w épée|strong="H2719"\w* \w Nob|strong="H5011"\w*, \w ville|strong="H5892"\w* \w sacerdotale|strong="H3548"\w*; \w hommes|strong="H376"\w* et \w femmes|strong="H802"\w*, \w enfants|strong="H5768"\w* et \w nourrissons|strong="H3243" x-morph="strongMorph:TH8802"\w*, \w boeufs|strong="H7794"\w*, \w ânes|strong="H2543"\w*, et \w brebis|strong="H7716"\w*, tombèrent sous le \w tranchant|strong="H6310"\w* de l'\w épée|strong="H2719"\w*. 

\s5
\v 20  \w Un|strong="H259"\w* \w fils|strong="H1121"\w* d'\w Achimélec|strong="H288"\w*, \w fils|strong="H1121"\w* d'\w Achithub|strong="H285"\w*, \w échappa|strong="H4422" x-morph="strongMorph:TH8735"\w*. Son \w nom|strong="H8034"\w* était \w Abiathar|strong="H54"\w*. Il s'\w enfuit|strong="H1272" x-morph="strongMorph:TH8799"\w* \w auprès|strong="H310"\w* de \w David|strong="H1732"\w*, 
\v 21  \w et|strong="H54"\w* \w lui|strong="H1732"\w* \w rapporta|strong="H5046" x-morph="strongMorph:TH8686"\w* que \w Saül|strong="H7586"\w* avait \w tué|strong="H2026" x-morph="strongMorph:TH8804"\w* les \w sacrificateurs|strong="H3548"\w* de l'\w Éternel|strong="H3068"\w*. 

\s5
