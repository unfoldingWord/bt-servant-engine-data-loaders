\mt1 PREMIER LIVRE DE SAMUEL 

\s5
\c 1  
\p
\v 1 Il y avait \w un|strong="H259"\w* \w homme|strong="H376"\w* de Ramathaïm-\w Tsophim|strong="H7436"\w*, de la \w montagne|strong="H2022"\w* d'\w Éphraïm|strong="H669"\w*, \w nommé|strong="H8034"\w* \w Elkana|strong="H511"\w*, \w fils|strong="H1121"\w* de \w Jeroham|strong="H3395"\w*, \w fils|strong="H1121"\w* d'\w Élihu|strong="H453"\w*, \w fils|strong="H1121"\w* de \w Thohu|strong="H8459"\w*, \w fils|strong="H1121"\w* de \w Tsuph|strong="H6689"\w*, \w Éphratien|strong="H673"\w*. 
\v 2 Il avait \w deux|strong="H8147"\w* \w femmes|strong="H802"\w*, dont l'\w une|strong="H259"\w* s'\w appelait|strong="H8034"\w* \w Anne|strong="H2584"\w*, et l'\w autre|strong="H8034"\w*\w |strong="H8145"\w* \w Peninna|strong="H6444"\w*; \w Peninna|strong="H6444"\w* avait des \w enfants|strong="H3206"\w*, mais \w Anne|strong="H2584"\w* n'en avait \w point|strong="H3206"\w*. 

\s5
\v 3 Chaque \w année|strong="H3117"\w*\w |strong="H3117"\w*, \w cet|strong="H1931"\w* \w homme|strong="H376"\w* \w montait|strong="H5927" x-morph="strongMorph:TH8804"\w* de sa \w ville|strong="H5892"\w* à \w Silo|strong="H7887"\w*, pour se \w prosterner|strong="H7812" x-morph="strongMorph:TH8692"\w* devant l'\w Éternel|strong="H3068"\w* des \w armées|strong="H6635"\w* et pour lui offrir des \w sacrifices|strong="H2076" x-morph="strongMorph:TH8800"\w*. Là se trouvaient les \w deux|strong="H8147"\w* \w fils|strong="H1121"\w* d'\w Éli|strong="H5941"\w*, \w Hophni|strong="H2652"\w* et \w Phinées|strong="H6372"\w*, \w sacrificateurs|strong="H3548"\w* de l'\w Éternel|strong="H3068"\w*. 
\v 4 Le \w jour|strong="H3117"\w* où \w Elkana|strong="H511"\w* \w offrait|strong="H2076" x-morph="strongMorph:TH8799"\w* son sacrifice, il \w donnait|strong="H5414" x-morph="strongMorph:TH8804"\w* des \w portions|strong="H4490"\w* à \w Peninna|strong="H6444"\w*, sa \w femme|strong="H802"\w*, et à tous les \w fils|strong="H1121"\w* et à toutes les \w filles|strong="H1323"\w* qu'il avait d'elle. 

\s5
\v 5 Mais il \w donnait|strong="H5414" x-morph="strongMorph:TH8799"\w* à \w Anne|strong="H2584"\w* \w une|strong="H259"\w* \w portion|strong="H4490"\w* \w double|strong="H639"\w*; car il \w aimait|strong="H157" x-morph="strongMorph:TH8804"\w* \w Anne|strong="H2584"\w*, que l'\w Éternel|strong="H3068"\w* avait rendue \w stérile|strong="H5462" x-morph="strongMorph:TH8804"\w*\w |strong="H7358"\w*. 
\v 6 Sa \w rivale|strong="H6869"\w* lui \w prodiguait|strong="H3707" x-morph="strongMorph:TH8765"\w* les \w mortifications|strong="H3708"\w*, pour la porter à s'\w irriter|strong="H7481" x-morph="strongMorph:TH8687"\w* de ce que l'\w Éternel|strong="H3068"\w* l'avait rendue \w stérile|strong="H5462" x-morph="strongMorph:TH8804"\w*\w |strong="H1157"\w*\w |strong="H7358"\w*. 

\s5
\v 7 Et toutes les \w années|strong="H8141"\w*\w |strong="H8141"\w* il en \w était|strong="H6213" x-morph="strongMorph:TH8799"\w* ainsi. Chaque \w fois|strong="H1767"\w* qu'Anne \w montait|strong="H5927" x-morph="strongMorph:TH8800"\w* à la \w maison|strong="H1004"\w* de l'\w Éternel|strong="H3068"\w*, Peninna la \w mortifiait|strong="H3707" x-morph="strongMorph:TH8686"\w* de la même manière. Alors elle \w pleurait|strong="H1058" x-morph="strongMorph:TH8799"\w* et ne \w mangeait|strong="H398" x-morph="strongMorph:TH8799"\w* point. 
\v 8  \w Elkana|strong="H511"\w*, son \w mari|strong="H376"\w*, lui \w disait|strong="H559" x-morph="strongMorph:TH8799"\w*: \w Anne|strong="H2584"\w*, pourquoi \w pleures|strong="H1058" x-morph="strongMorph:TH8799"\w*-tu, et ne \w manges|strong="H398" x-morph="strongMorph:TH8799"\w*-tu pas? pourquoi ton \w coeur|strong="H3824"\w* est-il \w attristé|strong="H3415" x-morph="strongMorph:TH8799"\w*? Est-ce que je ne vaux pas pour toi \w mieux|strong="H2896"\w* que \w dix|strong="H6235"\w* \w fils|strong="H1121"\w*? 

\s5
\v 9  \w Anne|strong="H2584"\w* se \w leva|strong="H6965" x-morph="strongMorph:TH8799"\w*, \w après|strong="H310"\w* que l'on eut \w mangé|strong="H398" x-morph="strongMorph:TH8800"\w* \w et|strong="H310"\w* \w bu|strong="H8354" x-morph="strongMorph:TH8800"\w* à \w Silo|strong="H7887"\w*. Le \w sacrificateur|strong="H3548"\w* \w Éli|strong="H5941"\w* était \w assis|strong="H3427" x-morph="strongMorph:TH8802"\w* sur un \w siège|strong="H3678"\w*, près de l'un des \w poteaux|strong="H4201"\w* du \w temple|strong="H1964"\w* de l'\w Éternel|strong="H3068"\w*. 
\v 10 Et, l'\w amertume|strong="H4751"\w* dans l'\w âme|strong="H5315"\w*, elle \w pria|strong="H6419" x-morph="strongMorph:TH8691"\w* l'\w Éternel|strong="H3068"\w* et versa des \w pleurs|strong="H1058" x-morph="strongMorph:TH8799"\w*\w |strong="H1058" x-morph="strongMorph:TH8800"\w*. 

\s5
\v 11 Elle \w fit|strong="H5087" x-morph="strongMorph:TH8799"\w* un \w voeu|strong="H5088"\w*, en \w disant|strong="H559" x-morph="strongMorph:TH8799"\w*: \w Éternel|strong="H3068"\w* des \w armées|strong="H6635"\w*! si tu daignes \w regarder|strong="H7200" x-morph="strongMorph:TH8800"\w*\w |strong="H7200" x-morph="strongMorph:TH8799"\w* l'\w affliction|strong="H6040"\w* de ta \w servante|strong="H519"\w*, si tu te \w souviens|strong="H2142" x-morph="strongMorph:TH8804"\w* de moi et n'\w oublies|strong="H7911" x-morph="strongMorph:TH8799"\w* point ta \w servante|strong="H519"\w*, et si tu \w donnes|strong="H5414" x-morph="strongMorph:TH8804"\w* à ta \w servante|strong="H519"\w* un \w enfant|strong="H2233"\w* \w mâle|strong="H582"\w*, je le \w consacrerai|strong="H5414" x-morph="strongMorph:TH8804"\w* à l'\w Éternel|strong="H3068"\w* pour tous les \w jours|strong="H3117"\w* de sa \w vie|strong="H2416"\w*, et le \w rasoir|strong="H4177"\w* ne \w passera|strong="H5927" x-morph="strongMorph:TH8799"\w* point sur sa \w tête|strong="H7218"\w*. 

\s5
\v 12 Comme elle restait \w longtemps|strong="H7235" x-morph="strongMorph:TH8689"\w* en \w prière|strong="H6419" x-morph="strongMorph:TH8692"\w* \w devant|strong="H6440"\w* l'\w Éternel|strong="H3068"\w*, \w Éli|strong="H5941"\w* \w observa|strong="H8104" x-morph="strongMorph:TH8802"\w* sa \w bouche|strong="H6310"\w*. 
\v 13  \w Anne|strong="H2584"\w* \w parlait|strong="H1696" x-morph="strongMorph:TH8764"\w* dans son \w coeur|strong="H3820"\w*, et ne faisait que \w remuer|strong="H5128" x-morph="strongMorph:TH8801"\w* les \w lèvres|strong="H8193"\w*, mais on n'\w entendait|strong="H8085" x-morph="strongMorph:TH8735"\w* point sa \w voix|strong="H6963"\w*. \w Éli|strong="H5941"\w* \w pensa|strong="H2803" x-morph="strongMorph:TH8799"\w* qu'elle était \w ivre|strong="H7910"\w*, 
\v 14 et \w il|strong="H5941"\w* lui \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: Jusques à quand seras-tu dans l'\w ivresse|strong="H7937" x-morph="strongMorph:TH8691"\w*? Fais \w passer|strong="H5493" x-morph="strongMorph:TH8685"\w* ton \w vin|strong="H3196"\w*. 

\s5
\v 15  \w Anne|strong="H2584"\w* \w répondit|strong="H6030" x-morph="strongMorph:TH8799"\w*\w |strong="H559" x-morph="strongMorph:TH8799"\w*: Non, mon \w seigneur|strong="H113"\w*, je suis une \w femme|strong="H802"\w* qui \w souffre|strong="H7186"\w* en son \w coeur|strong="H7307"\w*, et je n'ai \w bu|strong="H8354" x-morph="strongMorph:TH8804"\w* ni \w vin|strong="H3196"\w* ni boisson \w enivrante|strong="H7941"\w*; mais je \w répandais|strong="H8210" x-morph="strongMorph:TH8799"\w* mon \w âme|strong="H5315"\w* \w devant|strong="H6440"\w* l'\w Éternel|strong="H3068"\w*. 
\v 16 Ne \w prends|strong="H5414" x-morph="strongMorph:TH8799"\w* pas ta \w servante|strong="H519"\w* \w pour|strong="H6440"\w* une \w femme|strong="H1323"\w* \w pervertie|strong="H1100"\w*, car c'est l'\w excès|strong="H7230"\w* de ma \w douleur|strong="H7879"\w* et de mon \w chagrin|strong="H3708"\w* qui m'a fait \w parler|strong="H1696" x-morph="strongMorph:TH8765"\w* jusqu'à présent. 

\s5
\v 17  \w Éli|strong="H5941"\w* reprit la \w parole|strong="H6030" x-morph="strongMorph:TH8799"\w*, et \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: \w Va|strong="H3212" x-morph="strongMorph:TH8798"\w* en \w paix|strong="H7965"\w*, et que le \w Dieu|strong="H430"\w* d'\w Israël|strong="H3478"\w* \w exauce|strong="H5414" x-morph="strongMorph:TH8799"\w* la \w prière|strong="H7596"\w* que tu lui as \w adressée|strong="H7592" x-morph="strongMorph:TH8804"\w*! 
\v 18 Elle \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: Que ta \w servante|strong="H8198"\w* \w trouve|strong="H4672" x-morph="strongMorph:TH8799"\w* \w grâce|strong="H2580"\w* à tes \w yeux|strong="H5869"\w*! Et cette \w femme|strong="H802"\w* s'en \w alla|strong="H3212" x-morph="strongMorph:TH8799"\w*\w |strong="H1870"\w*. Elle \w mangea|strong="H398" x-morph="strongMorph:TH8799"\w*, et son \w visage|strong="H6440"\w* ne fut plus le même. 

\s5
\v 19 Ils se \w levèrent|strong="H7925" x-morph="strongMorph:TH8686"\w* de bon \w matin|strong="H1242"\w*, et après s'être \w prosternés|strong="H7812" x-morph="strongMorph:TH8691"\w* \w devant|strong="H6440"\w* l'\w Éternel|strong="H3068"\w*, ils s'en \w retournèrent|strong="H7725" x-morph="strongMorph:TH8799"\w* et \w revinrent|strong="H935" x-morph="strongMorph:TH8799"\w* dans leur \w maison|strong="H1004"\w* à \w Rama|strong="H7414"\w*. \w Elkana|strong="H511"\w* \w connut|strong="H3045" x-morph="strongMorph:TH8799"\w* \w Anne|strong="H2584"\w*, sa \w femme|strong="H802"\w*, et l'\w Éternel|strong="H3068"\w* se \w souvint|strong="H2142" x-morph="strongMorph:TH8799"\w* d'elle. 
\v 20 Dans le \w cours|strong="H8622"\w* de l'\w année|strong="H3117"\w*, \w Anne|strong="H2584"\w* devint \w enceinte|strong="H2029" x-morph="strongMorph:TH8799"\w*, et elle \w enfanta|strong="H3205" x-morph="strongMorph:TH8799"\w* un \w fils|strong="H1121"\w*, qu'elle \w nomma|strong="H7121" x-morph="strongMorph:TH8799"\w*\w |strong="H8034"\w* \w Samuel|strong="H8050"\w*, car, dit-elle, je l'ai \w demandé|strong="H7592" x-morph="strongMorph:TH8804"\w* à l'\w Éternel|strong="H3068"\w*. 

\s5
\v 21 Son \w mari|strong="H376"\w* \w Elkana|strong="H511"\w* \w monta|strong="H5927" x-morph="strongMorph:TH8799"\w* ensuite avec toute sa \w maison|strong="H1004"\w*, pour \w offrir|strong="H2076" x-morph="strongMorph:TH8800"\w* à l'\w Éternel|strong="H3068"\w* le \w sacrifice|strong="H2077"\w* \w annuel|strong="H3117"\w*, et pour accomplir son \w voeu|strong="H5088"\w*. 
\v 22 Mais \w Anne|strong="H2584"\w* ne \w monta|strong="H5927" x-morph="strongMorph:TH8804"\w* point, et elle \w dit|strong="H559" x-morph="strongMorph:TH8804"\w* à son \w mari|strong="H376"\w*: Lorsque l'\w enfant|strong="H5288"\w* sera \w sevré|strong="H1580" x-morph="strongMorph:TH8735"\w*, je le \w mènerai|strong="H935" x-morph="strongMorph:TH8689"\w*, afin qu'il soit \w présenté|strong="H7200" x-morph="strongMorph:TH8738"\w* \w devant|strong="H6440"\w* l'\w Éternel|strong="H3068"\w* et qu'il \w reste|strong="H3427" x-morph="strongMorph:TH8804"\w* là \w pour|strong="H5704"\w* \w toujours|strong="H5769"\w*. 
\v 23  \w Elkana|strong="H511"\w*, son \w mari|strong="H376"\w*, lui \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: \w Fais|strong="H6213" x-morph="strongMorph:TH8798"\w* ce qui te \w semblera|strong="H5869"\w* \w bon|strong="H2896"\w*, \w attends|strong="H3427" x-morph="strongMorph:TH8798"\w* de l'avoir \w sevré|strong="H1580" x-morph="strongMorph:TH8800"\w*. Veuille seulement l'\w Éternel|strong="H3068"\w* \w accomplir|strong="H6965" x-morph="strongMorph:TH8686"\w* sa \w parole|strong="H1697"\w*! Et la \w femme|strong="H802"\w* \w resta|strong="H3427" x-morph="strongMorph:TH8799"\w* et \w allaita|strong="H3243" x-morph="strongMorph:TH8686"\w* son \w fils|strong="H1121"\w*, jusqu'à ce qu'elle le \w sevrât|strong="H1580" x-morph="strongMorph:TH8800"\w*. 

\s5
\v 24 Quand elle l'eut \w sevré|strong="H1580" x-morph="strongMorph:TH8804"\w*, elle le fit \w monter|strong="H5927" x-morph="strongMorph:TH8686"\w* avec elle, et prit \w trois|strong="H7969"\w* \w taureaux|strong="H6499"\w*, \w un|strong="H259"\w* \w épha|strong="H374"\w* de \w farine|strong="H7058"\w*, et une \w outre|strong="H5035"\w* de \w vin|strong="H3196"\w*. Elle le \w mena|strong="H935" x-morph="strongMorph:TH8686"\w* dans la \w maison|strong="H1004"\w* de l'\w Éternel|strong="H3068"\w* à \w Silo|strong="H7887"\w*: l'\w enfant|strong="H5288"\w* était encore tout \w jeune|strong="H5288"\w*. 
\v 25 Ils \w égorgèrent|strong="H7819" x-morph="strongMorph:TH8799"\w* les \w taureaux|strong="H6499"\w*, et ils \w conduisirent|strong="H935" x-morph="strongMorph:TH8686"\w* l'\w enfant|strong="H5288"\w* à \w Éli|strong="H5941"\w*. 

\s5
\v 26 Anne \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: Mon \w seigneur|strong="H113"\w*, \w pardon|strong="H994"\w*! aussi vrai que ton \w âme|strong="H5315"\w* \w vit|strong="H2416"\w*, mon \w seigneur|strong="H113"\w*, je suis cette \w femme|strong="H802"\w* qui me \w tenais|strong="H5324" x-morph="strongMorph:TH8737"\w* ici près de toi pour \w prier|strong="H6419" x-morph="strongMorph:TH8692"\w* l'\w Éternel|strong="H3068"\w*. 
\v 27 C'était pour cet \w enfant|strong="H5288"\w* que je \w priais|strong="H6419" x-morph="strongMorph:TH8694"\w*, et l'\w Éternel|strong="H3068"\w* a \w exaucé|strong="H5414" x-morph="strongMorph:TH8799"\w* la \w prière|strong="H7596"\w* que je lui \w adressais|strong="H7592" x-morph="strongMorph:TH8804"\w*. 
\v 28 Aussi je veux le \w prêter|strong="H7592" x-morph="strongMorph:TH8689"\w* à l'\w Éternel|strong="H3068"\w*: il sera toute sa \w vie|strong="H3117"\w* \w prêté|strong="H7592" x-morph="strongMorph:TH8803"\w* à l'\w Éternel|strong="H3068"\w*. Et ils se \w prosternèrent|strong="H7812" x-morph="strongMorph:TH8691"\w* là devant l'\w Éternel|strong="H3068"\w*. 

\s5
\c 2  
\p
\v 1  \w Anne|strong="H2584"\w* \w pria|strong="H6419" x-morph="strongMorph:TH8691"\w*, et \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: Mon \w coeur|strong="H3820"\w* se \w réjouit|strong="H5970" x-morph="strongMorph:TH8804"\w* en l'\w Éternel|strong="H3068"\w*, Ma \w force|strong="H7161"\w* a été \w relevée|strong="H7311" x-morph="strongMorph:TH8804"\w* par l'\w Éternel|strong="H3068"\w*; Ma \w bouche|strong="H6310"\w* s'est \w ouverte|strong="H7337" x-morph="strongMorph:TH8804"\w* contre mes \w ennemis|strong="H341" x-morph="strongMorph:TH8802"\w*, Car je me \w réjouis|strong="H8055" x-morph="strongMorph:TH8804"\w* de ton \w secours|strong="H3444"\w*. 

\s5
\v 2 Nul n'est \w saint|strong="H6918"\w* comme l'\w Éternel|strong="H3068"\w*; Il n'y a point d'\w autre|strong="H1115"\w* Dieu que toi; Il n'y a point de \w rocher|strong="H6697"\w* comme notre \w Dieu|strong="H430"\w*. 

\s5
\v 3 Ne \w parlez|strong="H1696" x-morph="strongMorph:TH8762"\w* \w plus|strong="H7235" x-morph="strongMorph:TH8686"\w* avec \w tant|strong="H1364"\w* de \w hauteur|strong="H1364"\w*; Que l'\w arrogance|strong="H6277"\w* ne \w sorte|strong="H3318" x-morph="strongMorph:TH8799"\w* plus de votre \w bouche|strong="H6310"\w*; Car l'\w Éternel|strong="H3068"\w* est un \w Dieu|strong="H410"\w* qui \w sait|strong="H1844"\w* tout, Et par lui sont \w pesées|strong="H8505" x-morph="strongMorph:TH8738"\w* toutes les \w actions|strong="H5949"\w*. 
\v 4 L'\w arc|strong="H7198"\w* des \w puissants|strong="H1368"\w* est \w brisé|strong="H2844"\w*, Et les \w faibles|strong="H3782" x-morph="strongMorph:TH8737"\w* ont la \w force|strong="H2428"\w* pour \w ceinture|strong="H247" x-morph="strongMorph:TH8804"\w*. 

\s5
\v 5 Ceux qui étaient \w rassasiés|strong="H7649"\w* se \w louent|strong="H7936" x-morph="strongMorph:TH8738"\w* pour du \w pain|strong="H3899"\w*, Et ceux qui étaient \w affamés|strong="H7457"\w* se \w reposent|strong="H2308" x-morph="strongMorph:TH8804"\w*; Même la \w stérile|strong="H6135"\w* \w enfante|strong="H3205" x-morph="strongMorph:TH8804"\w* \w sept|strong="H7651"\w* fois, Et celle qui avait \w beaucoup|strong="H7227"\w* d'\w enfants|strong="H1121"\w* est \w flétrie|strong="H535" x-morph="strongMorph:TH8797"\w*. 

\s5
\v 6 L'\w Éternel|strong="H3068"\w* fait \w mourir|strong="H4191" x-morph="strongMorph:TH8688"\w* et il fait \w vivre|strong="H2421" x-morph="strongMorph:TH8764"\w*. Il fait \w descendre|strong="H3381" x-morph="strongMorph:TH8688"\w* au séjour des \w morts|strong="H7585"\w* et il en fait \w remonter|strong="H5927" x-morph="strongMorph:TH8686"\w*. 
\v 7 L'\w Éternel|strong="H3068"\w* \w appauvrit|strong="H3423" x-morph="strongMorph:TH8688"\w* et il \w enrichit|strong="H6238" x-morph="strongMorph:TH8688"\w*, Il \w abaisse|strong="H8213" x-morph="strongMorph:TH8688"\w* \w et|strong="H637"\w* il \w élève|strong="H7311" x-morph="strongMorph:TH8789"\w*. 

\s5
\v 8 De la \w poussière|strong="H6083"\w* il \w retire|strong="H6965" x-morph="strongMorph:TH8688"\w* le \w pauvre|strong="H1800"\w*, Du \w fumier|strong="H830"\w* il \w relève|strong="H7311" x-morph="strongMorph:TH8686"\w* l'\w indigent|strong="H34"\w*, Pour les faire \w asseoir|strong="H3427" x-morph="strongMorph:TH8687"\w* avec les \w grands|strong="H5081"\w*. Et il leur donne en \w partage|strong="H5157" x-morph="strongMorph:TH8686"\w* un \w trône|strong="H3678"\w* de \w gloire|strong="H3519"\w*; Car à l'\w Éternel|strong="H3068"\w* sont les \w colonnes|strong="H4690"\w* de la \w terre|strong="H776"\w*, Et c'est sur elles qu'il a \w posé|strong="H7896" x-morph="strongMorph:TH8799"\w* le \w monde|strong="H8398"\w*. 

\s5
\v 9 Il \w gardera|strong="H8104" x-morph="strongMorph:TH8799"\w* les \w pas|strong="H7272"\w* de ses bien-\w aimés|strong="H2623"\w*. Mais les \w méchants|strong="H7563"\w* seront \w anéantis|strong="H1826" x-morph="strongMorph:TH8735"\w* dans les \w ténèbres|strong="H2822"\w*; Car l'\w homme|strong="H376"\w* ne \w triomphera|strong="H1396" x-morph="strongMorph:TH8799"\w* point par la \w force|strong="H3581"\w*. 

\s5
