\s5
\c 36  
\p
\v 1 Les \w chefs|strong="H7218"\w* de la \w famille|strong="H1"\w*\w |strong="H4940"\w*\w |strong="H1121"\w* de \w Galaad|strong="H1568"\w*, \w fils|strong="H1121"\w* de \w Makir|strong="H4353"\w*, \w fils|strong="H1121"\w* de \w Manassé|strong="H4519"\w*, d'entre les \w familles|strong="H4940"\w* des \w fils|strong="H1121"\w* de \w Joseph|strong="H3130"\w*, s'\w approchèrent|strong="H7126" x-morph="strongMorph:TH8799"\w* et \w parlèrent|strong="H1696" x-morph="strongMorph:TH8762"\w* \w devant|strong="H6440"\w* \w Moïse|strong="H4872"\w* et \w devant|strong="H6440"\w* les \w princes|strong="H5387"\w*, \w chefs|strong="H7218"\w* de \w famille|strong="H1"\w* des \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w*. 
\v 2 Ils \w dirent|strong="H559" x-morph="strongMorph:TH8799"\w*: L'\w Éternel|strong="H3068"\w* a \w ordonné|strong="H6680" x-morph="strongMorph:TH8765"\w* à mon \w seigneur|strong="H113"\w* de \w donner|strong="H5414" x-morph="strongMorph:TH8800"\w* le \w pays|strong="H776"\w* en \w héritage|strong="H5159"\w* par le \w sort|strong="H1486"\w* aux \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w*. Mon \w seigneur|strong="H113"\w* a aussi reçu de l'\w Éternel|strong="H3068"\w* l'\w ordre|strong="H6680" x-morph="strongMorph:TH8795"\w* de \w donner|strong="H5414" x-morph="strongMorph:TH8800"\w* l'\w héritage|strong="H5159"\w* de \w Tselophchad|strong="H6765"\w*, notre \w frère|strong="H251"\w*, à ses \w filles|strong="H1323"\w*. 

\s5
\v 3 Si elles se \w marient|strong="H802"\w* à l'\w un|strong="H259"\w* des \w fils|strong="H1121"\w* d'une autre \w tribu|strong="H7626"\w* des \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w*, leur \w héritage|strong="H5159"\w* sera \w retranché|strong="H1639" x-morph="strongMorph:TH8738"\w* de l'\w héritage|strong="H5159"\w* de nos \w pères|strong="H1"\w* et \w ajouté|strong="H3254" x-morph="strongMorph:TH8738"\w* à \w celui|strong="H5159"\w* de la \w tribu|strong="H4294"\w* à laquelle elles appartiendront; ainsi sera \w diminué|strong="H1639" x-morph="strongMorph:TH8735"\w* l'\w héritage|strong="H5159"\w* qui nous est échu par le \w sort|strong="H1486"\w*. 
\v 4 Et quand viendra le \w jubilé|strong="H3104"\w* pour les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w*, leur \w héritage|strong="H5159"\w* sera \w ajouté|strong="H3254" x-morph="strongMorph:TH8738"\w* à \w celui|strong="H5159"\w* de la \w tribu|strong="H4294"\w* à laquelle elles appartiendront, et \w il|strong="H5159"\w* sera \w retranché|strong="H1639" x-morph="strongMorph:TH8735"\w* de \w celui|strong="H5159"\w* de la \w tribu|strong="H4294"\w* de nos \w pères|strong="H1"\w*. 

\s5
\v 5  \w Moïse|strong="H4872"\w* \w transmit|strong="H6680"\w*\w \w* aux \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w* les \w ordres|strong="H6680" x-morph="strongMorph:TH8762"\w*\w |strong="H6310"\w* de l'\w Éternel|strong="H3068"\w*. Il \w dit|strong="H559" x-morph="strongMorph:TH8800"\w*: La \w tribu|strong="H4294"\w* des \w fils|strong="H1121"\w* de \w Joseph|strong="H3130"\w* a \w raison|strong="H1696" x-morph="strongMorph:TH8802"\w*. 
\v 6  \w Voici|strong="H1697"\w* ce que l'\w Éternel|strong="H3068"\w* \w ordonne|strong="H6680" x-morph="strongMorph:TH8765"\w*\w |strong="H559" x-morph="strongMorph:TH8800"\w* au sujet des \w filles|strong="H1323"\w* de \w Tselophchad|strong="H6765"\w*: elles se \w marieront|strong="H802"\w* à qui elles \w voudront|strong="H5869"\w*\w |strong="H2896"\w*, pourvu qu'elles se \w marient|strong="H802"\w* dans une \w famille|strong="H4940"\w* de la \w tribu|strong="H4294"\w* de leurs \w pères|strong="H1"\w*. 

\s5
