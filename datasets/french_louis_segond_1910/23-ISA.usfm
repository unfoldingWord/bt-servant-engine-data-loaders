\mt1 ISAÏE 

\s5
\c 1  
\p
\v 1  \w Prophétie|strong="H2377"\w* d'\w Ésaïe|strong="H3470"\w*, \w fils|strong="H1121"\w* d'\w Amots|strong="H531"\w*, \w sur|strong="H2372" x-morph="strongMorph:TH8804"\w* \w Juda|strong="H3063"\w* et \w Jérusalem|strong="H3389"\w*, au \w temps|strong="H3117"\w* d'\w Ozias|strong="H5818"\w*, de \w Jotham|strong="H3147"\w*, d'\w Achaz|strong="H271"\w*, d'\w Ézéchias|strong="H3169"\w*, \w rois|strong="H4428"\w* de \w Juda|strong="H3063"\w*. 

\s5
\v 2  \w Cieux|strong="H8064"\w*, \w écoutez|strong="H8085" x-morph="strongMorph:TH8798"\w*! \w terre|strong="H776"\w*, prête l'\w oreille|strong="H238" x-morph="strongMorph:TH8685"\w*! Car l'\w Éternel|strong="H3068"\w* \w parle|strong="H1696" x-morph="strongMorph:TH8765"\w*. J'ai \w nourri|strong="H1431" x-morph="strongMorph:TH8765"\w* et \w élevé|strong="H7311" x-morph="strongMorph:TH8790"\w* des \w enfants|strong="H1121"\w*, Mais ils se sont \w révoltés|strong="H6586" x-morph="strongMorph:TH8804"\w* contre moi. 
\v 3 Le \w boeuf|strong="H7794"\w* \w connaît|strong="H3045" x-morph="strongMorph:TH8804"\w* son \w possesseur|strong="H7069" x-morph="strongMorph:TH8802"\w*, Et l'\w âne|strong="H2543"\w* la \w crèche|strong="H18"\w* de son \w maître|strong="H1167"\w*: \w Israël|strong="H3478"\w* ne \w connaît|strong="H3045" x-morph="strongMorph:TH8804"\w* rien, Mon \w peuple|strong="H5971"\w* n'a point d'\w intelligence|strong="H995" x-morph="strongMorph:TH8712"\w*. 

\s5
\v 4  \w Malheur|strong="H1945"\w* à la \w nation|strong="H1471"\w* \w pécheresse|strong="H2398" x-morph="strongMorph:TH8802"\w*, au \w peuple|strong="H5971"\w* \w chargé|strong="H3515"\w* d'\w iniquités|strong="H5771"\w*, À la \w race|strong="H2233"\w* des \w méchants|strong="H7489" x-morph="strongMorph:TH8688"\w*, aux \w enfants|strong="H1121"\w* \w corrompus|strong="H7843" x-morph="strongMorph:TH8688"\w*! Ils ont \w abandonné|strong="H5800" x-morph="strongMorph:TH8804"\w*\w |strong="H853"\w* l'\w Éternel|strong="H3068"\w*, ils ont \w méprisé|strong="H853"\w*\w |strong="H5006" x-morph="strongMorph:TH8765"\w* le \w Saint|strong="H6918"\w* d'\w Israël|strong="H3478"\w*. Ils se sont \w retirés|strong="H2114" x-morph="strongMorph:TH8738"\w* en \w arrière|strong="H268"\w*... 

\s5
\v 5 Quels châtiments nouveaux vous \w infliger|strong="H5221" x-morph="strongMorph:TH8714"\w*, Quand vous \w multipliez|strong="H3254" x-morph="strongMorph:TH8686"\w* vos \w révoltes|strong="H5627"\w*? La \w tête|strong="H7218"\w* entière est \w malade|strong="H2483"\w*, Et tout le \w coeur|strong="H3824"\w* est \w souffrant|strong="H1742"\w*. 
\v 6 De la \w plante|strong="H3709"\w* du \w pied|strong="H7272"\w* jusqu'à la \w tête|strong="H7218"\w*, rien n'est en bon \w état|strong="H4974"\w*: Ce ne sont que \w blessures|strong="H6482"\w*, \w contusions|strong="H2250"\w* et \w plaies|strong="H4347"\w* \w vives|strong="H2961"\w*, Qui n'ont été ni \w pansées|strong="H2115" x-morph="strongMorph:TH8804"\w*, ni \w bandées|strong="H2280" x-morph="strongMorph:TH8795"\w*, Ni \w adoucies|strong="H7401" x-morph="strongMorph:TH8795"\w* par l'\w huile|strong="H8081"\w*. 

\s5
\v 7 Votre \w pays|strong="H776"\w* est \w dévasté|strong="H8077"\w*, Vos \w villes|strong="H5892"\w* sont \w consumées|strong="H8313" x-morph="strongMorph:TH8803"\w* par le \w feu|strong="H784"\w*, Des \w étrangers|strong="H2114" x-morph="strongMorph:TH8801"\w* \w dévorent|strong="H398" x-morph="strongMorph:TH8802"\w* vos \w campagnes|strong="H127"\w* sous vos yeux, Ils \w ravagent|strong="H8077"\w* et \w détruisent|strong="H4114"\w*, comme des \w barbares|strong="H2114" x-morph="strongMorph:TH8801"\w*. 
\v 8 Et la \w fille|strong="H1323"\w* de \w Sion|strong="H6726"\w* est \w restée|strong="H3498" x-morph="strongMorph:TH8738"\w* Comme une \w cabane|strong="H5521"\w* dans une \w vigne|strong="H3754"\w*, Comme une \w hutte|strong="H4412"\w* dans un champ de \w concombres|strong="H4750"\w*, Comme une \w ville|strong="H5892"\w* \w épargnée|strong="H5341" x-morph="strongMorph:TH8803"\w*. 

\s5
\v 9  \w Si|strong="H3884"\w* l'\w Éternel|strong="H3068"\w* des \w armées|strong="H6635"\w* Ne nous eût \w conservé|strong="H3498" x-morph="strongMorph:TH8689"\w* un \w faible|strong="H4592"\w* \w reste|strong="H8300"\w*, Nous \w serions|strong="H1961" x-morph="strongMorph:TH8804"\w* comme \w Sodome|strong="H5467"\w*, Nous \w ressemblerions|strong="H1819" x-morph="strongMorph:TH8804"\w* à \w Gomorrhe|strong="H6017"\w*. 

\s5
\v 10  \w Écoutez|strong="H8085" x-morph="strongMorph:TH8798"\w* la \w parole|strong="H1697"\w* de l'\w Éternel|strong="H3068"\w*, \w chefs|strong="H7101"\w* de \w Sodome|strong="H5467"\w*! Prête l'\w oreille|strong="H238" x-morph="strongMorph:TH8685"\w* à la \w loi|strong="H8451"\w* de notre \w Dieu|strong="H430"\w*, \w peuple|strong="H5971"\w* de \w Gomorrhe|strong="H6017"\w*! 
\v 11 Qu'ai-je \w affaire|strong="H4100"\w* de la \w multitude|strong="H7230"\w* de vos \w sacrifices|strong="H2077"\w*? \w dit|strong="H559" x-morph="strongMorph:TH8799"\w* l'\w Éternel|strong="H3068"\w*. Je suis \w rassasié|strong="H7646" x-morph="strongMorph:TH8804"\w* des \w holocaustes|strong="H5930"\w* de \w béliers|strong="H352"\w* et de la \w graisse|strong="H2459"\w* des \w veaux|strong="H4806"\w*; Je ne prends point \w plaisir|strong="H2654" x-morph="strongMorph:TH8804"\w* au \w sang|strong="H1818"\w* des \w taureaux|strong="H6499"\w*, des \w brebis|strong="H3532"\w* et des \w boucs|strong="H6260"\w*. 

\s5
\v 12 Quand vous \w venez|strong="H935" x-morph="strongMorph:TH8799"\w* vous \w présenter|strong="H7200" x-morph="strongMorph:TH8736"\w* \w devant|strong="H6440"\w* moi, Qui vous \w demande|strong="H1245" x-morph="strongMorph:TH8765"\w*\w |strong="H3027"\w* de \w souiller|strong="H7429" x-morph="strongMorph:TH8800"\w* mes \w parvis|strong="H2691"\w*? 
\v 13  \w Cessez|strong="H3254" x-morph="strongMorph:TH8686"\w* d'\w apporter|strong="H935" x-morph="strongMorph:TH8687"\w* de \w vaines|strong="H7723"\w* \w offrandes|strong="H4503"\w*: J'ai en \w horreur|strong="H8441"\w* l'\w encens|strong="H7004"\w*, Les nouvelles \w lunes|strong="H2320"\w*, les \w sabbats|strong="H7676"\w* et les \w assemblées|strong="H7121" x-morph="strongMorph:TH8800"\w*\w |strong="H4744"\w*; Je ne puis \w voir|strong="H3201" x-morph="strongMorph:TH8799"\w* le \w crime|strong="H205"\w* s'associer aux \w solennités|strong="H6116"\w*. 

\s5
\v 14 Mon \w âme|strong="H5315"\w* \w hait|strong="H8130" x-morph="strongMorph:TH8804"\w* vos nouvelles \w lunes|strong="H4150"\w*\w |strong="H2320"\w* et vos fêtes; Elles me sont à \w charge|strong="H2960"\w*; Je suis \w las|strong="H3811" x-morph="strongMorph:TH8738"\w* de les \w supporter|strong="H5375" x-morph="strongMorph:TH8800"\w*. 
\v 15 Quand vous \w étendez|strong="H6566" x-morph="strongMorph:TH8763"\w* vos \w mains|strong="H3709"\w*, je \w détourne|strong="H5956" x-morph="strongMorph:TH8686"\w* de vous mes \w yeux|strong="H5869"\w*; Quand vous \w multipliez|strong="H7235" x-morph="strongMorph:TH8686"\w* les \w prières|strong="H8605"\w*, je n'\w écoute|strong="H8085" x-morph="strongMorph:TH8802"\w* pas: Vos \w mains|strong="H3027"\w* sont \w pleines|strong="H4390" x-morph="strongMorph:TH8804"\w* de \w sang|strong="H1818"\w*. 

\s5
\v 16  \w Lavez|strong="H7364" x-morph="strongMorph:TH8798"\w*-vous, \w purifiez|strong="H2135" x-morph="strongMorph:TH8690"\w*-vous, \w Otez|strong="H5493" x-morph="strongMorph:TH8685"\w* de \w devant|strong="H5048"\w* mes \w yeux|strong="H5869"\w* la \w méchanceté|strong="H7455"\w* de vos \w actions|strong="H4611"\w*; \w Cessez|strong="H2308" x-morph="strongMorph:TH8798"\w* de faire le \w mal|strong="H7489" x-morph="strongMorph:TH8687"\w*. 
\v 17  \w Apprenez|strong="H3925" x-morph="strongMorph:TH8798"\w* à faire le \w bien|strong="H3190" x-morph="strongMorph:TH8687"\w*, \w recherchez|strong="H1875" x-morph="strongMorph:TH8798"\w* la \w justice|strong="H4941"\w*, \w Protégez|strong="H833" x-morph="strongMorph:TH8761"\w* l'\w opprimé|strong="H2541"\w*; Faites \w droit|strong="H8199" x-morph="strongMorph:TH8798"\w* à l'\w orphelin|strong="H3490"\w*, \w Défendez|strong="H7378" x-morph="strongMorph:TH8798"\w* la \w veuve|strong="H490"\w*. 

\s5
\v 18  \w Venez|strong="H3212" x-morph="strongMorph:TH8798"\w* et \w plaidons|strong="H3198" x-morph="strongMorph:TH8735"\w*! \w dit|strong="H559" x-morph="strongMorph:TH8799"\w* l'\w Éternel|strong="H3068"\w*. Si vos \w péchés|strong="H2399"\w* sont comme le \w cramoisi|strong="H8144"\w*, ils deviendront \w blancs|strong="H3835" x-morph="strongMorph:TH8686"\w* comme la \w neige|strong="H7950"\w*; S'ils sont \w rouges|strong="H119" x-morph="strongMorph:TH8686"\w* comme la \w pourpre|strong="H8438"\w*, ils deviendront comme la \w laine|strong="H6785"\w*. 

\s5
\v 19 Si vous avez de la bonne \w volonté|strong="H14" x-morph="strongMorph:TH8799"\w* et si vous êtes \w dociles|strong="H8085" x-morph="strongMorph:TH8804"\w*, Vous \w mangerez|strong="H398" x-morph="strongMorph:TH8799"\w* les \w meilleures|strong="H2898"\w* productions du \w pays|strong="H776"\w*; 
\v 20 Mais si vous \w résistez|strong="H3985" x-morph="strongMorph:TH8762"\w* et si vous êtes \w rebelles|strong="H4784" x-morph="strongMorph:TH8804"\w*, Vous serez \w dévorés|strong="H398" x-morph="strongMorph:TH8792"\w* par le \w glaive|strong="H2719"\w*, Car la \w bouche|strong="H6310"\w* de l'\w Éternel|strong="H3068"\w* a \w parlé|strong="H1696" x-morph="strongMorph:TH8765"\w*. 

\s5
\v 21 Quoi donc! la \w cité|strong="H7151"\w* \w fidèle|strong="H539" x-morph="strongMorph:TH8737"\w* est devenue une \w prostituée|strong="H2181" x-morph="strongMorph:TH8802"\w*! Elle était \w remplie|strong="H4392"\w* d'\w équité|strong="H4941"\w*, la \w justice|strong="H6664"\w* y \w habitait|strong="H3885" x-morph="strongMorph:TH8799"\w*, Et maintenant il y a des \w assassins|strong="H7523" x-morph="strongMorph:TH8764"\w*! 
\v 22 Ton \w argent|strong="H3701"\w* s'est changé en \w scories|strong="H5509"\w*, Ton \w vin|strong="H5435"\w* a été \w coupé|strong="H4107" x-morph="strongMorph:TH8803"\w* d'\w eau|strong="H4325"\w*. 

\s5
\v 23 Tes \w chefs|strong="H8269"\w* sont \w rebelles|strong="H5637" x-morph="strongMorph:TH8802"\w* et \w complices|strong="H2270"\w* des \w voleurs|strong="H1590"\w*, Tous \w aiment|strong="H157" x-morph="strongMorph:TH8802"\w* les \w présents|strong="H7810"\w* et \w courent|strong="H7291" x-morph="strongMorph:TH8802"\w* après les \w récompenses|strong="H8021"\w*; Ils ne font pas \w droit|strong="H8199" x-morph="strongMorph:TH8799"\w* à l'\w orphelin|strong="H3490"\w*, Et la \w cause|strong="H7379"\w* de la \w veuve|strong="H490"\w* ne \w vient|strong="H935" x-morph="strongMorph:TH8799"\w* pas jusqu'à eux. 

\s5
\v 24 C'est pourquoi voici ce que \w dit|strong="H5002" x-morph="strongMorph:TH8803"\w* le \w Seigneur|strong="H113"\w*, l'\w Éternel|strong="H3068"\w* des \w armées|strong="H6635"\w*, Le \w Fort|strong="H46"\w* d'\w Israël|strong="H3478"\w*: \w Ah|strong="H1945"\w*! je tirerai \w satisfaction|strong="H5162" x-morph="strongMorph:TH8735"\w* de mes \w adversaires|strong="H6862"\w*, Et je me \w vengerai|strong="H5358" x-morph="strongMorph:TH8735"\w* de mes \w ennemis|strong="H341" x-morph="strongMorph:TH8802"\w*. 
\v 25 Je \w porterai|strong="H7725" x-morph="strongMorph:TH8686"\w* ma \w main|strong="H3027"\w* sur toi, Je \w fondrai|strong="H6884" x-morph="strongMorph:TH8799"\w* tes \w scories|strong="H5509"\w*, comme avec de la \w potasse|strong="H1253"\w*, Et j'\w enlèverai|strong="H5493" x-morph="strongMorph:TH8686"\w* toutes tes parcelles de \w plomb|strong="H913"\w*. 

\s5
\v 26 Je \w rétablirai|strong="H7725" x-morph="strongMorph:TH8686"\w* tes \w juges|strong="H8199" x-morph="strongMorph:TH8802"\w* tels qu'ils étaient \w autrefois|strong="H7223"\w*, Et tes \w conseillers|strong="H3289" x-morph="strongMorph:TH8802"\w* tels qu'ils étaient au \w commencement|strong="H8462"\w*. \w Après|strong="H310"\w* cela, on t'\w appellera|strong="H7121" x-morph="strongMorph:TH8735"\w* \w ville|strong="H5892"\w* de la \w justice|strong="H6664"\w*, \w Cité|strong="H7151"\w* \w fidèle|strong="H539" x-morph="strongMorph:TH8737"\w*. 

\s5
\v 27  \w Sion|strong="H6726"\w* sera \w sauvée|strong="H6299" x-morph="strongMorph:TH8735"\w* par la \w droiture|strong="H4941"\w*, Et ceux qui s'y \w convertiront|strong="H7725" x-morph="strongMorph:TH8802"\w* seront sauvés par la \w justice|strong="H6666"\w*. 
\v 28 Mais la \w ruine|strong="H7667"\w* atteindra \w tous|strong="H3162"\w* les \w rebelles|strong="H6586" x-morph="strongMorph:TH8802"\w* et les \w pécheurs|strong="H2400"\w*, Et ceux qui \w abandonnent|strong="H5800" x-morph="strongMorph:TH8802"\w* l'\w Éternel|strong="H3068"\w* \w périront|strong="H3615" x-morph="strongMorph:TH8799"\w*. 

\s5
\v 29 On aura \w honte|strong="H954" x-morph="strongMorph:TH8799"\w* à cause des \w térébinthes|strong="H352"\w* auxquels vous prenez \w plaisir|strong="H2530" x-morph="strongMorph:TH8804"\w*, Et vous \w rougirez|strong="H2659" x-morph="strongMorph:TH8799"\w* à cause des \w jardins|strong="H1593"\w* dont vous faites vos \w délices|strong="H977" x-morph="strongMorph:TH8804"\w*; 
\v 30 Car vous serez comme un \w térébinthe|strong="H424"\w* au \w feuillage|strong="H5929"\w* \w flétri|strong="H5034" x-morph="strongMorph:TH8802"\w*, Comme un \w jardin|strong="H1593"\w* qui n'a pas d'\w eau|strong="H4325"\w*. 

\s5
\v 31 L'homme \w fort|strong="H2634"\w* sera comme de l'\w étoupe|strong="H5296"\w*, Et son \w oeuvre|strong="H6467"\w* comme une \w étincelle|strong="H5213"\w*; Ils \w brûleront|strong="H1197" x-morph="strongMorph:TH8804"\w* l'un et l'\w autre|strong="H8147"\w* \w ensemble|strong="H3162"\w*, Et il n'y aura personne pour \w éteindre|strong="H3518" x-morph="strongMorph:TH8764"\w*. 

\s5
\c 2  
\p
\v 1  \w Prophétie|strong="H1697"\w* d'\w Ésaïe|strong="H3470"\w*, \w fils|strong="H1121"\w* d'\w Amots|strong="H531"\w*,\w |strong="H2372" x-morph="strongMorph:TH8804"\w* sur \w Juda|strong="H3063"\w* et \w Jérusalem|strong="H3389"\w*. 
\v 2 Il arrivera, dans la \w suite|strong="H319"\w* des \w temps|strong="H3117"\w*, Que la \w montagne|strong="H2022"\w* de la \w maison|strong="H1004"\w* de l'\w Éternel|strong="H3068"\w* Sera \w fondée|strong="H3559" x-morph="strongMorph:TH8737"\w* sur le \w sommet|strong="H7218"\w* des \w montagnes|strong="H2022"\w*, Qu'elle s'\w élèvera|strong="H5375" x-morph="strongMorph:TH8737"\w* par-dessus les \w collines|strong="H1389"\w*, Et que toutes les \w nations|strong="H1471"\w* y \w afflueront|strong="H5102" x-morph="strongMorph:TH8804"\w*. 

\s5
\v 3 Des \w peuples|strong="H5971"\w* s'y \w rendront|strong="H1980" x-morph="strongMorph:TH8804"\w* en \w foule|strong="H7227"\w*, et \w diront|strong="H559" x-morph="strongMorph:TH8804"\w*: \w Venez|strong="H3212" x-morph="strongMorph:TH8798"\w*, et \w montons|strong="H5927" x-morph="strongMorph:TH8799"\w* à la \w montagne|strong="H2022"\w* de l'\w Éternel|strong="H3068"\w*, À la \w maison|strong="H1004"\w* du \w Dieu|strong="H430"\w* de \w Jacob|strong="H3290"\w*, Afin qu'il nous \w enseigne|strong="H3384" x-morph="strongMorph:TH8686"\w* ses \w voies|strong="H1870"\w*, Et que nous \w marchions|strong="H3212" x-morph="strongMorph:TH8799"\w* dans ses \w sentiers|strong="H734"\w*. Car de \w Sion|strong="H6726"\w* \w sortira|strong="H3318" x-morph="strongMorph:TH8799"\w* la \w loi|strong="H8451"\w*, Et de \w Jérusalem|strong="H3389"\w* la \w parole|strong="H1697"\w* de l'\w Éternel|strong="H3068"\w*. 

\s5
\v 4 Il sera le \w juge|strong="H8199" x-morph="strongMorph:TH8804"\w* des \w nations|strong="H1471"\w*, L'\w arbitre|strong="H3198" x-morph="strongMorph:TH8689"\w* d'un grand \w nombre|strong="H7227"\w* de \w peuples|strong="H5971"\w*. De leurs \w glaives|strong="H2719"\w* ils \w forgeront|strong="H3807" x-morph="strongMorph:TH8765"\w* des \w hoyaux|strong="H855"\w*, Et de leurs \w lances|strong="H2595"\w* des \w serpes|strong="H4211"\w*: Une \w nation|strong="H1471"\w* ne \w tirera|strong="H5375" x-morph="strongMorph:TH8799"\w* plus l'\w épée|strong="H2719"\w* contre une \w autre|strong="H1471"\w*, Et l'on n'\w apprendra|strong="H3925" x-morph="strongMorph:TH8799"\w* plus la \w guerre|strong="H4421"\w*. 

\s5
\v 5  \w Maison|strong="H1004"\w* de \w Jacob|strong="H3290"\w*, \w Venez|strong="H3212" x-morph="strongMorph:TH8798"\w*, et \w marchons|strong="H3212" x-morph="strongMorph:TH8799"\w* à la \w lumière|strong="H216"\w* de l'\w Éternel|strong="H3068"\w*! 
\v 6 Car tu as \w abandonné|strong="H5203" x-morph="strongMorph:TH8804"\w* ton \w peuple|strong="H5971"\w*, la \w maison|strong="H1004"\w* de \w Jacob|strong="H3290"\w*, Parce qu'ils sont \w pleins|strong="H4390" x-morph="strongMorph:TH8804"\w* de l'\w Orient|strong="H6924"\w*, Et adonnés à la \w magie|strong="H6049" x-morph="strongMorph:TH8781"\w* comme les \w Philistins|strong="H6430"\w*, Et parce qu'ils s'\w allient|strong="H5606" x-morph="strongMorph:TH8686"\w* aux \w fils|strong="H3206"\w* des \w étrangers|strong="H5237"\w*. 

\s5
\v 7 Le \w pays|strong="H776"\w* est \w rempli|strong="H4390" x-morph="strongMorph:TH8735"\w* d'\w argent|strong="H3701"\w* et d'\w or|strong="H2091"\w*, Et il y a des \w trésors|strong="H214"\w* sans \w fin|strong="H7097"\w*; Le \w pays|strong="H776"\w* est \w rempli|strong="H4390" x-morph="strongMorph:TH8735"\w* de \w chevaux|strong="H5483"\w*, Et il y a des \w chars|strong="H4818"\w* sans \w nombre|strong="H7097"\w*. 
\v 8 Le \w pays|strong="H776"\w* est \w rempli|strong="H4390" x-morph="strongMorph:TH8735"\w* d'\w idoles|strong="H457"\w*; Ils se \w prosternent|strong="H7812" x-morph="strongMorph:TH8691"\w* devant l'\w ouvrage|strong="H4639"\w* de leurs \w mains|strong="H3027"\w*, Devant ce que leurs \w doigts|strong="H676"\w* ont \w fabriqué|strong="H6213" x-morph="strongMorph:TH8804"\w*. 

\s5
\v 9 Les \w petits|strong="H120"\w* seront \w abattus|strong="H7817" x-morph="strongMorph:TH8735"\w*, et les \w grands|strong="H376"\w* seront \w abaissés|strong="H8213" x-morph="strongMorph:TH8799"\w*: Tu ne leur \w pardonneras|strong="H5375" x-morph="strongMorph:TH8799"\w* point. 
\v 10  \w Entre|strong="H935" x-morph="strongMorph:TH8798"\w* dans les \w rochers|strong="H6697"\w*, Et \w cache|strong="H2934" x-morph="strongMorph:TH8734"\w*-toi dans la \w poussière|strong="H6083"\w*, \w Pour|strong="H6440"\w* éviter la \w terreur|strong="H6343"\w* de l'\w Éternel|strong="H3068"\w* Et l'\w éclat|strong="H1926"\w* de sa \w majesté|strong="H1347"\w*. 
\v 11 L'\w homme|strong="H120"\w* au \w regard|strong="H5869"\w* \w hautain|strong="H1365"\w* sera \w abaissé|strong="H8213" x-morph="strongMorph:TH8804"\w*, Et l'\w orgueilleux|strong="H7312"\w*\w |strong="H582"\w* sera \w humilié|strong="H7817" x-morph="strongMorph:TH8804"\w*: L'\w Éternel|strong="H3068"\w* seul sera \w élevé|strong="H7682" x-morph="strongMorph:TH8738"\w* ce \w jour|strong="H3117"\w*-là. 

\s5
\v 12 Car il y a un \w jour|strong="H3117"\w* pour l'\w Éternel|strong="H3068"\w* des \w armées|strong="H6635"\w* Contre tout homme \w orgueilleux|strong="H1343"\w* et \w hautain|strong="H7311" x-morph="strongMorph:TH8802"\w*, Contre quiconque s'\w élève|strong="H5375" x-morph="strongMorph:TH8737"\w*, afin qu'il soit \w abaissé|strong="H8213" x-morph="strongMorph:TH8804"\w*; 
\v 13 Contre tous les \w cèdres|strong="H730"\w* du \w Liban|strong="H3844"\w*, \w hauts|strong="H7311" x-morph="strongMorph:TH8802"\w* et \w élevés|strong="H5375" x-morph="strongMorph:TH8737"\w*, Et contre tous les \w chênes|strong="H437"\w* de \w Basan|strong="H1316"\w*; 

\s5
\v 14 Contre toutes les \w hautes|strong="H7311" x-morph="strongMorph:TH8802"\w* \w montagnes|strong="H2022"\w*, Et contre toutes les \w collines|strong="H1389"\w* \w élevées|strong="H5375" x-morph="strongMorph:TH8737"\w*; 
\v 15 Contre toutes les \w hautes|strong="H1364"\w* \w tours|strong="H4026"\w*, Et contre toutes les \w murailles|strong="H2346"\w* \w fortifiées|strong="H1219" x-morph="strongMorph:TH8803"\w*; 
\v 16 Contre tous les \w navires|strong="H591"\w* de \w Tarsis|strong="H8659"\w*, Et contre tout ce qui \w plaît|strong="H2532"\w* à la \w vue|strong="H7914"\w*. 

\s5
\v 17 L'\w homme|strong="H120"\w* \w orgueilleux|strong="H1365"\w* sera \w humilié|strong="H7817" x-morph="strongMorph:TH8804"\w*, Et le \w hautain|strong="H7312"\w*\w |strong="H582"\w* sera \w abaissé|strong="H8213" x-morph="strongMorph:TH8804"\w*: L'\w Éternel|strong="H3068"\w* seul sera \w élevé|strong="H7682" x-morph="strongMorph:TH8738"\w* ce \w jour|strong="H3117"\w*-là. 
\v 18 Toutes les \w idoles|strong="H457"\w* \w disparaîtront|strong="H3632"\w*\w |strong="H2498" x-morph="strongMorph:TH8799"\w*. 
\v 19 On \w entrera|strong="H935" x-morph="strongMorph:TH8804"\w* dans les \w cavernes|strong="H4631"\w* des \w rochers|strong="H6697"\w* Et dans les \w profondeurs|strong="H4247"\w* de la \w poussière|strong="H6083"\w*,\w |strong="H6440"\w* Pour éviter la \w terreur|strong="H6343"\w* de l'\w Éternel|strong="H3068"\w* et l'\w éclat|strong="H1926"\w* de sa \w majesté|strong="H1347"\w*, Quand il se \w lèvera|strong="H6965" x-morph="strongMorph:TH8800"\w* pour \w effrayer|strong="H6206" x-morph="strongMorph:TH8800"\w* la \w terre|strong="H776"\w*. 

\s5
\v 20 En ce \w jour|strong="H3117"\w*, les \w hommes|strong="H120"\w* \w jetteront|strong="H7993" x-morph="strongMorph:TH8686"\w* Leurs \w idoles|strong="H457"\w* d'\w argent|strong="H3701"\w* et leurs \w idoles|strong="H457"\w* d'\w or|strong="H2091"\w*, Qu'ils s'étaient \w faites|strong="H6213" x-morph="strongMorph:TH8804"\w* pour les \w adorer|strong="H7812" x-morph="strongMorph:TH8692"\w*, Aux \w rats|strong="H2661"\w*\w |strong="H6512"\w* et aux chauves-\w souris|strong="H5847"\w*; 
\v 21 Et ils \w entreront|strong="H935" x-morph="strongMorph:TH8800"\w* dans les \w fentes|strong="H5366"\w* des \w rochers|strong="H6697"\w* Et dans les \w creux|strong="H5585"\w* des \w pierres|strong="H5553"\w*, \w Pour|strong="H6440"\w* éviter la \w terreur|strong="H6343"\w* de l'\w Éternel|strong="H3068"\w* et l'\w éclat|strong="H1926"\w* de sa \w majesté|strong="H1347"\w*, Quand il se \w lèvera|strong="H6965" x-morph="strongMorph:TH8800"\w* pour \w effrayer|strong="H6206" x-morph="strongMorph:TH8800"\w* la \w terre|strong="H776"\w*. 
\v 22  \w Cessez|strong="H2308" x-morph="strongMorph:TH8798"\w* de vous confier en l'\w homme|strong="H120"\w*, Dans les \w narines|strong="H639"\w* duquel il n'y a qu'un \w souffle|strong="H5397"\w*: Car de quelle \w valeur|strong="H2803" x-morph="strongMorph:TH8737"\w* est-il? 

\s5
\c 3  
\p
\v 1 Le \w Seigneur|strong="H113"\w*, l'\w Éternel|strong="H3068"\w* des \w armées|strong="H6635"\w*, Va \w ôter|strong="H5493" x-morph="strongMorph:TH8688"\w* de \w Jérusalem|strong="H3389"\w* et de \w Juda|strong="H3063"\w* Tout \w appui|strong="H4937"\w* et toute \w ressource|strong="H4938"\w*, Toute \w ressource|strong="H4937"\w* de \w pain|strong="H3899"\w* Et \w toute|strong="H3605"\w* \w ressource|strong="H4937"\w* d'\w eau|strong="H4325"\w*, 
\v 2 Le \w héros|strong="H1368"\w* et l'\w homme|strong="H376"\w* de \w guerre|strong="H4421"\w*, Le \w juge|strong="H8199" x-morph="strongMorph:TH8802"\w* et le \w prophète|strong="H5030"\w*, le \w devin|strong="H7080" x-morph="strongMorph:TH8802"\w* et l'\w ancien|strong="H2205"\w*, 
\v 3 Le \w chef|strong="H8269"\w* de \w cinquante|strong="H2572"\w* et le \w magistrat|strong="H5375" x-morph="strongMorph:TH8803"\w*,\w |strong="H6440"\w* Le \w conseiller|strong="H3289" x-morph="strongMorph:TH8802"\w*, l'\w artisan|strong="H2791"\w* \w distingué|strong="H2450"\w* et l'\w habile|strong="H995" x-morph="strongMorph:TH8737"\w* \w enchanteur|strong="H3908"\w*. 

\s5
\v 4 Je leur \w donnerai|strong="H5414" x-morph="strongMorph:TH8804"\w* des jeunes \w gens|strong="H5288"\w* pour \w chefs|strong="H8269"\w*, Et des \w enfants|strong="H8586"\w* \w domineront|strong="H4910" x-morph="strongMorph:TH8799"\w* sur eux. 
\v 5 Il y aura \w réciprocité|strong="H376"\w*\w |strong="H376"\w* d'\w oppression|strong="H5065" x-morph="strongMorph:TH8738"\w* parmi le \w peuple|strong="H5971"\w*; L'\w un|strong="H376"\w* opprimera l'\w autre|strong="H7453"\w*, chacun son prochain; Le jeune \w homme|strong="H5288"\w* \w attaquera|strong="H7292" x-morph="strongMorph:TH8799"\w* le \w vieillard|strong="H2205"\w*, Et l'homme de \w rien|strong="H7034" x-morph="strongMorph:TH8737"\w* celui qui est \w honoré|strong="H3513" x-morph="strongMorph:TH8737"\w*. 

\s5
\v 6  \w On|strong="H376"\w* ira jusqu'à \w saisir|strong="H8610" x-morph="strongMorph:TH8799"\w* son \w frère|strong="H251"\w* dans la \w maison|strong="H1004"\w* \w paternelle|strong="H1"\w*: Tu as un \w habit|strong="H8071"\w*, sois notre \w chef|strong="H7101"\w*! Prends ces \w ruines|strong="H4384"\w* sous ta \w main|strong="H3027"\w*! - 
\v 7 Ce \w jour|strong="H3117"\w*-là même il \w répondra|strong="H5375" x-morph="strongMorph:TH8799"\w*\w |strong="H559" x-morph="strongMorph:TH8800"\w*: Je ne saurais être un \w médecin|strong="H2280" x-morph="strongMorph:TH8802"\w*, Et dans ma \w maison|strong="H1004"\w* il n'y a ni \w pain|strong="H3899"\w* ni \w vêtement|strong="H8071"\w*; Ne m'\w établissez|strong="H7760" x-morph="strongMorph:TH8799"\w* pas \w chef|strong="H7101"\w* du \w peuple|strong="H5971"\w*! 

\s5
\v 8  \w Jérusalem|strong="H3389"\w* \w chancelle|strong="H3782" x-morph="strongMorph:TH8804"\w*, Et \w Juda|strong="H3063"\w* s'\w écroule|strong="H5307" x-morph="strongMorph:TH8804"\w*, Parce que leurs \w paroles|strong="H3956"\w* et leurs \w oeuvres|strong="H4611"\w* sont contre l'\w Éternel|strong="H3068"\w*, \w Bravant|strong="H4784" x-morph="strongMorph:TH8687"\w* les \w regards|strong="H5869"\w* de sa \w majesté|strong="H3519"\w*. 
\v 9 L'\w aspect|strong="H1971"\w* de leur \w visage|strong="H6440"\w* \w témoigne|strong="H6030" x-morph="strongMorph:TH8804"\w* contre eux, Et, comme \w Sodome|strong="H5467"\w*, ils \w publient|strong="H5046" x-morph="strongMorph:TH8689"\w* leur \w crime|strong="H2403"\w*, sans \w dissimuler|strong="H3582" x-morph="strongMorph:TH8765"\w*. \w Malheur|strong="H188"\w* à leur \w âme|strong="H5315"\w*! Car ils se \w préparent|strong="H1580" x-morph="strongMorph:TH8804"\w* des \w maux|strong="H7451"\w*. 

\s5
\v 10  \w Dites|strong="H559" x-morph="strongMorph:TH8798"\w* que le \w juste|strong="H6662"\w* \w prospérera|strong="H2896"\w*, Car il \w jouira|strong="H398" x-morph="strongMorph:TH8799"\w* du \w fruit|strong="H6529"\w* de ses \w oeuvres|strong="H4611"\w*. 
\v 11  \w Malheur|strong="H188"\w* au \w méchant|strong="H7563"\w*! il sera dans l'\w infortune|strong="H7451"\w*, Car il \w recueillera|strong="H6213" x-morph="strongMorph:TH8735"\w* le \w produit|strong="H1576"\w* de ses \w mains|strong="H3027"\w*. 
\v 12 Mon \w peuple|strong="H5971"\w* a pour \w oppresseurs|strong="H5065" x-morph="strongMorph:TH8802"\w* des \w enfants|strong="H5953" x-morph="strongMorph:TH8781"\w*, Et des \w femmes|strong="H802"\w* \w dominent|strong="H4910" x-morph="strongMorph:TH8804"\w* sur lui; Mon \w peuple|strong="H5971"\w*, ceux qui te \w conduisent|strong="H833" x-morph="strongMorph:TH8764"\w* t'\w égarent|strong="H8582" x-morph="strongMorph:TH8688"\w*, Et ils \w corrompent|strong="H1104" x-morph="strongMorph:TH8765"\w* la \w voie|strong="H1870"\w* dans laquelle tu \w marches|strong="H734"\w*. 

\s5
\v 13 L'\w Éternel|strong="H3068"\w* se \w présente|strong="H5324" x-morph="strongMorph:TH8737"\w* pour \w plaider|strong="H7378" x-morph="strongMorph:TH8800"\w*, Il est \w debout|strong="H5975" x-morph="strongMorph:TH8802"\w* pour \w juger|strong="H1777" x-morph="strongMorph:TH8800"\w* les \w peuples|strong="H5971"\w*. 
\v 14 L'\w Éternel|strong="H3068"\w* \w entre|strong="H935" x-morph="strongMorph:TH8799"\w* en \w jugement|strong="H4941"\w* Avec les \w anciens|strong="H2205"\w* de son \w peuple|strong="H5971"\w* et avec ses \w chefs|strong="H8269"\w*: Vous avez \w brouté|strong="H1197" x-morph="strongMorph:TH8765"\w* la \w vigne|strong="H3754"\w*! La \w dépouille|strong="H1500"\w* du \w pauvre|strong="H6041"\w* est dans vos \w maisons|strong="H1004"\w*! 
\v 15 De quel droit \w foulez|strong="H1792" x-morph="strongMorph:TH8762"\w*-vous mon \w peuple|strong="H5971"\w*, Et \w écrasez|strong="H2912" x-morph="strongMorph:TH8799"\w*-vous la \w face|strong="H6440"\w* des \w pauvres|strong="H6041"\w*? \w Dit|strong="H5002" x-morph="strongMorph:TH8803"\w* le \w Seigneur|strong="H136"\w*, l'\w Éternel|strong="H3069"\w* des \w armées|strong="H6635"\w*. 

\s5
\v 16 L'\w Éternel|strong="H3068"\w* \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: Parce \w que|strong="H3282"\w* les \w filles|strong="H1323"\w* de \w Sion|strong="H6726"\w* sont \w orgueilleuses|strong="H1361" x-morph="strongMorph:TH8804"\w*, Et qu'elles \w marchent|strong="H3212" x-morph="strongMorph:TH8799"\w* le \w cou|strong="H1627"\w* \w tendu|strong="H5186" x-morph="strongMorph:TH8803"\w* Et les \w regards|strong="H5869"\w* \w effrontés|strong="H8265" x-morph="strongMorph:TH8764"\w*, Parce qu'elles \w vont|strong="H1980" x-morph="strongMorph:TH8800"\w*\w |strong="H3212" x-morph="strongMorph:TH8799"\w* à petits \w pas|strong="H2952" x-morph="strongMorph:TH8800"\w*, Et qu'elles font résonner les \w boucles|strong="H5913" x-morph="strongMorph:TH8762"\w* de leurs \w pieds|strong="H7272"\w*, 
\v 17 Le \w Seigneur|strong="H136"\w* rendra \w chauve|strong="H5596" x-morph="strongMorph:TH8765"\w* le sommet de la \w tête|strong="H6936"\w* des \w filles|strong="H1323"\w* de \w Sion|strong="H6726"\w*, L'\w Éternel|strong="H3068"\w* \w découvrira|strong="H6168" x-morph="strongMorph:TH8762"\w* leur \w nudité|strong="H6596"\w*. 

\s5
\v 18 En ce \w jour|strong="H3117"\w*, le \w Seigneur|strong="H136"\w* \w ôtera|strong="H5493" x-morph="strongMorph:TH8686"\w* les \w boucles|strong="H8597"\w* qui servent d'\w ornement|strong="H5914"\w* à leurs pieds, Et les \w filets|strong="H7636"\w* et les \w croissants|strong="H7720"\w*; 
\v 19 Les \w pendants|strong="H5188"\w* d'oreilles, les \w bracelets|strong="H8285"\w* et les \w voiles|strong="H7479"\w*; 
\v 20 Les \w diadèmes|strong="H6287"\w*, les chaînettes des \w pieds|strong="H6807"\w* et les \w ceintures|strong="H7196"\w*, Les \w boîtes|strong="H1004"\w* de \w senteur|strong="H5315"\w* et les \w amulettes|strong="H3908"\w*; 

\s5
\v 21 Les \w bagues|strong="H2885"\w* et les \w anneaux|strong="H5141"\w* du \w nez|strong="H639"\w*; 
\v 22 Les \w vêtements|strong="H4254"\w* précieux et les larges \w tuniques|strong="H4595"\w*, Les \w manteaux|strong="H4304"\w* et les \w gibecières|strong="H2754"\w*; 
\v 23 Les \w miroirs|strong="H1549"\w* et les chemises \w fines|strong="H5466"\w*, Les \w turbans|strong="H6797"\w* et les surtouts \w légers|strong="H7289"\w*. 

\s5
\v 24 Au lieu de \w parfum|strong="H1314"\w*, il y aura de l'\w infection|strong="H4716"\w*; Au lieu de \w ceinture|strong="H2290"\w*, une \w corde|strong="H5364"\w*; Au lieu de \w cheveux|strong="H4748"\w* \w bouclés|strong="H4639"\w*, une tête \w chauve|strong="H7144"\w*; Au lieu d'un large \w manteau|strong="H6614"\w*, un \w sac|strong="H8242"\w* \w étroit|strong="H4228"\w*; Une marque \w flétrissante|strong="H3587"\w*, au lieu de \w beauté|strong="H3308"\w*. 
\v 25 Tes \w hommes|strong="H4962"\w* \w tomberont|strong="H5307" x-morph="strongMorph:TH8799"\w* sous le \w glaive|strong="H2719"\w*, Et tes \w héros|strong="H1369"\w* dans le \w combat|strong="H4421"\w*. 
\v 26 Les \w portes|strong="H6607"\w* de Sion \w gémiront|strong="H578" x-morph="strongMorph:TH8804"\w* et seront dans le \w deuil|strong="H56" x-morph="strongMorph:TH8804"\w*; \w Dépouillée|strong="H5352" x-morph="strongMorph:TH8738"\w*, elle s'\w assiéra|strong="H3427" x-morph="strongMorph:TH8799"\w* par \w terre|strong="H776"\w*. 

\s5
\c 4  
\p
\v 1 Et \w sept|strong="H7651"\w* \w femmes|strong="H802"\w* \w saisiront|strong="H2388" x-morph="strongMorph:TH8689"\w* en ce \w jour|strong="H3117"\w* un \w seul|strong="H259"\w* \w homme|strong="H376"\w*, et \w diront|strong="H559" x-morph="strongMorph:TH8800"\w*: Nous \w mangerons|strong="H398" x-morph="strongMorph:TH8799"\w* notre \w pain|strong="H3899"\w*, Et nous nous \w vêtirons|strong="H3847" x-morph="strongMorph:TH8799"\w* de nos \w habits|strong="H8071"\w*; Fais-nous seulement \w porter|strong="H7121" x-morph="strongMorph:TH8735"\w* ton \w nom|strong="H8034"\w*! \w Enlève|strong="H622" x-morph="strongMorph:TH8798"\w* notre \w opprobre|strong="H2781"\w*! 
\v 2 En ce \w temps|strong="H3117"\w*-là, le \w germe|strong="H6780"\w* de l'\w Éternel|strong="H3068"\w* Aura de la \w magnificence|strong="H6643"\w* et de la \w gloire|strong="H3519"\w*, Et le \w fruit|strong="H6529"\w* du \w pays|strong="H776"\w* aura de l'\w éclat|strong="H1347"\w* et de la \w beauté|strong="H8597"\w* Pour les \w réchappés|strong="H6413"\w* d'\w Israël|strong="H3478"\w*. 

\s5
\v 3 Et les \w restes|strong="H7604" x-morph="strongMorph:TH8737"\w* de \w Sion|strong="H6726"\w*, les \w restes|strong="H3498" x-morph="strongMorph:TH8737"\w* de \w Jérusalem|strong="H3389"\w*, Seront \w appelés|strong="H559" x-morph="strongMorph:TH8735"\w* \w saints|strong="H6918"\w*, Quiconque à \w Jérusalem|strong="H3389"\w* sera \w inscrit|strong="H3789" x-morph="strongMorph:TH8803"\w* parmi les \w vivants|strong="H2416"\w*, 
\v 4 Après que le \w Seigneur|strong="H136"\w* aura \w lavé|strong="H7364" x-morph="strongMorph:TH8804"\w* les \w ordures|strong="H6675"\w* des \w filles|strong="H1323"\w* de \w Sion|strong="H6726"\w*, Et \w purifié|strong="H1740" x-morph="strongMorph:TH8686"\w* \w Jérusalem|strong="H3389"\w* du \w sang|strong="H1818"\w* qui est au \w milieu|strong="H7130"\w* d'elle, Par le \w souffle|strong="H7307"\w* de la \w justice|strong="H4941"\w* et par le \w souffle|strong="H7307"\w* de la \w destruction|strong="H1197" x-morph="strongMorph:TH8763"\w*. 

\s5
\v 5 L'\w Éternel|strong="H3068"\w* \w établira|strong="H1254" x-morph="strongMorph:TH8804"\w*, sur toute l'\w étendue|strong="H4349"\w* de la \w montagne|strong="H2022"\w* de \w Sion|strong="H6726"\w* Et sur ses lieux d'\w assemblées|strong="H4744"\w*, Une \w nuée|strong="H6051"\w* \w fumante|strong="H6227"\w* pendant le \w jour|strong="H3119"\w*, Et un \w feu|strong="H5051"\w* de \w flammes|strong="H784"\w* \w éclatantes|strong="H3852"\w* pendant la \w nuit|strong="H3915"\w*; Car tout ce qui est \w glorieux|strong="H3519"\w* sera mis à \w couvert|strong="H2646"\w*. 
\v 6 Il y aura un \w abri|strong="H5521"\w* pour donner de l'\w ombre|strong="H6738"\w* contre la \w chaleur|strong="H2721"\w* du \w jour|strong="H3119"\w*, Pour servir de \w refuge|strong="H4268"\w* et d'\w asile|strong="H4563"\w* contre l'\w orage|strong="H2230"\w* et la \w pluie|strong="H4306"\w*. 

\s5
\c 5  
\p
\v 1 Je \w chanterai|strong="H7891" x-morph="strongMorph:TH8799"\w* à mon bien-\w aimé|strong="H3039"\w* Le \w cantique|strong="H7892"\w* de mon bien-\w aimé|strong="H1730"\w* sur sa \w vigne|strong="H3754"\w*. Mon bien-\w aimé|strong="H3039"\w* avait une \w vigne|strong="H3754"\w*, Sur un \w coteau|strong="H7161"\w* \w fertile|strong="H1121"\w*\w |strong="H8081"\w*. 
\v 2 Il en \w remua|strong="H5823" x-morph="strongMorph:TH8762"\w* le sol, ôta les \w pierres|strong="H5619" x-morph="strongMorph:TH8762"\w*, et y \w mit|strong="H5193" x-morph="strongMorph:TH8799"\w* un plant \w délicieux|strong="H8321"\w*; Il \w bâtit|strong="H1129" x-morph="strongMorph:TH8799"\w* une \w tour|strong="H4026"\w* au \w milieu|strong="H8432"\w* d'elle, Et il y \w creusa|strong="H2672" x-morph="strongMorph:TH8804"\w* aussi une \w cuve|strong="H3342"\w*. Puis il \w espéra|strong="H6960" x-morph="strongMorph:TH8762"\w* qu'elle \w produirait|strong="H6213" x-morph="strongMorph:TH8800"\w* de bons \w raisins|strong="H6025"\w*, Mais elle en a \w produit|strong="H6213" x-morph="strongMorph:TH8799"\w* de \w mauvais|strong="H891"\w*. 

\s5
\v 3 Maintenant donc, \w habitants|strong="H3427" x-morph="strongMorph:TH8802"\w* de \w Jérusalem|strong="H3389"\w* et \w hommes|strong="H376"\w* de \w Juda|strong="H3063"\w*, Soyez \w juges|strong="H8199" x-morph="strongMorph:TH8798"\w* entre moi et ma \w vigne|strong="H3754"\w*! 
