\s5
\v 22 Le \w méchant|strong="H7563"\w* est \w pris|strong="H3920" x-morph="strongMorph:TH8799"\w* dans ses propres \w iniquités|strong="H5771"\w*, Il est \w saisi|strong="H8551" x-morph="strongMorph:TH8735"\w* par les \w liens|strong="H2256"\w* de son \w péché|strong="H2403"\w*. 
\v 23 Il \w mourra|strong="H4191" x-morph="strongMorph:TH8799"\w* faute d'\w instruction|strong="H4148"\w*, Il \w chancellera|strong="H7686" x-morph="strongMorph:TH8799"\w* par l'\w excès|strong="H7230"\w* de sa \w folie|strong="H200"\w*. 

\s5
\c 6  
\p
\v 1 Mon \w fils|strong="H1121"\w*, si tu as \w cautionné|strong="H6148" x-morph="strongMorph:TH8804"\w* ton \w prochain|strong="H7453"\w*, Si tu t'es \w engagé|strong="H8628" x-morph="strongMorph:TH8804"\w*\w |strong="H3709"\w* pour \w autrui|strong="H2114" x-morph="strongMorph:TH8801"\w*, 
\v 2 Si tu es \w enlacé|strong="H3369" x-morph="strongMorph:TH8738"\w* par les \w paroles|strong="H561"\w* de ta \w bouche|strong="H6310"\w*, Si tu es \w pris|strong="H3920" x-morph="strongMorph:TH8738"\w* par les \w paroles|strong="H561"\w* de ta \w bouche|strong="H6310"\w*, 

\s5
\v 3  \w Fais|strong="H6213" x-morph="strongMorph:TH8798"\w* \w donc|strong="H645"\w* ceci, mon \w fils|strong="H1121"\w*, \w dégage|strong="H5337" x-morph="strongMorph:TH8734"\w*-toi, Puisque tu es \w tombé|strong="H935" x-morph="strongMorph:TH8804"\w* au \w pouvoir|strong="H3709"\w* de ton \w prochain|strong="H7453"\w*; \w Va|strong="H3212" x-morph="strongMorph:TH8798"\w*, \w prosterne|strong="H7511" x-morph="strongMorph:TH8690"\w*-toi, et \w fais|strong="H7292" x-morph="strongMorph:TH8798"\w* des instances auprès de \w lui|strong="H7453"\w*; 

\s5
\v 4 Ne \w donne|strong="H5414" x-morph="strongMorph:TH8799"\w* ni \w sommeil|strong="H8142"\w* à tes \w yeux|strong="H5869"\w*, Ni \w assoupissement|strong="H8572"\w* à tes \w paupières|strong="H6079"\w*; 
\v 5  \w Dégage|strong="H5337" x-morph="strongMorph:TH8734"\w*-toi comme la \w gazelle|strong="H6643"\w* de la \w main|strong="H3027"\w* du chasseur, Comme l'\w oiseau|strong="H6833"\w* de la \w main|strong="H3027"\w* de l'\w oiseleur|strong="H3353"\w*. 

\s5
\v 6  \w Va|strong="H3212" x-morph="strongMorph:TH8798"\w* vers la \w fourmi|strong="H5244"\w*, \w paresseux|strong="H6102"\w*; \w Considère|strong="H7200" x-morph="strongMorph:TH8798"\w* ses \w voies|strong="H1870"\w*, et deviens \w sage|strong="H2449" x-morph="strongMorph:TH8798"\w*. 
\v 7 Elle n'a ni \w chef|strong="H7101"\w*, Ni \w inspecteur|strong="H7860" x-morph="strongMorph:TH8802"\w*, ni \w maître|strong="H4910" x-morph="strongMorph:TH8802"\w*; 
\v 8 Elle \w prépare|strong="H3559" x-morph="strongMorph:TH8686"\w* en \w été|strong="H7019"\w* sa \w nourriture|strong="H3899"\w*, Elle \w amasse|strong="H103" x-morph="strongMorph:TH8804"\w* pendant la \w moisson|strong="H7105"\w* de quoi \w manger|strong="H3978"\w*. 

\s5
\v 9  \w Paresseux|strong="H6102"\w*, jusqu'à quand seras-tu \w couché|strong="H7901" x-morph="strongMorph:TH8799"\w*? Quand te \w lèveras|strong="H6965" x-morph="strongMorph:TH8799"\w*-tu de ton \w sommeil|strong="H8142"\w*? 
\v 10 Un \w peu|strong="H4592"\w* de \w sommeil|strong="H8142"\w*, un \w peu|strong="H4592"\w* d'\w assoupissement|strong="H8572"\w*, Un \w peu|strong="H4592"\w* \w croiser|strong="H2264"\w* les \w mains|strong="H3027"\w* pour \w dormir|strong="H7901" x-morph="strongMorph:TH8800"\w*!... 
\v 11 Et la \w pauvreté|strong="H7389"\w* te \w surprendra|strong="H935" x-morph="strongMorph:TH8804"\w*, comme un \w rôdeur|strong="H1980" x-morph="strongMorph:TH8764"\w*, Et la \w disette|strong="H4270"\w*, comme un \w homme|strong="H376"\w* en \w armes|strong="H4043"\w*. 

\s5
\v 12 L'\w homme|strong="H120"\w* \w pervers|strong="H1100"\w*, l'\w homme|strong="H376"\w* \w inique|strong="H205"\w*, \w Marche|strong="H1980" x-morph="strongMorph:TH8802"\w* la \w fausseté|strong="H6143"\w* dans la \w bouche|strong="H6310"\w*; 
\v 13 Il \w cligne|strong="H7169" x-morph="strongMorph:TH8802"\w* des \w yeux|strong="H5869"\w*, \w parle|strong="H4448" x-morph="strongMorph:TH8802"\w* du \w pied|strong="H7272"\w*, Fait des \w signes|strong="H3384" x-morph="strongMorph:TH8688"\w* avec les \w doigts|strong="H676"\w*; 

\s5
\v 14 La \w perversité|strong="H8419"\w* est dans son \w coeur|strong="H3820"\w*, Il \w médite|strong="H2790" x-morph="strongMorph:TH8802"\w* le \w mal|strong="H7451"\w* en tout \w temps|strong="H6256"\w*, Il \w excite|strong="H7971" x-morph="strongMorph:TH8762"\w* des \w querelles|strong="H4066" x-morph="strongMorph:TH8675"\w*\w |strong="H4090"\w*. 
\v 15 Aussi sa \w ruine|strong="H343"\w* \w arrivera|strong="H935" x-morph="strongMorph:TH8799"\w*-t-elle \w subitement|strong="H6597"\w*; Il sera \w brisé|strong="H7665" x-morph="strongMorph:TH8735"\w* tout d'un \w coup|strong="H6621"\w*, et sans \w remède|strong="H4832"\w*. 

\s5
\v 16 Il y a \w six|strong="H8337"\w* choses que \w hait|strong="H8130" x-morph="strongMorph:TH8804"\w* l'\w Éternel|strong="H3068"\w*, Et même \w sept|strong="H7651"\w* qu'\w il|strong="H5315"\w* a en \w horreur|strong="H8441"\w*; 

\s5
\v 17 Les \w yeux|strong="H5869"\w* \w hautains|strong="H7311" x-morph="strongMorph:TH8802"\w*, la \w langue|strong="H3956"\w* \w menteuse|strong="H8267"\w*, Les \w mains|strong="H3027"\w* qui \w répandent|strong="H8210" x-morph="strongMorph:TH8802"\w* le \w sang|strong="H1818"\w* \w innocent|strong="H5355"\w*, 
\v 18 Le \w coeur|strong="H3820"\w* qui \w médite|strong="H2790" x-morph="strongMorph:TH8802"\w* des \w projets|strong="H4284"\w* \w iniques|strong="H205"\w*, Les \w pieds|strong="H7272"\w* qui se \w hâtent|strong="H4116" x-morph="strongMorph:TH8764"\w* de \w courir|strong="H7323" x-morph="strongMorph:TH8800"\w* au \w mal|strong="H7451"\w*, 
\v 19 Le \w faux|strong="H8267"\w* \w témoin|strong="H5707"\w* qui \w dit|strong="H6315" x-morph="strongMorph:TH8686"\w* des \w mensonges|strong="H3577"\w*, Et celui qui \w excite|strong="H7971" x-morph="strongMorph:TH8764"\w* des \w querelles|strong="H4090"\w* entre \w frères|strong="H251"\w*. 

\s5
\v 20 Mon \w fils|strong="H1121"\w*, \w garde|strong="H5341" x-morph="strongMorph:TH8798"\w* les \w préceptes|strong="H4687"\w* de ton \w père|strong="H1"\w*, Et ne \w rejette|strong="H5203" x-morph="strongMorph:TH8799"\w* pas l'\w enseignement|strong="H8451"\w* de ta \w mère|strong="H517"\w*. 
\v 21  \w Lie|strong="H7194" x-morph="strongMorph:TH8798"\w*-les \w constamment|strong="H8548"\w* sur ton \w coeur|strong="H3820"\w*, \w Attache|strong="H6029" x-morph="strongMorph:TH8798"\w*-les à ton \w cou|strong="H1621"\w*. 

\s5
\v 22 Ils te \w dirigeront|strong="H5148" x-morph="strongMorph:TH8686"\w* dans ta \w marche|strong="H1980" x-morph="strongMorph:TH8692"\w*, Ils te \w garderont|strong="H8104" x-morph="strongMorph:TH8799"\w* sur ta \w couche|strong="H7901" x-morph="strongMorph:TH8800"\w*, Ils te \w parleront|strong="H7878" x-morph="strongMorph:TH8799"\w* à ton \w réveil|strong="H6974" x-morph="strongMorph:TH8689"\w*. 
\v 23 Car le \w précepte|strong="H4687"\w* est une \w lampe|strong="H5216"\w*, et l'\w enseignement|strong="H8451"\w* une \w lumière|strong="H216"\w*, Et les \w avertissements|strong="H8433"\w* de la \w correction|strong="H4148"\w* sont le \w chemin|strong="H1870"\w* de la \w vie|strong="H2416"\w*: 

\s5
\v 24 Ils te \w préserveront|strong="H8104" x-morph="strongMorph:TH8800"\w* de la \w femme|strong="H802"\w* \w corrompue|strong="H7451"\w*, De la \w langue|strong="H3956"\w* \w doucereuse|strong="H2513"\w* de l'\w étrangère|strong="H5237"\w*. 
\v 25 Ne la \w convoite|strong="H2530" x-morph="strongMorph:TH8799"\w* pas dans ton \w coeur|strong="H3824"\w* pour sa \w beauté|strong="H3308"\w*, Et ne te laisse pas \w séduire|strong="H3947" x-morph="strongMorph:TH8799"\w* par ses \w paupières|strong="H6079"\w*. 

\s5
\v 26 Car \w pour|strong="H1157"\w* la \w femme|strong="H802"\w* \w prostituée|strong="H2181" x-morph="strongMorph:TH8802"\w* on se réduit à un \w morceau|strong="H3603"\w* de \w pain|strong="H3899"\w*, Et la femme \w mariée|strong="H376"\w*\w |strong="H802"\w* tend un \w piège|strong="H6679" x-morph="strongMorph:TH8799"\w* à la \w vie|strong="H5315"\w* \w précieuse|strong="H3368"\w*. 
\v 27 Quelqu'\w un|strong="H376"\w* \w mettra|strong="H2846" x-morph="strongMorph:TH8799"\w*-t-il du \w feu|strong="H784"\w* dans son \w sein|strong="H2436"\w*, Sans que ses \w vêtements|strong="H899"\w* s'\w enflamment|strong="H8313" x-morph="strongMorph:TH8735"\w*? 

\s5
\v 28 Quelqu'\w un|strong="H376"\w* \w marchera|strong="H1980" x-morph="strongMorph:TH8762"\w*-t-il sur des charbons \w ardents|strong="H1513"\w*, Sans que ses \w pieds|strong="H7272"\w* soient \w brûlés|strong="H3554" x-morph="strongMorph:TH8735"\w*? 
\v 29 Il en est de même pour celui qui \w va|strong="H935" x-morph="strongMorph:TH8802"\w* vers la \w femme|strong="H802"\w* de son \w prochain|strong="H7453"\w*: Quiconque la \w touche|strong="H5060" x-morph="strongMorph:TH8802"\w* ne restera pas \w impuni|strong="H5352" x-morph="strongMorph:TH8735"\w*. 

\s5
\v 30 On ne tient pas pour \w innocent|strong="H936" x-morph="strongMorph:TH8799"\w* le \w voleur|strong="H1590"\w* qui \w dérobe|strong="H1589" x-morph="strongMorph:TH8799"\w* Pour \w satisfaire|strong="H4390" x-morph="strongMorph:TH8763"\w* son \w appétit|strong="H5315"\w*, quand il a \w faim|strong="H7456" x-morph="strongMorph:TH8799"\w*; 
\v 31 Si on le \w trouve|strong="H4672" x-morph="strongMorph:TH8738"\w*, il fera une \w restitution|strong="H7999" x-morph="strongMorph:TH8762"\w* au \w septuple|strong="H7659"\w*, Il \w donnera|strong="H5414" x-morph="strongMorph:TH8799"\w* tout ce qu'il \w a|strong="H1952"\w* dans sa \w maison|strong="H1004"\w*. 

\s5
\v 32 Mais celui qui commet un \w adultère|strong="H5003" x-morph="strongMorph:TH8802"\w* avec une \w femme|strong="H802"\w* est \w dépourvu|strong="H2638"\w* de \w sens|strong="H3820"\w*, Celui qui veut se \w perdre|strong="H7843" x-morph="strongMorph:TH8688"\w*\w |strong="H5315"\w* \w agit|strong="H6213" x-morph="strongMorph:TH8799"\w* de la sorte; 
\v 33 Il n'\w aura|strong="H4672" x-morph="strongMorph:TH8799"\w* que \w plaie|strong="H5061"\w* et \w ignominie|strong="H7036"\w*, Et son \w opprobre|strong="H2781"\w* ne s'\w effacera|strong="H4229" x-morph="strongMorph:TH8735"\w* point. 

\s5
\v 34 Car la \w jalousie|strong="H7068"\w* met un \w homme|strong="H1397"\w* en \w fureur|strong="H2534"\w*, Et il est sans \w pitié|strong="H2550" x-morph="strongMorph:TH8799"\w* au \w jour|strong="H3117"\w* de la \w vengeance|strong="H5359"\w*; 
\v 35 Il n'a \w égard|strong="H5375" x-morph="strongMorph:TH8799"\w* à \w aucune|strong="H6440"\w* \w rançon|strong="H3724"\w*, Et il est \w inflexible|strong="H14" x-morph="strongMorph:TH8799"\w*, quand même tu \w multiplierais|strong="H7235" x-morph="strongMorph:TH8686"\w* les \w dons|strong="H7810"\w*. 

\s5
\c 7  
\p
\v 1 Mon \w fils|strong="H1121"\w*, \w retiens|strong="H8104" x-morph="strongMorph:TH8798"\w* mes \w paroles|strong="H561"\w*, Et \w garde|strong="H6845" x-morph="strongMorph:TH8799"\w* avec toi mes \w préceptes|strong="H4687"\w*. 
\v 2  \w Observe|strong="H8104" x-morph="strongMorph:TH8798"\w* mes \w préceptes|strong="H4687"\w*, et tu \w vivras|strong="H2421" x-morph="strongMorph:TH8798"\w*; Garde mes \w enseignements|strong="H8451"\w* comme la \w prunelle|strong="H380"\w* de tes \w yeux|strong="H5869"\w*. 
\v 3  \w Lie|strong="H7194" x-morph="strongMorph:TH8798"\w*-les sur tes \w doigts|strong="H676"\w*, \w Écris|strong="H3789" x-morph="strongMorph:TH8798"\w*-les sur la \w table|strong="H3871"\w* de ton \w coeur|strong="H3820"\w*. 

\s5
\v 4  \w Dis|strong="H559" x-morph="strongMorph:TH8798"\w* à la \w sagesse|strong="H2451"\w*: Tu es ma \w soeur|strong="H269"\w*! Et \w appelle|strong="H7121" x-morph="strongMorph:TH8799"\w* l'\w intelligence|strong="H998"\w* ton \w amie|strong="H4129"\w*, 
\v 5 Pour qu'elles te \w préservent|strong="H8104" x-morph="strongMorph:TH8800"\w* de la \w femme|strong="H802"\w* \w étrangère|strong="H2114" x-morph="strongMorph:TH8801"\w*, De l'\w étrangère|strong="H5237"\w* qui emploie des \w paroles|strong="H561"\w* \w doucereuses|strong="H2505" x-morph="strongMorph:TH8689"\w*. 

\s5
\v 6 J'étais à la \w fenêtre|strong="H2474"\w* de ma \w maison|strong="H1004"\w*, Et je \w regardais|strong="H8259" x-morph="strongMorph:TH8738"\w* à travers mon \w treillis|strong="H822"\w*. 
\v 7 J'\w aperçus|strong="H7200" x-morph="strongMorph:TH8799"\w* parmi les \w stupides|strong="H6612"\w*, Je \w remarquai|strong="H995" x-morph="strongMorph:TH8799"\w* parmi les jeunes \w gens|strong="H1121"\w* un \w garçon|strong="H5288"\w* \w dépourvu|strong="H2638"\w* de \w sens|strong="H3820"\w*. 

\s5
\v 8 Il \w passait|strong="H5674" x-morph="strongMorph:TH8802"\w* dans la \w rue|strong="H7784"\w*, \w près|strong="H681"\w* de l'\w angle|strong="H6434"\w* où se tenait une de ces étrangères, Et il se \w dirigeait|strong="H6805" x-morph="strongMorph:TH8799"\w* lentement du \w côté|strong="H1870"\w* de sa \w demeure|strong="H1004"\w*: 
\v 9 C'était au \w crépuscule|strong="H5399"\w*, pendant la \w soirée|strong="H3117"\w*\w |strong="H6153"\w*, Au \w milieu|strong="H380"\w* de la \w nuit|strong="H3915"\w* et de l'\w obscurité|strong="H653"\w*. 

\s5
\v 10 Et voici, il fut \w abordé|strong="H7125" x-morph="strongMorph:TH8800"\w* par une \w femme|strong="H802"\w* Ayant la \w mise|strong="H7897"\w* d'une \w prostituée|strong="H2181" x-morph="strongMorph:TH8802"\w* et la \w ruse|strong="H5341" x-morph="strongMorph:TH8803"\w* dans le \w coeur|strong="H3820"\w*. 
\v 11 Elle était \w bruyante|strong="H1993" x-morph="strongMorph:TH8802"\w* et \w rétive|strong="H5637" x-morph="strongMorph:TH8802"\w*; Ses \w pieds|strong="H7272"\w* ne \w restaient|strong="H7931" x-morph="strongMorph:TH8799"\w* point dans sa \w maison|strong="H1004"\w*; 
\v 12  \w Tantôt|strong="H6471"\w* dans la \w rue|strong="H2351"\w*, \w tantôt|strong="H6471"\w* sur les \w places|strong="H7339"\w*, Et \w près|strong="H681"\w* de tous les \w angles|strong="H6438"\w*, elle était aux \w aguets|strong="H693" x-morph="strongMorph:TH8799"\w*. 

\s5
\v 13 Elle le \w saisit|strong="H2388" x-morph="strongMorph:TH8689"\w* et l'\w embrassa|strong="H5401" x-morph="strongMorph:TH8804"\w*, Et d'un \w air|strong="H6440"\w* \w effronté|strong="H5810" x-morph="strongMorph:TH8689"\w* lui \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: 
\v 14 Je devais un \w sacrifice|strong="H2077"\w* d'actions de \w grâces|strong="H8002"\w*, Aujourd'\w hui|strong="H3117"\w* j'ai \w accompli|strong="H7999" x-morph="strongMorph:TH8765"\w* mes \w voeux|strong="H5088"\w*. 
\v 15 C'est pourquoi je suis \w sortie|strong="H3318" x-morph="strongMorph:TH8804"\w* au-\w devant|strong="H7125" x-morph="strongMorph:TH8800"\w* de toi Pour \w te|strong="H6440"\w* \w chercher|strong="H7836" x-morph="strongMorph:TH8763"\w*, et je t'ai \w trouvé|strong="H4672" x-morph="strongMorph:TH8799"\w*. 

\s5
\v 16 J'ai \w orné|strong="H7234" x-morph="strongMorph:TH8804"\w* mon \w lit|strong="H6210"\w* de \w couvertures|strong="H4765"\w*, De \w tapis|strong="H2405"\w* de \w fil|strong="H330"\w* d'\w Égypte|strong="H4714"\w*; 
\v 17 J'ai \w parfumé|strong="H5130" x-morph="strongMorph:TH8804"\w* ma \w couche|strong="H4904"\w* De \w myrrhe|strong="H4753"\w*, d'\w aloès|strong="H174"\w* et de \w cinnamome|strong="H7076"\w*. 
\v 18  \w Viens|strong="H3212" x-morph="strongMorph:TH8798"\w*, \w enivrons|strong="H7301" x-morph="strongMorph:TH8799"\w*-nous d'\w amour|strong="H1730"\w* jusqu'au \w matin|strong="H1242"\w*, Livrons-nous \w joyeusement|strong="H5965" x-morph="strongMorph:TH8691"\w* à la \w volupté|strong="H159"\w*. 

\s5
\v 19 Car mon \w mari|strong="H376"\w* n'est pas à la \w maison|strong="H1004"\w*, Il est \w parti|strong="H1980" x-morph="strongMorph:TH8804"\w* pour un \w voyage|strong="H1870"\w* \w lointain|strong="H7350"\w*; 
\v 20 Il a \w pris|strong="H3947" x-morph="strongMorph:TH8804"\w* avec \w lui|strong="H3027"\w* le \w sac|strong="H6872"\w* de l'\w argent|strong="H3701"\w*, Il ne \w reviendra|strong="H935" x-morph="strongMorph:TH8799"\w* à la \w maison|strong="H1004"\w* qu'à la nouvelle \w lune|strong="H3117"\w*\w |strong="H3677"\w*. 
\v 21 Elle le \w séduisit|strong="H5186" x-morph="strongMorph:TH8689"\w* à \w force|strong="H7230"\w* de \w paroles|strong="H3948"\w*, Elle l'\w entraîna|strong="H5080" x-morph="strongMorph:TH8686"\w* par ses \w lèvres|strong="H8193"\w* \w doucereuses|strong="H2506"\w*. 

