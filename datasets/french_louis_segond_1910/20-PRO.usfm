\v 30 Ce qui \w plaît|strong="H3974"\w* aux \w yeux|strong="H5869"\w* \w réjouit|strong="H8055" x-morph="strongMorph:TH8762"\w* le \w coeur|strong="H3820"\w*; Une \w bonne|strong="H2896"\w* \w nouvelle|strong="H8052"\w* \w fortifie|strong="H1878" x-morph="strongMorph:TH8762"\w* les \w membres|strong="H6106"\w*. 

\s5
\v 31 L'\w oreille|strong="H241"\w* \w attentive|strong="H8085" x-morph="strongMorph:TH8802"\w* aux \w réprimandes|strong="H8433"\w* qui mènent à la \w vie|strong="H2416"\w* Fait son \w séjour|strong="H3885" x-morph="strongMorph:TH8799"\w* au \w milieu|strong="H7130"\w* des \w sages|strong="H2450"\w*. 
\v 32 Celui qui \w rejette|strong="H6544" x-morph="strongMorph:TH8802"\w* la \w correction|strong="H4148"\w* \w méprise|strong="H3988" x-morph="strongMorph:TH8802"\w* son \w âme|strong="H5315"\w*, Mais celui qui \w écoute|strong="H8085" x-morph="strongMorph:TH8802"\w* la \w réprimande|strong="H8433"\w* \w acquiert|strong="H7069" x-morph="strongMorph:TH8802"\w* l'\w intelligence|strong="H3820"\w*. 

\s5
\v 33 La \w crainte|strong="H3374"\w* de l'\w Éternel|strong="H3068"\w* \w enseigne|strong="H4148"\w* la \w sagesse|strong="H2451"\w*, Et l'\w humilité|strong="H6038"\w* \w précède|strong="H6440"\w* la \w gloire|strong="H3519"\w*. 

\s5
\c 16  
\p
\v 1 Les \w projets|strong="H4633"\w* que forme le \w coeur|strong="H3820"\w* dépendent de l'\w homme|strong="H120"\w*, Mais la \w réponse|strong="H4617"\w* que donne la \w bouche|strong="H3956"\w* vient de l'\w Éternel|strong="H3068"\w*. 
\v 2 Toutes les \w voies|strong="H1870"\w* de l'\w homme|strong="H376"\w* sont \w pures|strong="H2134"\w* à ses \w yeux|strong="H5869"\w*; Mais celui qui \w pèse|strong="H8505" x-morph="strongMorph:TH8802"\w* les \w esprits|strong="H7307"\w*, c'est l'\w Éternel|strong="H3068"\w*. 

\s5
\v 3  \w Recommande|strong="H1556" x-morph="strongMorph:TH8798"\w* à l'\w Éternel|strong="H3068"\w* tes \w oeuvres|strong="H4639"\w*, Et tes \w projets|strong="H4284"\w* \w réussiront|strong="H3559" x-morph="strongMorph:TH8735"\w*. 
\v 4 L'\w Éternel|strong="H3068"\w* a tout \w fait|strong="H6466" x-morph="strongMorph:TH8804"\w* pour un \w but|strong="H4617"\w*, Même le \w méchant|strong="H7563"\w* pour le \w jour|strong="H3117"\w* du \w malheur|strong="H7451"\w*. 

\s5
\v 5 Tout \w coeur|strong="H3820"\w* \w hautain|strong="H1362"\w* est en \w abomination|strong="H8441"\w* à l'\w Éternel|strong="H3068"\w*; \w Certes|strong="H3027"\w*\w |strong="H3027"\w*, il ne restera pas \w impuni|strong="H5352" x-morph="strongMorph:TH8735"\w*. 
\v 6 Par la \w bonté|strong="H2617"\w* et la \w fidélité|strong="H571"\w* on \w expie|strong="H3722" x-morph="strongMorph:TH8792"\w* l'\w iniquité|strong="H5771"\w*, Et par la \w crainte|strong="H3374"\w* de l'\w Éternel|strong="H3068"\w* on se \w détourne|strong="H5493" x-morph="strongMorph:TH8800"\w* du \w mal|strong="H7451"\w*. 

\s5
\v 7 Quand l'\w Éternel|strong="H3068"\w* \w approuve|strong="H7521" x-morph="strongMorph:TH8800"\w* les \w voies|strong="H1870"\w* d'un \w homme|strong="H376"\w*, Il dispose \w favorablement|strong="H7999" x-morph="strongMorph:TH8686"\w* à son égard même ses \w ennemis|strong="H341" x-morph="strongMorph:TH8802"\w*. 
\v 8  \w Mieux|strong="H2896"\w* vaut \w peu|strong="H4592"\w*, avec la \w justice|strong="H6666"\w*, Que de \w grands|strong="H7230"\w* \w revenus|strong="H8393"\w*, avec l'\w injustice|strong="H4941"\w*. 

\s5
\v 9 Le \w coeur|strong="H3820"\w* de l'\w homme|strong="H120"\w* \w médite|strong="H2803" x-morph="strongMorph:TH8762"\w* sa \w voie|strong="H1870"\w*, Mais c'est l'\w Éternel|strong="H3068"\w* qui \w dirige|strong="H3559" x-morph="strongMorph:TH8686"\w* ses \w pas|strong="H6806"\w*. 
\v 10 Des \w oracles|strong="H7081"\w* sont sur les \w lèvres|strong="H8193"\w* du \w roi|strong="H4428"\w*: Sa \w bouche|strong="H6310"\w* ne doit pas être \w infidèle|strong="H4603" x-morph="strongMorph:TH8799"\w* quand il \w juge|strong="H4941"\w*. 

\s5
\v 11 Le \w poids|strong="H6425"\w* et la \w balance|strong="H3976"\w* \w justes|strong="H4941"\w* sont à l'\w Éternel|strong="H3068"\w*; Tous les \w poids|strong="H68"\w* du \w sac|strong="H3599"\w* sont son \w ouvrage|strong="H4639"\w*. 
\v 12 Les \w rois|strong="H4428"\w* ont \w horreur|strong="H8441"\w* de \w faire|strong="H6213" x-morph="strongMorph:TH8800"\w* le \w mal|strong="H7562"\w*, Car c'est par la \w justice|strong="H6666"\w* que le \w trône|strong="H3678"\w* s'\w affermit|strong="H3559" x-morph="strongMorph:TH8735"\w*. 

\s5
\v 13 Les \w lèvres|strong="H8193"\w* \w justes|strong="H6664"\w* gagnent la \w faveur|strong="H7522"\w* des \w rois|strong="H4428"\w*, Et ils \w aiment|strong="H157" x-morph="strongMorph:TH8799"\w* celui qui \w parle|strong="H1696" x-morph="strongMorph:TH8802"\w* avec \w droiture|strong="H3477"\w*. 
\v 14 La \w fureur|strong="H2534"\w* du \w roi|strong="H4428"\w* est un \w messager|strong="H4397"\w* de \w mort|strong="H4194"\w*, Et un \w homme|strong="H376"\w* \w sage|strong="H2450"\w* doit l'\w apaiser|strong="H3722" x-morph="strongMorph:TH8762"\w*. 

\s5
\v 15 La \w sérénité|strong="H216"\w* du \w visage|strong="H6440"\w* du \w roi|strong="H4428"\w* donne la \w vie|strong="H2416"\w*, Et sa \w faveur|strong="H7522"\w* est comme une \w pluie|strong="H5645"\w* du \w printemps|strong="H4456"\w*. 
\v 16 Combien \w acquérir|strong="H7069" x-morph="strongMorph:TH8800"\w* la \w sagesse|strong="H2451"\w* vaut \w mieux|strong="H2896"\w* que l'\w or|strong="H2742"\w*! Combien \w acquérir|strong="H7069" x-morph="strongMorph:TH8800"\w* l'\w intelligence|strong="H998"\w* est \w préférable|strong="H977" x-morph="strongMorph:TH8737"\w* à l'\w argent|strong="H3701"\w*! 

\s5
\v 17 Le \w chemin|strong="H4546"\w* des hommes \w droits|strong="H3477"\w*, c'est d'\w éviter|strong="H5493" x-morph="strongMorph:TH8800"\w* le \w mal|strong="H7451"\w*; Celui qui \w garde|strong="H8104" x-morph="strongMorph:TH8802"\w* son \w âme|strong="H5315"\w* \w veille|strong="H5341" x-morph="strongMorph:TH8802"\w* sur sa \w voie|strong="H1870"\w*. 
\v 18 L'\w arrogance|strong="H1347"\w* \w précède|strong="H6440"\w* la \w ruine|strong="H7667"\w*, Et l'\w orgueil|strong="H1363"\w*\w |strong="H7307"\w* \w précède|strong="H6440"\w* la \w chute|strong="H3783"\w*. 

\s5
\v 19  \w Mieux|strong="H2896"\w* vaut être \w humble|strong="H8217"\w*\w |strong="H7307"\w* avec les \w humbles|strong="H6035" x-morph="strongMorph:TH8675"\w*\w |strong="H6041"\w* Que de \w partager|strong="H2505" x-morph="strongMorph:TH8763"\w* le \w butin|strong="H7998"\w* avec les \w orgueilleux|strong="H1343"\w*. 
\v 20 Celui qui \w réfléchit|strong="H7919" x-morph="strongMorph:TH8688"\w* sur les \w choses|strong="H1697"\w* \w trouve|strong="H4672" x-morph="strongMorph:TH8799"\w* le \w bonheur|strong="H2896"\w*, Et celui qui se \w confie|strong="H982" x-morph="strongMorph:TH8802"\w* en l'\w Éternel|strong="H3068"\w* est \w heureux|strong="H835"\w*. 

\s5
\v 21 Celui qui est \w sage|strong="H2450"\w* de \w coeur|strong="H3820"\w* est \w appelé|strong="H7121" x-morph="strongMorph:TH8735"\w* \w intelligent|strong="H995" x-morph="strongMorph:TH8737"\w*, Et la \w douceur|strong="H4986"\w* des \w lèvres|strong="H8193"\w* \w augmente|strong="H3254" x-morph="strongMorph:TH8686"\w* le \w savoir|strong="H3948"\w*. 
\v 22 La \w sagesse|strong="H7922"\w* est une \w source|strong="H4726"\w* de \w vie|strong="H2416"\w* pour celui qui la \w possède|strong="H1167"\w*; Et le \w châtiment|strong="H4148"\w* des \w insensés|strong="H191"\w*, c'est leur \w folie|strong="H200"\w*. 

\s5
\v 23 Celui qui est \w sage|strong="H2450"\w* de \w coeur|strong="H3820"\w* \w manifeste|strong="H7919" x-morph="strongMorph:TH8686"\w* la sagesse par sa \w bouche|strong="H6310"\w*, Et l'\w accroissement|strong="H3254" x-morph="strongMorph:TH8686"\w* de son \w savoir|strong="H3948"\w* paraît sur ses \w lèvres|strong="H8193"\w*. 
\v 24 Les \w paroles|strong="H561"\w* \w agréables|strong="H5278"\w* sont un \w rayon|strong="H6688"\w* de \w miel|strong="H1706"\w*, \w Douces|strong="H4966"\w* pour l'\w âme|strong="H5315"\w* et \w salutaires|strong="H4832"\w* pour le \w corps|strong="H6106"\w*. 

\s5
\v 25  \w Telle|strong="H3426"\w* \w voie|strong="H1870"\w* paraît \w droite|strong="H3477"\w* \w à|strong="H6440"\w* un \w homme|strong="H376"\w*, Mais son \w issue|strong="H319"\w*, c'est la \w voie|strong="H1870"\w* de la \w mort|strong="H4194"\w*. 
\v 26  \w Celui|strong="H5315"\w* qui \w travaille|strong="H6001"\w*, \w travaille|strong="H5998" x-morph="strongMorph:TH8804"\w* pour lui, Car sa \w bouche|strong="H6310"\w* l'y \w excite|strong="H404" x-morph="strongMorph:TH8804"\w*. 

\s5
\v 27 L'\w homme|strong="H376"\w* \w pervers|strong="H1100"\w* \w prépare|strong="H3738" x-morph="strongMorph:TH8802"\w* le \w malheur|strong="H7451"\w*, Et il y a sur ses \w lèvres|strong="H8193"\w* comme un \w feu|strong="H784"\w* \w ardent|strong="H6867"\w*. 
\v 28 L'\w homme|strong="H376"\w* \w pervers|strong="H8419"\w* \w excite|strong="H7971" x-morph="strongMorph:TH8762"\w* des \w querelles|strong="H4066"\w*, Et le \w rapporteur|strong="H5372"\w* \w divise|strong="H6504" x-morph="strongMorph:TH8688"\w* les \w amis|strong="H441"\w*. 

\s5
\v 29 L'\w homme|strong="H376"\w* \w violent|strong="H2555"\w* \w séduit|strong="H6601" x-morph="strongMorph:TH8762"\w* son \w prochain|strong="H7453"\w*, Et le fait \w marcher|strong="H3212" x-morph="strongMorph:TH8689"\w* dans une \w voie|strong="H1870"\w* qui n'est pas \w bonne|strong="H2896"\w*. 
\v 30 Celui qui \w ferme|strong="H6095" x-morph="strongMorph:TH8802"\w* les \w yeux|strong="H5869"\w* pour se \w livrer|strong="H2803" x-morph="strongMorph:TH8800"\w* à des pensées \w perverses|strong="H8419"\w*, Celui qui se \w mord|strong="H7169" x-morph="strongMorph:TH8802"\w* les \w lèvres|strong="H8193"\w*, a déjà \w consommé|strong="H3615" x-morph="strongMorph:TH8765"\w* le \w mal|strong="H7451"\w*. 

\s5
\v 31 Les cheveux \w blancs|strong="H7872"\w* sont une \w couronne|strong="H5850"\w* d'\w honneur|strong="H8597"\w*; C'est dans le \w chemin|strong="H1870"\w* de la \w justice|strong="H6666"\w* qu'on la \w trouve|strong="H4672" x-morph="strongMorph:TH8735"\w*. 
\v 32 Celui qui est \w lent|strong="H750"\w* à la \w colère|strong="H639"\w* vaut \w mieux|strong="H2896"\w* qu'un \w héros|strong="H1368"\w*, Et celui qui est \w maître|strong="H4910" x-morph="strongMorph:TH8802"\w* de lui-\w même|strong="H7307"\w*, que celui qui \w prend|strong="H3920" x-morph="strongMorph:TH8802"\w* des \w villes|strong="H5892"\w*. 

\s5
\v 33 On \w jette|strong="H2904" x-morph="strongMorph:TH8714"\w* le \w sort|strong="H1486"\w* dans le \w pan|strong="H2436"\w* de la robe, Mais toute \w décision|strong="H4941"\w* vient de l'\w Éternel|strong="H3068"\w*. 

\s5
\c 17  
\p
\v 1  \w Mieux|strong="H2896"\w* vaut un \w morceau|strong="H6595"\w* de pain \w sec|strong="H2720"\w*, avec la \w paix|strong="H7962"\w*, Qu'une \w maison|strong="H1004"\w* \w pleine|strong="H4392"\w* de \w viandes|strong="H2077"\w*, avec des \w querelles|strong="H7379"\w*. 
\v 2 Un \w serviteur|strong="H5650"\w* \w prudent|strong="H7919" x-morph="strongMorph:TH8688"\w* \w domine|strong="H4910" x-morph="strongMorph:TH8799"\w* sur le \w fils|strong="H1121"\w* qui fait \w honte|strong="H954" x-morph="strongMorph:TH8688"\w*, Et il aura \w part|strong="H2505" x-morph="strongMorph:TH8799"\w* à l'\w héritage|strong="H5159"\w* au \w milieu|strong="H8432"\w* des \w frères|strong="H251"\w*. 

\s5
\v 3 Le \w creuset|strong="H4715"\w* est pour l'\w argent|strong="H3701"\w*, et le \w fourneau|strong="H3564"\w* pour l'\w or|strong="H2091"\w*; Mais celui qui \w éprouve|strong="H974" x-morph="strongMorph:TH8802"\w* les \w coeurs|strong="H3826"\w*, c'est l'\w Éternel|strong="H3068"\w*. 
\v 4 Le \w méchant|strong="H7489" x-morph="strongMorph:TH8688"\w* est \w attentif|strong="H7181" x-morph="strongMorph:TH8688"\w* à la \w lèvre|strong="H8193"\w* \w inique|strong="H205"\w*, Le \w menteur|strong="H8267"\w* prête l'\w oreille|strong="H238" x-morph="strongMorph:TH8688"\w* à la \w langue|strong="H3956"\w* \w pernicieuse|strong="H1942"\w*. 

\s5
\v 5 Celui qui se \w moque|strong="H3932" x-morph="strongMorph:TH8802"\w* du \w pauvre|strong="H7326" x-morph="strongMorph:TH8802"\w* \w outrage|strong="H2778" x-morph="strongMorph:TH8765"\w* celui qui l'a \w fait|strong="H6213" x-morph="strongMorph:TH8802"\w*; Celui qui se \w réjouit|strong="H8056"\w* d'un \w malheur|strong="H343"\w* ne restera pas \w impuni|strong="H5352" x-morph="strongMorph:TH8735"\w*. 
\v 6 Les \w enfants|strong="H1121"\w* des \w enfants|strong="H1121"\w* sont la \w couronne|strong="H5850"\w* des \w vieillards|strong="H2205"\w*, Et les \w pères|strong="H1"\w* sont la \w gloire|strong="H8597"\w* de leurs \w enfants|strong="H1121"\w*. 

\s5
\v 7 Les \w paroles|strong="H8193"\w* \w distinguées|strong="H3499"\w* ne \w conviennent|strong="H5000"\w* pas à un \w insensé|strong="H5036"\w*; Combien moins à un \w noble|strong="H5081"\w* les \w paroles|strong="H8193"\w* \w mensongères|strong="H8267"\w*! 
\v 8 Les \w présents|strong="H7810"\w* sont une \w pierre|strong="H68"\w* \w précieuse|strong="H2580"\w* aux \w yeux|strong="H5869"\w* de qui en \w reçoit|strong="H1167"\w*; De quelque côté qu'ils se \w tournent|strong="H6437" x-morph="strongMorph:TH8799"\w*, ils ont du \w succès|strong="H7919" x-morph="strongMorph:TH8686"\w*. 

\s5
\v 9 Celui qui \w couvre|strong="H3680" x-morph="strongMorph:TH8764"\w* une \w faute|strong="H6588"\w* \w cherche|strong="H1245" x-morph="strongMorph:TH8764"\w* l'\w amour|strong="H160"\w*, Et celui qui la \w rappelle|strong="H8138" x-morph="strongMorph:TH8802"\w* dans ses \w discours|strong="H1697"\w* \w divise|strong="H6504" x-morph="strongMorph:TH8688"\w* les \w amis|strong="H441"\w*. 
\v 10 Une \w réprimande|strong="H1606"\w* fait plus d'\w impression|strong="H5181" x-morph="strongMorph:TH8799"\w* sur l'homme \w intelligent|strong="H995" x-morph="strongMorph:TH8688"\w* Que \w cent|strong="H3967"\w* \w coups|strong="H5221" x-morph="strongMorph:TH8687"\w* sur l'\w insensé|strong="H3684"\w*. 

\s5
\v 11 Le \w méchant|strong="H7451"\w* ne \w cherche|strong="H1245" x-morph="strongMorph:TH8762"\w* que \w révolte|strong="H4805"\w*, Mais un \w messager|strong="H4397"\w* \w cruel|strong="H394"\w* sera \w envoyé|strong="H7971" x-morph="strongMorph:TH8792"\w* contre lui. 
\v 12  \w Rencontre|strong="H6298" x-morph="strongMorph:TH8800"\w* une \w ourse|strong="H1677"\w* \w privée|strong="H7909"\w* de ses petits, \w Plutôt|strong="H408"\w* qu'un \w insensé|strong="H3684"\w*\w |strong="H376"\w* pendant sa \w folie|strong="H200"\w*. 

\s5
\v 13 De celui qui \w rend|strong="H7725" x-morph="strongMorph:TH8688"\w* le \w mal|strong="H7451"\w* pour le \w bien|strong="H2896"\w* Le \w mal|strong="H7451"\w* ne \w quittera|strong="H4185" x-morph="strongMorph:TH8799"\w*\w |x-morph="strongMorph:TH8675"\w*\w |strong="H4185" x-morph="strongMorph:TH8686"\w* point la \w maison|strong="H1004"\w*. 
\v 14  \w Commencer|strong="H7225"\w* une \w querelle|strong="H4066"\w*, c'est \w ouvrir|strong="H6362" x-morph="strongMorph:TH8802"\w* une \w digue|strong="H4325"\w*; \w Avant|strong="H6440"\w* que la \w dispute|strong="H7379"\w* s'\w anime|strong="H1566" x-morph="strongMorph:TH8694"\w*, \w retire|strong="H5203" x-morph="strongMorph:TH8800"\w*-toi. 

\s5
\v 15 Celui qui \w absout|strong="H6663" x-morph="strongMorph:TH8688"\w* le \w coupable|strong="H7563"\w* et celui qui \w condamne|strong="H7561" x-morph="strongMorph:TH8688"\w* le \w juste|strong="H6662"\w* Sont tous \w deux|strong="H8147"\w* en \w abomination|strong="H8441"\w* à l'\w Éternel|strong="H3068"\w*. 
\v 16 À quoi sert l'\w argent|strong="H4242"\w* dans la \w main|strong="H3027"\w* de l'\w insensé|strong="H3684"\w*? À \w acheter|strong="H7069" x-morph="strongMorph:TH8800"\w* la \w sagesse|strong="H2451"\w*?... Mais il n'a point de \w sens|strong="H3820"\w*. 

\s5
\v 17 L'\w ami|strong="H7453"\w* \w aime|strong="H157" x-morph="strongMorph:TH8802"\w* en tout \w temps|strong="H6256"\w*, Et dans le \w malheur|strong="H6869"\w* il se \w montre|strong="H3205" x-morph="strongMorph:TH8735"\w* un \w frère|strong="H251"\w*. 
\v 18 L'\w homme|strong="H120"\w* \w dépourvu|strong="H2638"\w* de \w sens|strong="H3820"\w* prend des \w engagements|strong="H8628" x-morph="strongMorph:TH8802"\w*\w |strong="H3709"\w*, Il \w cautionne|strong="H6148" x-morph="strongMorph:TH8802"\w*\w |strong="H6161"\w*\w |strong="H6440"\w* son \w prochain|strong="H7453"\w*. 

\s5
\v 19 Celui qui \w aime|strong="H157" x-morph="strongMorph:TH8802"\w* les \w querelles|strong="H4683"\w* \w aime|strong="H157" x-morph="strongMorph:TH8802"\w* le \w péché|strong="H6588"\w*; Celui qui \w élève|strong="H1361" x-morph="strongMorph:TH8688"\w* sa \w porte|strong="H6607"\w* \w cherche|strong="H1245" x-morph="strongMorph:TH8764"\w* la \w ruine|strong="H7667"\w*. 
\v 20 Un \w coeur|strong="H3820"\w* \w faux|strong="H6141"\w* ne \w trouve|strong="H4672" x-morph="strongMorph:TH8799"\w* pas le \w bonheur|strong="H2896"\w*, Et celui dont la \w langue|strong="H3956"\w* est \w perverse|strong="H2015" x-morph="strongMorph:TH8738"\w* \w tombe|strong="H5307" x-morph="strongMorph:TH8799"\w* dans le \w malheur|strong="H7451"\w*. 

\s5
\v 21 Celui qui donne \w naissance|strong="H3205" x-morph="strongMorph:TH8802"\w* à un \w insensé|strong="H3684"\w* aura du \w chagrin|strong="H8424"\w*; Le \w père|strong="H1"\w* d'un \w fou|strong="H5036"\w* ne peut pas se \w réjouir|strong="H8055" x-morph="strongMorph:TH8799"\w*. 
\v 22 Un \w coeur|strong="H3820"\w* \w joyeux|strong="H8056"\w* est un \w bon|strong="H3190" x-morph="strongMorph:TH8686"\w* \w remède|strong="H1456"\w*, Mais un \w esprit|strong="H7307"\w* \w abattu|strong="H5218"\w* \w dessèche|strong="H3001" x-morph="strongMorph:TH8762"\w* les \w os|strong="H1634"\w*. 

\s5
