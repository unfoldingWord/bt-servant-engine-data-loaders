\v 18  \w Châtie|strong="H3256" x-morph="strongMorph:TH8761"\w* ton \w fils|strong="H1121"\w*, car il y a \w encore|strong="H3426"\w* de l'\w espérance|strong="H8615"\w*;\w |strong="H5315"\w* Mais ne \w désire|strong="H5375" x-morph="strongMorph:TH8799"\w* point le faire \w mourir|strong="H4191" x-morph="strongMorph:TH8687"\w*. 

\s5
\v 19  \w Celui|strong="H1419"\w* que la \w colère|strong="H2534"\w* \w emporte|strong="H1632"\w* doit en \w subir|strong="H5375" x-morph="strongMorph:TH8802"\w* la \w peine|strong="H6066"\w*; Car si tu le \w libères|strong="H5337" x-morph="strongMorph:TH8686"\w*, tu devras y \w revenir|strong="H3254" x-morph="strongMorph:TH8686"\w*. 
\v 20  \w Écoute|strong="H8085" x-morph="strongMorph:TH8798"\w* les \w conseils|strong="H6098"\w*, et \w reçois|strong="H6901" x-morph="strongMorph:TH8761"\w* l'\w instruction|strong="H4148"\w*, Afin que tu sois \w sage|strong="H2449" x-morph="strongMorph:TH8799"\w* dans la suite de ta \w vie|strong="H319"\w*. 

\s5
\v 21 Il y a dans le \w coeur|strong="H3820"\w* de l'\w homme|strong="H376"\w* \w beaucoup|strong="H7227"\w* de \w projets|strong="H4284"\w*, Mais c'est le \w dessein|strong="H6098"\w* de l'\w Éternel|strong="H3068"\w* qui s'\w accomplit|strong="H6965" x-morph="strongMorph:TH8799"\w*. 
\v 22 Ce qui fait le \w charme|strong="H8378"\w* d'un \w homme|strong="H120"\w*, c'est sa \w bonté|strong="H2617"\w*; Et \w mieux|strong="H2896"\w* vaut un \w pauvre|strong="H7326" x-morph="strongMorph:TH8802"\w* \w qu|strong="H376"\w*'un \w menteur|strong="H3577"\w*. 

\s5
\v 23 La \w crainte|strong="H3374"\w* de l'\w Éternel|strong="H3068"\w* mène à la \w vie|strong="H2416"\w*, Et l'on passe la \w nuit|strong="H3885" x-morph="strongMorph:TH8799"\w* \w rassasié|strong="H7649"\w*, sans être \w visité|strong="H6485" x-morph="strongMorph:TH8735"\w* par le \w malheur|strong="H7451"\w*. 
\v 24 Le \w paresseux|strong="H6102"\w* \w plonge|strong="H2934" x-morph="strongMorph:TH8804"\w* sa \w main|strong="H3027"\w* dans le \w plat|strong="H6747"\w*, Et il ne la \w ramène|strong="H7725" x-morph="strongMorph:TH8686"\w* pas à sa \w bouche|strong="H6310"\w*. 

\s5
\v 25  \w Frappe|strong="H5221" x-morph="strongMorph:TH8686"\w* le \w moqueur|strong="H3887" x-morph="strongMorph:TH8801"\w*, et le \w sot|strong="H6612"\w* deviendra \w sage|strong="H6191" x-morph="strongMorph:TH8686"\w*; \w Reprends|strong="H3198" x-morph="strongMorph:TH8689"\w* l'homme \w intelligent|strong="H995" x-morph="strongMorph:TH8737"\w*, et il \w comprendra|strong="H995" x-morph="strongMorph:TH8799"\w* la \w science|strong="H1847"\w*. 

\s5
\v 26 Celui qui \w ruine|strong="H7703" x-morph="strongMorph:TH8764"\w* son \w père|strong="H1"\w* et qui met en \w fuite|strong="H1272" x-morph="strongMorph:TH8686"\w* sa \w mère|strong="H517"\w* Est un \w fils|strong="H1121"\w* qui fait \w honte|strong="H954" x-morph="strongMorph:TH8688"\w* et qui fait \w rougir|strong="H2659" x-morph="strongMorph:TH8688"\w*. 
\v 27  \w Cesse|strong="H2308" x-morph="strongMorph:TH8798"\w*, mon \w fils|strong="H1121"\w*, d'\w écouter|strong="H8085" x-morph="strongMorph:TH8800"\w* l'\w instruction|strong="H4148"\w*, Si c'est pour t'\w éloigner|strong="H7686" x-morph="strongMorph:TH8800"\w* des \w paroles|strong="H561"\w* de la \w science|strong="H1847"\w*. 

\s5
\v 28 Un \w témoin|strong="H5707"\w* \w pervers|strong="H1100"\w* se \w moque|strong="H3887" x-morph="strongMorph:TH8686"\w* de la \w justice|strong="H4941"\w*, Et la \w bouche|strong="H6310"\w* des \w méchants|strong="H7563"\w* \w dévore|strong="H1104" x-morph="strongMorph:TH8762"\w* l'\w iniquité|strong="H205"\w*. 
