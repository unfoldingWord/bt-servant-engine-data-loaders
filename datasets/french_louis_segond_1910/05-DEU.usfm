\c 10  
\p
\v 1 En ce \w temps|strong="H6256"\w*-là, l'\w Éternel|strong="H3068"\w* me \w dit|strong="H559" x-morph="strongMorph:TH8804"\w*: \w Taille|strong="H6458" x-morph="strongMorph:TH8798"\w* \w deux|strong="H8147"\w* \w tables|strong="H3871"\w* de \w pierre|strong="H68"\w* comme les \w premières|strong="H7223"\w*, et \w monte|strong="H5927" x-morph="strongMorph:TH8798"\w* vers moi sur la \w montagne|strong="H2022"\w*; tu \w feras|strong="H6213" x-morph="strongMorph:TH8804"\w* aussi une \w arche|strong="H727"\w* de \w bois|strong="H6086"\w*. 
\v 2 J'\w écrirai|strong="H3789" x-morph="strongMorph:TH8799"\w* sur ces \w tables|strong="H3871"\w* les \w paroles|strong="H1697"\w* qui étaient sur les \w premières|strong="H7223"\w* \w tables|strong="H3871"\w* que tu as \w brisées|strong="H7665" x-morph="strongMorph:TH8765"\w*, et tu les \w mettras|strong="H7760" x-morph="strongMorph:TH8804"\w* dans l'\w arche|strong="H727"\w*. 

\s5
\v 3 Je \w fis|strong="H6213" x-morph="strongMorph:TH8799"\w* une \w arche|strong="H727"\w* de \w bois|strong="H6086"\w* d'\w acacia|strong="H7848"\w*, je \w taillai|strong="H6458" x-morph="strongMorph:TH8799"\w* \w deux|strong="H8147"\w* \w tables|strong="H3871"\w* de \w pierre|strong="H68"\w* comme les \w premières|strong="H7223"\w*, et je \w montai|strong="H5927" x-morph="strongMorph:TH8799"\w* sur la \w montagne|strong="H2022"\w*, les \w deux|strong="H8147"\w* \w tables|strong="H3871"\w* dans ma \w main|strong="H3027"\w*. 
\v 4 L'\w Éternel|strong="H3068"\w* \w écrivit|strong="H3789" x-morph="strongMorph:TH8799"\w* sur les \w tables|strong="H3871"\w* ce qui avait été \w écrit|strong="H4385"\w* sur les \w premières|strong="H7223"\w*, les \w dix|strong="H6235"\w* \w paroles|strong="H1697"\w* qu'il vous avait \w dites|strong="H1696" x-morph="strongMorph:TH8765"\w* sur la \w montagne|strong="H2022"\w*, du \w milieu|strong="H8432"\w* du \w feu|strong="H784"\w*, le \w jour|strong="H3117"\w* de l'\w assemblée|strong="H6951"\w*; et l'\w Éternel|strong="H3068"\w* me les \w donna|strong="H5414" x-morph="strongMorph:TH8799"\w*. 

\s5
\v 5 Je \w retournai|strong="H6437" x-morph="strongMorph:TH8799"\w* et je \w descendis|strong="H3381" x-morph="strongMorph:TH8799"\w* de la \w montagne|strong="H2022"\w*, je \w mis|strong="H7760" x-morph="strongMorph:TH8799"\w* les \w tables|strong="H3871"\w* dans l'\w arche|strong="H727"\w* que j'avais \w faite|strong="H6213" x-morph="strongMorph:TH8804"\w*, et elles restèrent là, comme l'\w Éternel|strong="H3068"\w* me l'avait \w ordonné|strong="H6680" x-morph="strongMorph:TH8765"\w*. 

\s5
\v 6 Les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w* \w partirent|strong="H5265" x-morph="strongMorph:TH8804"\w* de Beéroth-Bené \w Jaakan|strong="H885"\w* pour \w Moséra|strong="H4149"\w*. C'est là que \w mourut|strong="H4191" x-morph="strongMorph:TH8804"\w* \w Aaron|strong="H175"\w*, et qu'il fut \w enterré|strong="H6912" x-morph="strongMorph:TH8735"\w*; \w Éléazar|strong="H499"\w*, son \w fils|strong="H1121"\w*, lui succéda dans le \w sacerdoce|strong="H3547" x-morph="strongMorph:TH8762"\w*. 
\v 7 Ils \w partirent|strong="H5265" x-morph="strongMorph:TH8804"\w* de là pour \w Gudgoda|strong="H1412"\w*, et de \w Gudgoda|strong="H1412"\w* pour \w Jothbatha|strong="H3193"\w*, \w pays|strong="H776"\w* où il y a des \w cours|strong="H5158"\w* d'\w eaux|strong="H4325"\w*. 

\s5
\v 8 En ce \w temps|strong="H6256"\w*-là, l'\w Éternel|strong="H3068"\w* \w sépara|strong="H914" x-morph="strongMorph:TH8689"\w* la \w tribu|strong="H7626"\w* de \w Lévi|strong="H3878"\w*, et lui ordonna de \w porter|strong="H5375" x-morph="strongMorph:TH8800"\w* l'\w arche|strong="H727"\w* de l'\w alliance|strong="H1285"\w* de l'\w Éternel|strong="H3068"\w*, de se \w tenir|strong="H5975" x-morph="strongMorph:TH8800"\w* \w devant|strong="H6440"\w* l'\w Éternel|strong="H3068"\w* pour le \w servir|strong="H8334" x-morph="strongMorph:TH8763"\w*, et de \w bénir|strong="H1288" x-morph="strongMorph:TH8763"\w* le peuple en son \w nom|strong="H8034"\w*: ce qu'elle a fait jusqu'à ce \w jour|strong="H3117"\w*. 
\v 9 C'est pourquoi \w Lévi|strong="H3878"\w* n'a ni \w part|strong="H2506"\w* ni \w héritage|strong="H5159"\w* avec ses \w frères|strong="H251"\w*: l'\w Éternel|strong="H3068"\w* est son \w héritage|strong="H5159"\w*, comme l'\w Éternel|strong="H3068"\w*, ton \w Dieu|strong="H430"\w*, le lui a \w dit|strong="H1696" x-morph="strongMorph:TH8765"\w*. 

\s5
\v 10 Je \w restai|strong="H5975" x-morph="strongMorph:TH8804"\w* sur la \w montagne|strong="H2022"\w*, comme \w précédemment|strong="H7223"\w*\w |strong="H3117"\w*, \w quarante|strong="H705"\w* \w jours|strong="H3117"\w* et \w quarante|strong="H705"\w* \w nuits|strong="H3915"\w*. L'\w Éternel|strong="H3068"\w* m'\w exauça|strong="H8085" x-morph="strongMorph:TH8799"\w* encore cette \w fois|strong="H6471"\w*; l'\w Éternel|strong="H3068"\w* ne \w voulut|strong="H14" x-morph="strongMorph:TH8804"\w* pas te \w détruire|strong="H7843" x-morph="strongMorph:TH8687"\w*. 
\v 11 L'\w Éternel|strong="H3068"\w* me \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: \w Lève|strong="H6965" x-morph="strongMorph:TH8798"\w*-toi, va, \w marche|strong="H3212" x-morph="strongMorph:TH8798"\w*\w |strong="H4550"\w* à la \w tête|strong="H6440"\w* du \w peuple|strong="H5971"\w*. Qu'ils \w aillent|strong="H935" x-morph="strongMorph:TH8799"\w* prendre \w possession|strong="H3423" x-morph="strongMorph:TH8799"\w* du \w pays|strong="H776"\w* que j'ai \w juré|strong="H7650" x-morph="strongMorph:TH8738"\w* à leurs \w pères|strong="H1"\w* de leur \w donner|strong="H5414" x-morph="strongMorph:TH8800"\w*. 

\s5
\v 12 Maintenant, \w Israël|strong="H3478"\w*, que \w demande|strong="H7592" x-morph="strongMorph:TH8802"\w* de toi l'\w Éternel|strong="H3068"\w*, ton \w Dieu|strong="H430"\w*, si ce n'est que tu \w craignes|strong="H3372" x-morph="strongMorph:TH8800"\w* l'\w Éternel|strong="H3068"\w*, ton \w Dieu|strong="H430"\w*, afin de \w marcher|strong="H3212" x-morph="strongMorph:TH8800"\w* dans toutes ses \w voies|strong="H1870"\w*, d'\w aimer|strong="H157" x-morph="strongMorph:TH8800"\w* et de \w servir|strong="H5647" x-morph="strongMorph:TH8800"\w* l'\w Éternel|strong="H3068"\w*, ton \w Dieu|strong="H430"\w*, de tout ton \w coeur|strong="H3824"\w* et de toute ton \w âme|strong="H5315"\w*; 
\v 13 si ce n'est que tu \w observes|strong="H8104" x-morph="strongMorph:TH8800"\w* les \w commandements|strong="H4687"\w* de l'\w Éternel|strong="H3068"\w* et ses \w lois|strong="H2708"\w* que je te \w prescris|strong="H6680" x-morph="strongMorph:TH8764"\w* aujourd'\w hui|strong="H3117"\w*, afin que tu sois \w heureux|strong="H2896"\w*? 

\s5
\v 14 Voici, à l'\w Éternel|strong="H3068"\w*, ton \w Dieu|strong="H430"\w*, appartiennent les \w cieux|strong="H8064"\w* et les \w cieux|strong="H8064"\w* des \w cieux|strong="H8064"\w*, la \w terre|strong="H776"\w* et tout ce qu'elle renferme. 
\v 15 Et c'est à tes \w pères|strong="H1"\w* seulement que l'\w Éternel|strong="H3068"\w* s'est \w attaché|strong="H2836" x-morph="strongMorph:TH8804"\w* pour les \w aimer|strong="H157" x-morph="strongMorph:TH8800"\w*; et, \w après|strong="H310"\w* eux, c'est leur \w postérité|strong="H2233"\w*, c'est vous qu'il a \w choisis|strong="H977" x-morph="strongMorph:TH8799"\w* d'entre tous les \w peuples|strong="H5971"\w*, comme vous le voyez aujourd'\w hui|strong="H3117"\w*. 

\s5
\v 16 Vous \w circoncirez|strong="H4135" x-morph="strongMorph:TH8804"\w* donc votre \w coeur|strong="H3824"\w*\w |strong="H6190"\w*, et vous ne \w roidirez|strong="H7185" x-morph="strongMorph:TH8686"\w* plus votre \w cou|strong="H6203"\w*. 
\v 17 Car l'\w Éternel|strong="H3068"\w*, votre \w Dieu|strong="H430"\w*, est le \w Dieu|strong="H430"\w* des \w dieux|strong="H430"\w*, le \w Seigneur|strong="H113"\w* des \w seigneurs|strong="H113"\w*, le \w Dieu|strong="H410"\w* \w grand|strong="H1419"\w*, \w fort|strong="H1368"\w* et \w terrible|strong="H3372" x-morph="strongMorph:TH8737"\w*, qui ne fait point \w acception|strong="H5375" x-morph="strongMorph:TH8799"\w* des \w personnes|strong="H6440"\w* et qui ne \w reçoit|strong="H3947" x-morph="strongMorph:TH8799"\w* point de \w présent|strong="H7810"\w*, 

\s5
\v 18 qui \w fait|strong="H6213" x-morph="strongMorph:TH8802"\w* \w droit|strong="H4941"\w* à l'\w orphelin|strong="H3490"\w* et à la \w veuve|strong="H490"\w*, qui \w aime|strong="H157" x-morph="strongMorph:TH8802"\w* l'\w étranger|strong="H1616"\w* et lui \w donne|strong="H5414" x-morph="strongMorph:TH8800"\w* de la \w nourriture|strong="H3899"\w* et des \w vêtements|strong="H8071"\w*. 
\v 19 Vous \w aimerez|strong="H157" x-morph="strongMorph:TH8804"\w* l'\w étranger|strong="H1616"\w*, car vous avez été \w étrangers|strong="H1616"\w* dans le \w pays|strong="H776"\w* d'\w Égypte|strong="H4714"\w*. 

\s5
\v 20 Tu \w craindras|strong="H3372" x-morph="strongMorph:TH8799"\w* l'\w Éternel|strong="H3068"\w*, ton \w Dieu|strong="H430"\w*, tu le \w serviras|strong="H5647" x-morph="strongMorph:TH8799"\w*, tu t'\w attacheras|strong="H1692" x-morph="strongMorph:TH8799"\w* à lui, et tu \w jureras|strong="H7650" x-morph="strongMorph:TH8735"\w* par son \w nom|strong="H8034"\w*. 
\v 21 Il est ta \w gloire|strong="H8416"\w*, il est ton \w Dieu|strong="H430"\w*: c'est lui qui a \w fait|strong="H6213" x-morph="strongMorph:TH8804"\w* au milieu de toi ces choses \w grandes|strong="H1419"\w* et \w terribles|strong="H3372" x-morph="strongMorph:TH8737"\w* que tes \w yeux|strong="H5869"\w* ont \w vues|strong="H7200" x-morph="strongMorph:TH8804"\w*. 

\s5
\v 22 Tes \w pères|strong="H1"\w* \w descendirent|strong="H3381" x-morph="strongMorph:TH8804"\w* en \w Égypte|strong="H4714"\w* au nombre de soixante-\w dix|strong="H7657"\w* \w personnes|strong="H5315"\w*; et maintenant l'\w Éternel|strong="H3068"\w*, ton \w Dieu|strong="H430"\w*, a \w fait|strong="H7760" x-morph="strongMorph:TH8804"\w* de toi une \w multitude|strong="H7230"\w* pareille aux \w étoiles|strong="H3556"\w* des \w cieux|strong="H8064"\w*. 

\s5
\c 11  
\p
\v 1 Tu \w aimeras|strong="H157" x-morph="strongMorph:TH8804"\w* l'\w Éternel|strong="H3068"\w*, ton \w Dieu|strong="H430"\w*, et tu \w observeras|strong="H8104" x-morph="strongMorph:TH8804"\w* \w toujours|strong="H3117"\w* ses \w préceptes|strong="H4931"\w*, ses \w lois|strong="H2708"\w*, ses \w ordonnances|strong="H4941"\w* et ses \w commandements|strong="H4687"\w*. 

\s5
\v 2  \w Reconnaissez|strong="H3045" x-morph="strongMorph:TH8804"\w* aujourd'\w hui|strong="H3117"\w*-ce que n'ont pu \w connaître|strong="H3045" x-morph="strongMorph:TH8804"\w* et \w voir|strong="H7200" x-morph="strongMorph:TH8804"\w* vos \w enfants|strong="H1121"\w*-les \w châtiments|strong="H4148"\w* de l'\w Éternel|strong="H3068"\w*, votre \w Dieu|strong="H430"\w*, sa \w grandeur|strong="H1433"\w*, sa \w main|strong="H3027"\w* \w forte|strong="H2389"\w* et son \w bras|strong="H2220"\w* \w étendu|strong="H5186" x-morph="strongMorph:TH8803"\w*, 
\v 3 ses \w signes|strong="H226"\w* et ses \w actes|strong="H4639"\w* qu'il a \w accomplis|strong="H6213" x-morph="strongMorph:TH8804"\w* au \w milieu|strong="H8432"\w* de l'\w Égypte|strong="H4714"\w* contre \w Pharaon|strong="H6547"\w*, \w roi|strong="H4428"\w* d'\w Égypte|strong="H4714"\w*, et contre tout son \w pays|strong="H776"\w*. 

\s5
\v 4 Reconnaissez ce qu'\w il|strong="H3068"\w* a \w fait|strong="H6213" x-morph="strongMorph:TH8804"\w* à l'\w armée|strong="H2428"\w* d'\w Égypte|strong="H4714"\w*, à ses \w chevaux|strong="H5483"\w* et à ses \w chars|strong="H7393"\w*, comment il a fait \w couler|strong="H6687" x-morph="strongMorph:TH8689"\w* sur \w eux|strong="H6440"\w* les \w eaux|strong="H4325"\w* de la \w mer|strong="H3220"\w* \w Rouge|strong="H5488"\w*, lorsqu'ils vous \w poursuivaient|strong="H7291" x-morph="strongMorph:TH8800"\w*\w |strong="H310"\w*, et les a \w détruits|strong="H6" x-morph="strongMorph:TH8762"\w* pour \w toujours|strong="H3117"\w*; 
\v 5 ce qu'il vous a \w fait|strong="H6213" x-morph="strongMorph:TH8804"\w* dans le \w désert|strong="H4057"\w*, jusqu'à votre \w arrivée|strong="H935" x-morph="strongMorph:TH8800"\w* en ce \w lieu|strong="H4725"\w*; 

\s5
\v 6 ce qu'il a \w fait|strong="H6213" x-morph="strongMorph:TH8804"\w* à \w Dathan|strong="H1885"\w* et à \w Abiram|strong="H48"\w*, \w fils|strong="H1121"\w* d'\w Éliab|strong="H446"\w*, \w fils|strong="H1121"\w* de \w Ruben|strong="H7205"\w*, comment la \w terre|strong="H776"\w* \w ouvrit|strong="H6475" x-morph="strongMorph:TH8804"\w* sa \w bouche|strong="H6310"\w* et les \w engloutit|strong="H1104" x-morph="strongMorph:TH8799"\w*, avec leurs \w maisons|strong="H1004"\w* et leurs \w tentes|strong="H168"\w* et tout ce qui était à leur \w suite|strong="H3351"\w*\w |strong="H7272"\w*, au \w milieu|strong="H7130"\w* de tout \w Israël|strong="H3478"\w*. 
\v 7 Car vos \w yeux|strong="H5869"\w* ont \w vu|strong="H7200" x-morph="strongMorph:TH8802"\w* toutes les \w grandes|strong="H1419"\w* \w choses|strong="H4639"\w* que l'\w Éternel|strong="H3068"\w* a \w faites|strong="H6213" x-morph="strongMorph:TH8804"\w*. 

\s5
\v 8 Ainsi, vous \w observerez|strong="H8104" x-morph="strongMorph:TH8804"\w* tous les \w commandements|strong="H4687"\w* \w que|strong="H834"\w* je vous \w prescris|strong="H6680" x-morph="strongMorph:TH8764"\w* aujourd'\w hui|strong="H3117"\w*, afin que vous ayez la \w force|strong="H2388" x-morph="strongMorph:TH8799"\w* de vous \w emparer|strong="H935" x-morph="strongMorph:TH8804"\w*\w |strong="H3423" x-morph="strongMorph:TH8804"\w* du \w pays|strong="H776"\w* où vous allez \w passer|strong="H5674" x-morph="strongMorph:TH8802"\w* pour en prendre \w possession|strong="H3423" x-morph="strongMorph:TH8800"\w*, 
\v 9 et afin que vous \w prolongiez|strong="H748" x-morph="strongMorph:TH8686"\w* vos \w jours|strong="H3117"\w* dans le \w pays|strong="H127"\w* que l'\w Éternel|strong="H3068"\w* a \w juré|strong="H7650" x-morph="strongMorph:TH8738"\w* à vos \w pères|strong="H1"\w* de leur \w donner|strong="H5414" x-morph="strongMorph:TH8800"\w*, à eux et à leur \w postérité|strong="H2233"\w*, \w pays|strong="H776"\w* où \w coulent|strong="H2100" x-morph="strongMorph:TH8802"\w* le \w lait|strong="H2461"\w* et le \w miel|strong="H1706"\w*. 

\s5
\v 10 Car le \w pays|strong="H776"\w* dont tu vas \w entrer|strong="H935" x-morph="strongMorph:TH8802"\w* en \w possession|strong="H3423" x-morph="strongMorph:TH8800"\w*, n'est pas comme le \w pays|strong="H776"\w* d'\w Égypte|strong="H4714"\w*, d'où vous êtes \w sortis|strong="H3318" x-morph="strongMorph:TH8804"\w*, où tu \w jetais|strong="H2232" x-morph="strongMorph:TH8799"\w* dans les champs ta \w semence|strong="H2233"\w* et les \w arrosais|strong="H8248" x-morph="strongMorph:TH8689"\w* avec ton \w pied|strong="H7272"\w* comme un \w jardin|strong="H1588"\w* \w potager|strong="H3419"\w*. 
\v 11 Le \w pays|strong="H776"\w* que vous \w allez|strong="H5674" x-morph="strongMorph:TH8802"\w* \w posséder|strong="H3423" x-morph="strongMorph:TH8800"\w* est un \w pays|strong="H776"\w* de \w montagnes|strong="H2022"\w* et de \w vallées|strong="H1237"\w*, et qui \w boit|strong="H8354" x-morph="strongMorph:TH8799"\w* les \w eaux|strong="H4325"\w* de la \w pluie|strong="H4306"\w* du \w ciel|strong="H8064"\w*; 
\v 12 c'est un \w pays|strong="H776"\w* dont l'\w Éternel|strong="H3068"\w*, ton \w Dieu|strong="H430"\w*, prend \w soin|strong="H1875" x-morph="strongMorph:TH8802"\w*, et sur lequel l'\w Éternel|strong="H3068"\w*, ton \w Dieu|strong="H430"\w*, a \w continuellement|strong="H8548"\w* les \w yeux|strong="H5869"\w*, du \w commencement|strong="H7225"\w*\w |strong="H8141"\w* à la \w fin|strong="H319"\w* de l'\w année|strong="H8141"\w*. 

\s5
\v 13 Si vous \w obéissez|strong="H8085" x-morph="strongMorph:TH8799"\w*\w |strong="H8085" x-morph="strongMorph:TH8800"\w* à mes \w commandements|strong="H4687"\w* que je vous \w prescris|strong="H6680" x-morph="strongMorph:TH8764"\w* aujourd'\w hui|strong="H3117"\w*, si vous \w aimez|strong="H157" x-morph="strongMorph:TH8800"\w* l'\w Éternel|strong="H3068"\w*, votre \w Dieu|strong="H430"\w*, et si vous le \w servez|strong="H5647" x-morph="strongMorph:TH8800"\w* de tout votre \w coeur|strong="H3824"\w* et de toute votre \w âme|strong="H5315"\w*, 
\v 14 je \w donnerai|strong="H5414" x-morph="strongMorph:TH8804"\w* à votre \w pays|strong="H776"\w* la \w pluie|strong="H4306"\w* en son \w temps|strong="H6256"\w*, la pluie de la \w première|strong="H3138"\w* et de l'\w arrière|strong="H4456"\w*-saison, et tu \w recueilleras|strong="H622" x-morph="strongMorph:TH8804"\w* ton \w blé|strong="H1715"\w*, ton \w moût|strong="H8492"\w* et ton \w huile|strong="H3323"\w*; 
\v 15 je \w mettrai|strong="H5414" x-morph="strongMorph:TH8804"\w* aussi dans tes \w champs|strong="H7704"\w* de l'\w herbe|strong="H6212"\w* pour ton \w bétail|strong="H929"\w*, et tu \w mangeras|strong="H398" x-morph="strongMorph:TH8804"\w* et te \w rassasieras|strong="H7646" x-morph="strongMorph:TH8804"\w*. 

\s5
\v 16  \w Gardez|strong="H8104" x-morph="strongMorph:TH8734"\w*-vous de laisser \w séduire|strong="H6601" x-morph="strongMorph:TH8799"\w* votre \w coeur|strong="H3824"\w*, de vous \w détourner|strong="H5493" x-morph="strongMorph:TH8804"\w*, de \w servir|strong="H5647" x-morph="strongMorph:TH8804"\w* d'\w autres|strong="H312"\w* \w dieux|strong="H430"\w* et de vous \w prosterner|strong="H7812" x-morph="strongMorph:TH8694"\w* devant eux. 
\v 17 La \w colère|strong="H639"\w* de l'\w Éternel|strong="H3068"\w* s'\w enflammerait|strong="H2734" x-morph="strongMorph:TH8804"\w* alors contre vous; il \w fermerait|strong="H6113" x-morph="strongMorph:TH8804"\w* les \w cieux|strong="H8064"\w*, et il n'y aurait point de \w pluie|strong="H4306"\w*; la \w terre|strong="H127"\w* ne \w donnerait|strong="H5414" x-morph="strongMorph:TH8799"\w* plus ses \w produits|strong="H2981"\w*, et vous \w péririez|strong="H6" x-morph="strongMorph:TH8804"\w* \w promptement|strong="H4120"\w* dans le \w bon|strong="H2896"\w* \w pays|strong="H776"\w* que l'\w Éternel|strong="H3068"\w* vous \w donne|strong="H5414" x-morph="strongMorph:TH8802"\w*. 

\s5
