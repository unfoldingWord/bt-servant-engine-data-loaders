\s5
\c 31  
\p
\v 1  \w Moïse|strong="H4872"\w* \w adressa|strong="H3212" x-morph="strongMorph:TH8799"\w*\w |strong="H1696" x-morph="strongMorph:TH8762"\w* encore ces \w paroles|strong="H1697"\w* à tout \w Israël|strong="H3478"\w*: 
\v 2 Aujourd'\w hui|strong="H3117"\w*, leur \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*-il, je suis \w âgé|strong="H1121"\w* de \w cent|strong="H3967"\w* \w vingt|strong="H6242"\w* \w ans|strong="H8141"\w*, je ne \w pourrai|strong="H3201" x-morph="strongMorph:TH8799"\w* plus \w sortir|strong="H3318" x-morph="strongMorph:TH8800"\w* et \w entrer|strong="H935" x-morph="strongMorph:TH8800"\w*, et l'\w Éternel|strong="H3068"\w* m'a \w dit|strong="H559" x-morph="strongMorph:TH8804"\w*: Tu ne \w passeras|strong="H5674" x-morph="strongMorph:TH8799"\w* pas ce \w Jourdain|strong="H3383"\w*. 
\v 3 L'\w Éternel|strong="H3068"\w*, ton \w Dieu|strong="H430"\w*, \w marchera|strong="H5674" x-morph="strongMorph:TH8802"\w* lui-même \w devant|strong="H6440"\w* toi, il \w détruira|strong="H8045" x-morph="strongMorph:TH8686"\w* ces \w nations|strong="H1471"\w* \w devant|strong="H6440"\w* toi, et tu t'en rendras \w maître|strong="H3423" x-morph="strongMorph:TH8804"\w*. \w Josué|strong="H3091"\w* \w marchera|strong="H5674" x-morph="strongMorph:TH8802"\w* aussi \w devant|strong="H6440"\w* toi, comme l'\w Éternel|strong="H3068"\w* l'a \w dit|strong="H1696" x-morph="strongMorph:TH8765"\w*. 

\s5
\v 4 L'\w Éternel|strong="H3068"\w* \w traitera|strong="H6213" x-morph="strongMorph:TH8804"\w* ces nations comme il a \w traité|strong="H6213" x-morph="strongMorph:TH8804"\w* \w Sihon|strong="H5511"\w* et \w Og|strong="H5747"\w*, \w rois|strong="H4428"\w* des \w Amoréens|strong="H567"\w*, qu'il a \w détruits|strong="H8045" x-morph="strongMorph:TH8689"\w* avec leur \w pays|strong="H776"\w*. 
\v 5 L'\w Éternel|strong="H3068"\w* vous les \w livrera|strong="H5414" x-morph="strongMorph:TH8804"\w*\w |strong="H6440"\w*, et vous \w agirez|strong="H6213" x-morph="strongMorph:TH8804"\w* à leur égard selon tous les \w ordres|strong="H4687"\w* que je vous ai \w donnés|strong="H6680" x-morph="strongMorph:TH8765"\w*. 
\v 6  \w Fortifiez|strong="H2388" x-morph="strongMorph:TH8798"\w*-vous et ayez du \w courage|strong="H553" x-morph="strongMorph:TH8798"\w*! Ne \w craignez|strong="H3372" x-morph="strongMorph:TH8799"\w* point et ne soyez point \w effrayés|strong="H6206" x-morph="strongMorph:TH8799"\w* \w devant|strong="H6440"\w* eux; car l'\w Éternel|strong="H3068"\w*, ton \w Dieu|strong="H430"\w*, \w marchera|strong="H1980" x-morph="strongMorph:TH8802"\w* lui-même avec toi, il ne te \w délaissera|strong="H7503" x-morph="strongMorph:TH8686"\w* point, il ne t'\w abandonnera|strong="H5800" x-morph="strongMorph:TH8799"\w* point. 

\s5
\v 7  \w Moïse|strong="H4872"\w* \w appela|strong="H7121" x-morph="strongMorph:TH8799"\w* \w Josué|strong="H3091"\w*, et lui \w dit|strong="H559" x-morph="strongMorph:TH8799"\w* en \w présence|strong="H5869"\w* de tout \w Israël|strong="H3478"\w*: \w Fortifie|strong="H2388" x-morph="strongMorph:TH8798"\w*-toi et prends \w courage|strong="H553" x-morph="strongMorph:TH8798"\w*, car tu \w entreras|strong="H935" x-morph="strongMorph:TH8799"\w* avec ce \w peuple|strong="H5971"\w* dans le \w pays|strong="H776"\w* que l'\w Éternel|strong="H3068"\w* a \w juré|strong="H7650" x-morph="strongMorph:TH8738"\w* à leurs \w pères|strong="H1"\w* de leur \w donner|strong="H5414" x-morph="strongMorph:TH8800"\w*, et c'est toi qui les en mettras en \w possession|strong="H5157" x-morph="strongMorph:TH8686"\w*. 
\v 8 L'\w Éternel|strong="H3068"\w* \w marchera|strong="H1980" x-morph="strongMorph:TH8802"\w* lui-même \w devant|strong="H6440"\w* toi, il sera lui-même avec toi, il ne te \w délaissera|strong="H7503" x-morph="strongMorph:TH8686"\w* point, il ne t'\w abandonnera|strong="H5800" x-morph="strongMorph:TH8799"\w* point; ne \w crains|strong="H3372" x-morph="strongMorph:TH8799"\w* point, et ne t'\w effraie|strong="H2865" x-morph="strongMorph:TH8735"\w* point. 

\s5
\v 9  \w Moïse|strong="H4872"\w* \w écrivit|strong="H3789" x-morph="strongMorph:TH8799"\w* cette \w loi|strong="H8451"\w*, et il la \w remit|strong="H5414" x-morph="strongMorph:TH8799"\w* aux \w sacrificateurs|strong="H3548"\w*, \w fils|strong="H1121"\w* de \w Lévi|strong="H3878"\w*, qui \w portaient|strong="H5375" x-morph="strongMorph:TH8802"\w* l'\w arche|strong="H727"\w* de l'\w alliance|strong="H1285"\w* de l'\w Éternel|strong="H3068"\w*, et à tous les \w anciens|strong="H2205"\w* d'\w Israël|strong="H3478"\w*. 
\v 10  \w Moïse|strong="H4872"\w* leur donna cet \w ordre|strong="H6680" x-morph="strongMorph:TH8762"\w*\w |strong="H559" x-morph="strongMorph:TH8800"\w*:\w |strong="H7093"\w* Tous les \w sept|strong="H7651"\w* \w ans|strong="H8141"\w*, à l'\w époque|strong="H4150"\w* de l'\w année|strong="H8141"\w* du \w relâche|strong="H8059"\w*, à la \w fête|strong="H2282"\w* des \w tabernacles|strong="H5521"\w*, 
\v 11 quand tout \w Israël|strong="H3478"\w* \w viendra|strong="H935" x-morph="strongMorph:TH8800"\w* se \w présenter|strong="H7200" x-morph="strongMorph:TH8736"\w* \w devant|strong="H6440"\w* l'\w Éternel|strong="H3068"\w*, ton \w Dieu|strong="H430"\w*, dans le \w lieu|strong="H4725"\w* qu'il \w choisira|strong="H977" x-morph="strongMorph:TH8799"\w*, tu \w liras|strong="H7121" x-morph="strongMorph:TH8799"\w* cette \w loi|strong="H8451"\w* devant tout \w Israël|strong="H3478"\w*, en leur \w présence|strong="H241"\w*. 

\s5
\v 12 Tu \w rassembleras|strong="H6950" x-morph="strongMorph:TH8685"\w* le \w peuple|strong="H5971"\w*, les \w hommes|strong="H582"\w*, les \w femmes|strong="H802"\w*, les \w enfants|strong="H2945"\w*, et l'\w étranger|strong="H1616"\w* qui sera dans tes \w portes|strong="H8179"\w*, afin qu'ils t'\w entendent|strong="H8085" x-morph="strongMorph:TH8799"\w*, et afin qu'ils \w apprennent|strong="H3925" x-morph="strongMorph:TH8799"\w* à \w craindre|strong="H3372" x-morph="strongMorph:TH8804"\w* l'\w Éternel|strong="H3068"\w*, votre \w Dieu|strong="H430"\w*, à \w observer|strong="H8104" x-morph="strongMorph:TH8804"\w* et à mettre en \w pratique|strong="H6213" x-morph="strongMorph:TH8800"\w* toutes les \w paroles|strong="H1697"\w* de cette \w loi|strong="H8451"\w*. 
\v 13 Et leurs \w enfants|strong="H1121"\w* qui ne la \w connaîtront|strong="H3045" x-morph="strongMorph:TH8804"\w* pas l'\w entendront|strong="H8085" x-morph="strongMorph:TH8799"\w*, et ils \w apprendront|strong="H3925" x-morph="strongMorph:TH8804"\w* à \w craindre|strong="H3372" x-morph="strongMorph:TH8800"\w* l'\w Éternel|strong="H3068"\w*, votre \w Dieu|strong="H430"\w*, tout le \w temps|strong="H3117"\w* que vous \w vivrez|strong="H2416"\w* dans le \w pays|strong="H127"\w* dont vous prendrez \w possession|strong="H3423" x-morph="strongMorph:TH8800"\w*, après avoir \w passé|strong="H5674" x-morph="strongMorph:TH8802"\w* le \w Jourdain|strong="H3383"\w*. 

\s5
\v 14 L'\w Éternel|strong="H3068"\w* \w dit|strong="H559" x-morph="strongMorph:TH8799"\w* à \w Moïse|strong="H4872"\w*: Voici, le \w moment|strong="H3117"\w* \w approche|strong="H7126" x-morph="strongMorph:TH8804"\w* où tu vas \w mourir|strong="H4191" x-morph="strongMorph:TH8800"\w*. \w Appelle|strong="H7121" x-morph="strongMorph:TH8798"\w* \w Josué|strong="H3091"\w*, et \w présentez|strong="H3320" x-morph="strongMorph:TH8690"\w*-vous dans la \w tente|strong="H168"\w* d'\w assignation|strong="H4150"\w*. Je lui donnerai mes \w ordres|strong="H6680" x-morph="strongMorph:TH8762"\w*. \w Moïse|strong="H4872"\w* et \w Josué|strong="H3091"\w* \w allèrent|strong="H3212" x-morph="strongMorph:TH8799"\w* se \w présenter|strong="H3320" x-morph="strongMorph:TH8691"\w* dans la \w tente|strong="H168"\w* d'\w assignation|strong="H4150"\w*. 
