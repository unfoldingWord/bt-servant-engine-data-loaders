\s5
\v 5  \w Éluzaï|strong="H498"\w*; \w Jerimoth|strong="H3406"\w*; \w Bealia|strong="H1183"\w*; \w Schemaria|strong="H8114"\w*; \w Schephathia|strong="H8203"\w*, de \w Haroph|strong="H2741"\w*; 
