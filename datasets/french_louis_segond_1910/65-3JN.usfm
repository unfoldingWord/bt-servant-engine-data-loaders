\v 10 C'est \w pourquoi|strong="G1223"\w*\w |strong="G5124"\w*, \w si|strong="G1437"\w* je vais vous \w voir|strong="G2064" x-morph="strongMorph:TG5632"\w*, je \w rappellerai|strong="G5279" x-morph="strongMorph:TG5692"\w*\w |strong="G846"\w* les \w actes|strong="G2041"\w* \w qu|strong="G3739"\w*'il \w commet|strong="G4160" x-morph="strongMorph:TG5719"\w*, en tenant \w contre|strong="G5396" x-morph="strongMorph:TG5723"\w* \w nous|strong="G2248"\w* de \w méchants|strong="G4190"\w* \w propos|strong="G3056"\w*;\w |strong="G2532"\w* \w non|strong="G3361"\w* \w content|strong="G714" x-morph="strongMorph:TG5746"\w* \w de|strong="G1909"\w* \w cela|strong="G5125"\w*, \w il|strong="G846"\w* ne \w reçoit|strong="G1926" x-morph="strongMorph:TG5736"\w* \w pas|strong="G3777"\w* les \w frères|strong="G80"\w*, \w et|strong="G2532"\w* ceux qui \w voudraient|strong="G1014" x-morph="strongMorph:TG5740"\w* le faire, il les en \w empêche|strong="G2967" x-morph="strongMorph:TG5719"\w* \w et|strong="G2532"\w* les \w chasse|strong="G1544" x-morph="strongMorph:TG5719"\w* \w de|strong="G1537"\w* l'\w Église|strong="G1577"\w*. 

\s5
\v 11 Bien-\w aimé|strong="G27"\w*, n'\w imite|strong="G3401" x-morph="strongMorph:TG5737"\w* \w pas|strong="G3361"\w* le \w mal|strong="G2556"\w*, \w mais|strong="G235"\w* le \w bien|strong="G18"\w*. Celui qui fait le \w bien|strong="G15" x-morph="strongMorph:TG5723"\w* \w est|strong="G2076" x-morph="strongMorph:TG5748"\w* \w de|strong="G1537"\w* \w Dieu|strong="G2316"\w*;\w |strong="G1161"\w* celui qui fait le \w mal|strong="G2554" x-morph="strongMorph:TG5723"\w* n'a \w point|strong="G3756"\w* \w vu|strong="G3708" x-morph="strongMorph:TG5758"\w* \w Dieu|strong="G2316"\w*. 
\v 12  \w Tous|strong="G3956"\w*\w |strong="G5259"\w*, \w et|strong="G2532"\w*\w |strong="G5259"\w* la \w vérité|strong="G225"\w* elle-\w même|strong="G846"\w*, rendent un bon \w témoignage|strong="G3140" x-morph="strongMorph:TG5769"\w* à \w Démétrius|strong="G1216"\w*;\w |strong="G1161"\w* \w nous|strong="G2249"\w* \w aussi|strong="G2532"\w*, nous lui rendons \w témoignage|strong="G3140" x-morph="strongMorph:TG5719"\w*, \w et|strong="G2532"\w* tu \w sais|strong="G1492" x-morph="strongMorph:TG5758"\w* \w que|strong="G3754"\w* \w notre|strong="G2257"\w* \w témoignage|strong="G3141"\w* \w est|strong="G2076" x-morph="strongMorph:TG5748"\w* \w vrai|strong="G227"\w*. 

\s5
\v 13 J'\w aurais|strong="G2192" x-morph="strongMorph:TG5707"\w* beaucoup de \w choses|strong="G4183"\w* à \w t|strong="G4671"\w*'\w écrire|strong="G1125" x-morph="strongMorph:TG5721"\w*, \w mais|strong="G235"\w* je ne \w veux|strong="G2309" x-morph="strongMorph:TG5719"\w* \w pas|strong="G3756"\w* le \w faire|strong="G1125" x-morph="strongMorph:TG5658"\w* \w avec|strong="G1223"\w* l'\w encre|strong="G3188"\w* \w et|strong="G2532"\w* la \w plume|strong="G2563"\w*. 
\v 14  \w J|strong="G1161"\w*'\w espère|strong="G1679" x-morph="strongMorph:TG5719"\w* \w te|strong="G4571"\w* \w voir|strong="G1492" x-morph="strongMorph:TG5629"\w* \w bientôt|strong="G2112"\w*, \w et|strong="G2532"\w* nous \w parlerons|strong="G2980" x-morph="strongMorph:TG5692"\w* de \w bouche|strong="G4750"\w* \w à|strong="G4314"\w* \w bouche|strong="G4750"\w*. Que la \w paix|strong="G1515"\w* soit avec \w toi|strong="G4671"\w*! Les \w amis|strong="G5384"\w* \w te|strong="G4571"\w* \w saluent|strong="G782" x-morph="strongMorph:TG5736"\w*. \w Salue|strong="G782" x-morph="strongMorph:TG5737"\w* les \w amis|strong="G5384"\w*, chacun en \w particulier|strong="G2596"\w*\w |strong="G3686"\w*. 