\mt1 LE LIVRE DES JUGES 

\s5
\c 1  
\p
\v 1  \w Après|strong="H310"\w* la \w mort|strong="H4194"\w* de \w Josué|strong="H3091"\w*, les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w* \w consultèrent|strong="H7592" x-morph="strongMorph:TH8799"\w* l'\w Éternel|strong="H3068"\w*, en \w disant|strong="H559" x-morph="strongMorph:TH8800"\w*: Qui de nous \w montera|strong="H5927" x-morph="strongMorph:TH8799"\w* le \w premier|strong="H8462"\w* contre les \w Cananéens|strong="H3669"\w*, pour les \w attaquer|strong="H3898" x-morph="strongMorph:TH8736"\w*? 
\v 2 L'\w Éternel|strong="H3068"\w* \w répondit|strong="H559" x-morph="strongMorph:TH8799"\w*: \w Juda|strong="H3063"\w* \w montera|strong="H5927" x-morph="strongMorph:TH8799"\w*, voici, j'ai \w livré|strong="H5414" x-morph="strongMorph:TH8804"\w* le \w pays|strong="H776"\w* entre ses \w mains|strong="H3027"\w*. 
\v 3 Et \w Juda|strong="H3063"\w* \w dit|strong="H559" x-morph="strongMorph:TH8799"\w* à \w Siméon|strong="H8095"\w*, son \w frère|strong="H251"\w*: \w Monte|strong="H5927" x-morph="strongMorph:TH8798"\w* avec moi dans le pays qui m'est échu par le \w sort|strong="H1486"\w*, et nous \w combattrons|strong="H3898" x-morph="strongMorph:TH8735"\w* les \w Cananéens|strong="H3669"\w*; j'\w irai|strong="H1980" x-morph="strongMorph:TH8804"\w* aussi avec toi dans celui qui t'est tombé en \w partage|strong="H1486"\w*. Et \w Siméon|strong="H8095"\w* \w alla|strong="H3212" x-morph="strongMorph:TH8799"\w* avec lui. 

\s5
\v 4  \w Juda|strong="H3063"\w* \w monta|strong="H5927" x-morph="strongMorph:TH8799"\w*, et l'\w Éternel|strong="H3068"\w* \w livra|strong="H5414" x-morph="strongMorph:TH8799"\w* entre leurs \w mains|strong="H3027"\w* les \w Cananéens|strong="H3669"\w* et les \w Phéréziens|strong="H6522"\w*; ils \w battirent|strong="H5221" x-morph="strongMorph:TH8686"\w* \w dix|strong="H6235"\w* \w mille|strong="H505"\w* \w hommes|strong="H376"\w* à \w Bézek|strong="H966"\w*. 
\v 5 Ils \w trouvèrent|strong="H4672" x-morph="strongMorph:TH8799"\w* Adoni-\w Bézek|strong="H137"\w* à \w Bézek|strong="H966"\w*; ils l'\w attaquèrent|strong="H3898" x-morph="strongMorph:TH8735"\w*, et ils \w battirent|strong="H5221" x-morph="strongMorph:TH8686"\w* les \w Cananéens|strong="H3669"\w* et les \w Phéréziens|strong="H6522"\w*. 

\s5
\v 6 Adoni-\w Bézek|strong="H137"\w* prit la \w fuite|strong="H5127" x-morph="strongMorph:TH8799"\w*; mais ils le \w poursuivirent|strong="H7291" x-morph="strongMorph:TH8799"\w*\w |strong="H310"\w* et le \w saisirent|strong="H270" x-morph="strongMorph:TH8799"\w*, et ils lui \w coupèrent|strong="H7112" x-morph="strongMorph:TH8762"\w* les \w pouces|strong="H931"\w* des \w mains|strong="H3027"\w* et des \w pieds|strong="H7272"\w*. 
\v 7 Adoni-\w Bézek|strong="H137"\w* \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: Soixante-\w dix|strong="H7657"\w* \w rois|strong="H4428"\w*, ayant les \w pouces|strong="H931"\w* des \w mains|strong="H3027"\w* et des \w pieds|strong="H7272"\w* \w coupés|strong="H7112" x-morph="strongMorph:TH8794"\w*, \w ramassaient|strong="H3950" x-morph="strongMorph:TH8764"\w* sous ma \w table|strong="H7979"\w*; \w Dieu|strong="H430"\w* me \w rend|strong="H7999" x-morph="strongMorph:TH8765"\w* ce que j'ai \w fait|strong="H6213" x-morph="strongMorph:TH8804"\w*. On l'\w emmena|strong="H935" x-morph="strongMorph:TH8686"\w* à \w Jérusalem|strong="H3389"\w*, et il y \w mourut|strong="H4191" x-morph="strongMorph:TH8799"\w*. 

\s5
\v 8 Les \w fils|strong="H1121"\w* de \w Juda|strong="H3063"\w* \w attaquèrent|strong="H3898" x-morph="strongMorph:TH8735"\w* \w Jérusalem|strong="H3389"\w* et la \w prirent|strong="H3920" x-morph="strongMorph:TH8799"\w*, ils la \w frappèrent|strong="H5221" x-morph="strongMorph:TH8686"\w* du \w tranchant|strong="H6310"\w* de l'\w épée|strong="H2719"\w* et \w mirent|strong="H7971" x-morph="strongMorph:TH8765"\w* le \w feu|strong="H784"\w* à la \w ville|strong="H5892"\w*. 
\v 9 Les \w fils|strong="H1121"\w* de \w Juda|strong="H3063"\w* \w descendirent|strong="H3381" x-morph="strongMorph:TH8804"\w* \w ensuite|strong="H310"\w*, pour \w combattre|strong="H3898" x-morph="strongMorph:TH8736"\w* les \w Cananéens|strong="H3669"\w* qui \w habitaient|strong="H3427" x-morph="strongMorph:TH8802"\w* la \w montagne|strong="H2022"\w*, la contrée du \w midi|strong="H5045"\w* et la \w plaine|strong="H8219"\w*. 
\v 10  \w Juda|strong="H3063"\w* \w marcha|strong="H3212" x-morph="strongMorph:TH8799"\w* contre les \w Cananéens|strong="H3669"\w* qui \w habitaient|strong="H3427" x-morph="strongMorph:TH8802"\w* à \w Hébron|strong="H2275"\w*, \w appelée|strong="H8034"\w*\w |strong="H2275"\w* \w autrefois|strong="H6440"\w* Kirjath-\w Arba|strong="H7153"\w*; et il \w battit|strong="H5221" x-morph="strongMorph:TH8686"\w* \w Schéschaï|strong="H8344"\w*, \w Ahiman|strong="H289"\w* et \w Talmaï|strong="H8526"\w*. 

\s5
\v 11 De là il \w marcha|strong="H3212" x-morph="strongMorph:TH8799"\w* contre les \w habitants|strong="H3427" x-morph="strongMorph:TH8802"\w* de \w Debir|strong="H1688"\w*: \w Debir|strong="H1688"\w* s'\w appelait|strong="H8034"\w* \w autrefois|strong="H6440"\w* Kirjath-\w Sépher|strong="H7158"\w*. 
\v 12  \w Caleb|strong="H3612"\w* \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: Je \w donnerai|strong="H5414" x-morph="strongMorph:TH8804"\w* ma \w fille|strong="H1323"\w* \w Acsa|strong="H5915"\w* pour \w femme|strong="H802"\w* à celui qui \w battra|strong="H5221" x-morph="strongMorph:TH8686"\w* Kirjath-\w Sépher|strong="H7158"\w* et qui la \w prendra|strong="H3920" x-morph="strongMorph:TH8804"\w*. 
\v 13  \w Othniel|strong="H6274"\w*, \w fils|strong="H1121"\w* de \w Kenaz|strong="H7073"\w*, \w frère|strong="H251"\w* \w cadet|strong="H6996"\w* de \w Caleb|strong="H3612"\w*, s'en \w empara|strong="H3920" x-morph="strongMorph:TH8799"\w*; et Caleb lui \w donna|strong="H5414" x-morph="strongMorph:TH8799"\w* pour \w femme|strong="H802"\w* sa \w fille|strong="H1323"\w* \w Acsa|strong="H5915"\w*. 

\s5
\v 14 Lorsqu'elle fut \w entrée|strong="H935" x-morph="strongMorph:TH8800"\w* chez Othniel, elle le \w sollicita|strong="H5496" x-morph="strongMorph:TH8686"\w* de \w demander|strong="H7592" x-morph="strongMorph:TH8800"\w* à son \w père|strong="H1"\w* un \w champ|strong="H7704"\w*. Elle \w descendit|strong="H6795" x-morph="strongMorph:TH8799"\w* de dessus son \w âne|strong="H2543"\w*; et \w Caleb|strong="H3612"\w* lui \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: Qu'as-tu? 
\v 15 Elle lui \w répondit|strong="H559" x-morph="strongMorph:TH8799"\w*: \w Fais|strong="H3051" x-morph="strongMorph:TH8798"\w*-moi un \w présent|strong="H1293"\w*, car tu m'as \w donné|strong="H5414" x-morph="strongMorph:TH8804"\w* une \w terre|strong="H776"\w* du \w midi|strong="H5045"\w*; \w donne|strong="H5414" x-morph="strongMorph:TH8804"\w*-moi aussi des \w sources|strong="H1543"\w* d'\w eau|strong="H4325"\w*. Et \w Caleb|strong="H3612"\w* lui \w donna|strong="H5414" x-morph="strongMorph:TH8799"\w* les \w sources|strong="H1543"\w* \w supérieures|strong="H5942"\w* et les \w sources|strong="H1543"\w* \w inférieures|strong="H8482"\w*. 

\s5
\v 16 Les \w fils|strong="H1121"\w* du \w Kénien|strong="H7017"\w*, beau-\w père|strong="H2859" x-morph="strongMorph:TH8802"\w* de \w Moïse|strong="H4872"\w*, \w montèrent|strong="H5927" x-morph="strongMorph:TH8804"\w* de la \w ville|strong="H5892"\w* des \w palmiers|strong="H8558" x-morph="strongMorph:TH8677"\w*\w |strong="H5899"\w*, avec les \w fils|strong="H1121"\w* de \w Juda|strong="H3063"\w*, dans le \w désert|strong="H4057"\w* de \w Juda|strong="H3063"\w* au \w midi|strong="H5045"\w* d'\w Arad|strong="H6166"\w*, et ils \w allèrent|strong="H3212" x-morph="strongMorph:TH8799"\w* s'\w établir|strong="H3427" x-morph="strongMorph:TH8799"\w* \w parmi|strong="H854"\w* le \w peuple|strong="H5971"\w*. 
\v 17  \w Juda|strong="H3063"\w* se mit en \w marche|strong="H3212" x-morph="strongMorph:TH8799"\w* avec \w Siméon|strong="H8095"\w*, son \w frère|strong="H251"\w*, et ils \w battirent|strong="H5221" x-morph="strongMorph:TH8686"\w* les \w Cananéens|strong="H3669"\w* qui \w habitaient|strong="H3427" x-morph="strongMorph:TH8802"\w* à \w Tsephath|strong="H6857"\w*; ils \w dévouèrent|strong="H2763"\w*\w \w* la \w ville|strong="H5892"\w* par \w interdit|strong="H2763" x-morph="strongMorph:TH8686"\w*, et on l'\w appela|strong="H7121" x-morph="strongMorph:TH8799"\w*\w |strong="H8034"\w* \w Horma|strong="H2767"\w*. 

\s5
\v 18  \w Juda|strong="H3063"\w* s'\w empara|strong="H3920" x-morph="strongMorph:TH8799"\w* encore de \w Gaza|strong="H5804"\w* et de son \w territoire|strong="H1366"\w*, d'\w Askalon|strong="H831"\w* et de son \w territoire|strong="H1366"\w*, et d'\w Ékron|strong="H6138"\w* et de son \w territoire|strong="H1366"\w*. 
\v 19 L'\w Éternel|strong="H3068"\w* fut avec \w Juda|strong="H3063"\w*; et Juda se rendit \w maître|strong="H3423" x-morph="strongMorph:TH8686"\w* de la \w montagne|strong="H2022"\w*, mais il ne \w put|strong="H3808"\w* \w chasser|strong="H3423" x-morph="strongMorph:TH8687"\w* les \w habitants|strong="H3427" x-morph="strongMorph:TH8802"\w* de la \w plaine|strong="H6010"\w*, parce qu'ils avaient des \w chars|strong="H7393"\w* de \w fer|strong="H1270"\w*. 

\s5
\v 20 On \w donna|strong="H5414" x-morph="strongMorph:TH8799"\w* \w Hébron|strong="H2275"\w* à \w Caleb|strong="H3612"\w*, comme l'avait \w dit|strong="H1696" x-morph="strongMorph:TH8765"\w* \w Moïse|strong="H4872"\w*; et il en \w chassa|strong="H3423" x-morph="strongMorph:TH8686"\w* les \w trois|strong="H7969"\w* \w fils|strong="H1121"\w* d'\w Anak|strong="H6061"\w*. 
\v 21 Les \w fils|strong="H1121"\w* de \w Benjamin|strong="H1144"\w* ne \w chassèrent|strong="H3423" x-morph="strongMorph:TH8689"\w* point les \w Jébusiens|strong="H2983"\w* qui \w habitaient|strong="H3427" x-morph="strongMorph:TH8802"\w* à \w Jérusalem|strong="H3389"\w*; et les \w Jébusiens|strong="H2983"\w* ont \w habité|strong="H3427" x-morph="strongMorph:TH8799"\w* jusqu'à ce \w jour|strong="H3117"\w* dans \w Jérusalem|strong="H3389"\w* avec les \w fils|strong="H1121"\w* de \w Benjamin|strong="H1144"\w*. 

\s5
\v 22 La \w maison|strong="H1004"\w* de \w Joseph|strong="H3130"\w* \w monta|strong="H5927" x-morph="strongMorph:TH8799"\w* aussi contre \w Béthel|strong="H1008"\w*, et l'\w Éternel|strong="H3068"\w* fut avec eux. 
\v 23 La \w maison|strong="H1004"\w* de \w Joseph|strong="H3130"\w* fit \w explorer|strong="H8446" x-morph="strongMorph:TH8686"\w* \w Béthel|strong="H1008"\w*, qui s'\w appelait|strong="H8034"\w*\w |strong="H5892"\w* \w autrefois|strong="H6440"\w* \w Luz|strong="H3870"\w*. 
\v 24 Les \w gardes|strong="H8104" x-morph="strongMorph:TH8802"\w* \w virent|strong="H7200" x-morph="strongMorph:TH8799"\w* un \w homme|strong="H376"\w* qui \w sortait|strong="H3318" x-morph="strongMorph:TH8802"\w* de la \w ville|strong="H5892"\w*, et ils lui \w dirent|strong="H559" x-morph="strongMorph:TH8799"\w*: \w Montre|strong="H7200" x-morph="strongMorph:TH8685"\w*-nous par où nous pourrons \w entrer|strong="H3996"\w* dans la \w ville|strong="H5892"\w*, et nous te \w ferons|strong="H6213" x-morph="strongMorph:TH8804"\w* \w grâce|strong="H2617"\w*. 

\s5
\v 25 Il leur \w montra|strong="H7200" x-morph="strongMorph:TH8686"\w* par où ils pourraient \w entrer|strong="H3996"\w* dans la \w ville|strong="H5892"\w*. Et ils \w frappèrent|strong="H5221" x-morph="strongMorph:TH8686"\w* la \w ville|strong="H5892"\w* du \w tranchant|strong="H6310"\w* de l'\w épée|strong="H2719"\w*; mais ils laissèrent \w aller|strong="H7971" x-morph="strongMorph:TH8765"\w* cet \w homme|strong="H376"\w* et toute sa \w famille|strong="H4940"\w*. 
\v 26 Cet \w homme|strong="H376"\w* se \w rendit|strong="H3212" x-morph="strongMorph:TH8799"\w* dans le \w pays|strong="H776"\w* des \w Héthiens|strong="H2850"\w*; il \w bâtit|strong="H1129" x-morph="strongMorph:TH8799"\w* une \w ville|strong="H5892"\w*, et lui \w donna|strong="H7121" x-morph="strongMorph:TH8799"\w* le \w nom|strong="H8034"\w* de \w Luz|strong="H3870"\w*, \w nom|strong="H8034"\w* qu'elle a porté jusqu'à ce \w jour|strong="H3117"\w*. 

\s5
\v 27  \w Manassé|strong="H4519"\w* ne \w chassa|strong="H3423" x-morph="strongMorph:TH8689"\w* point les \w habitants|strong="H3427" x-morph="strongMorph:TH8802"\w* de Beth-\w Schean|strong="H1052"\w* et des \w villes|strong="H1323"\w* de son ressort, de \w Thaanac|strong="H8590"\w* et des \w villes|strong="H1323"\w* de son ressort, de \w Dor|strong="H1756"\w* et des \w villes|strong="H1323"\w* de son ressort,\w |strong="H3427" x-morph="strongMorph:TH8802"\w* de \w Jibleam|strong="H2991"\w* et des \w villes|strong="H1323"\w* de son ressort,\w |strong="H3427" x-morph="strongMorph:TH8802"\w* de \w Meguiddo|strong="H4023"\w* et des \w villes|strong="H1323"\w* de son ressort; et les \w Cananéens|strong="H3669"\w* \w voulurent|strong="H2974" x-morph="strongMorph:TH8686"\w* \w rester|strong="H3427" x-morph="strongMorph:TH8800"\w* dans ce \w pays|strong="H776"\w*. 
\v 28 Lorsqu'\w Israël|strong="H3478"\w* fut assez \w fort|strong="H2388" x-morph="strongMorph:TH8804"\w*, il \w assujettit|strong="H7760" x-morph="strongMorph:TH8799"\w* les \w Cananéens|strong="H3669"\w* à un \w tribut|strong="H4522"\w*, mais il ne les \w chassa|strong="H3423" x-morph="strongMorph:TH8687"\w*\w |strong="H3423" x-morph="strongMorph:TH8689"\w* point. 

\s5
\v 29  \w Éphraïm|strong="H669"\w* ne \w chassa|strong="H3423" x-morph="strongMorph:TH8689"\w* point les \w Cananéens|strong="H3669"\w* qui \w habitaient|strong="H3427" x-morph="strongMorph:TH8802"\w* à \w Guézer|strong="H1507"\w*, et les \w Cananéens|strong="H3669"\w* \w habitèrent|strong="H3427" x-morph="strongMorph:TH8799"\w* au \w milieu|strong="H7130"\w* d'Éphraïm à \w Guézer|strong="H1507"\w*. 

\s5
\v 30  \w Zabulon|strong="H2074"\w* ne \w chassa|strong="H3423" x-morph="strongMorph:TH8689"\w* point les \w habitants|strong="H3427" x-morph="strongMorph:TH8802"\w* de \w Kitron|strong="H7003"\w*, ni les \w habitants|strong="H3427" x-morph="strongMorph:TH8802"\w* de \w Nahalol|strong="H5096"\w*; et les \w Cananéens|strong="H3669"\w* \w habitèrent|strong="H3427" x-morph="strongMorph:TH8799"\w* au \w milieu|strong="H7130"\w* de Zabulon, mais ils furent assujettis à un \w tribut|strong="H4522"\w*. 

\s5
\v 31  \w Aser|strong="H836"\w* ne \w chassa|strong="H3423" x-morph="strongMorph:TH8689"\w* point les \w habitants|strong="H3427" x-morph="strongMorph:TH8802"\w* d'\w Acco|strong="H5910"\w*, ni les \w habitants|strong="H3427" x-morph="strongMorph:TH8802"\w* de \w Sidon|strong="H6721"\w*, ni ceux d'\w Achlal|strong="H303"\w*, d'\w Aczib|strong="H392"\w*, de \w Helba|strong="H2462"\w*, d'\w Aphik|strong="H663"\w* et de \w Rehob|strong="H7340"\w*; 
\v 32 et les \w Asérites|strong="H843"\w* \w habitèrent|strong="H3427" x-morph="strongMorph:TH8799"\w* au \w milieu|strong="H7130"\w* des \w Cananéens|strong="H3669"\w*, \w habitants|strong="H3427" x-morph="strongMorph:TH8802"\w* du \w pays|strong="H776"\w*, car ils ne les \w chassèrent|strong="H3423" x-morph="strongMorph:TH8689"\w* point. 

\s5
\v 33  \w Nephthali|strong="H5321"\w* ne \w chassa|strong="H3423" x-morph="strongMorph:TH8689"\w* point les \w habitants|strong="H3427" x-morph="strongMorph:TH8802"\w* de Beth-\w Schémesch|strong="H1053"\w*, ni les \w habitants|strong="H3427" x-morph="strongMorph:TH8802"\w* de Beth-\w Anath|strong="H1043"\w*, et il \w habita|strong="H3427" x-morph="strongMorph:TH8799"\w* au \w milieu|strong="H7130"\w* des \w Cananéens|strong="H3669"\w*, \w habitants|strong="H3427" x-morph="strongMorph:TH8802"\w* du \w pays|strong="H776"\w*, mais les \w habitants|strong="H3427" x-morph="strongMorph:TH8802"\w* de Beth-\w Schémesch|strong="H1053"\w* et de Beth-\w Anath|strong="H1043"\w* furent assujettis à un \w tribut|strong="H4522"\w*. 

\s5
\v 34 Les \w Amoréens|strong="H567"\w* \w repoussèrent|strong="H3905" x-morph="strongMorph:TH8799"\w* dans la \w montagne|strong="H2022"\w* les \w fils|strong="H1121"\w* de \w Dan|strong="H1835"\w*, et ne les \w laissèrent|strong="H5414" x-morph="strongMorph:TH8804"\w* pas \w descendre|strong="H3381" x-morph="strongMorph:TH8800"\w* dans la \w plaine|strong="H6010"\w*. 
\v 35 Les \w Amoréens|strong="H567"\w* \w voulurent|strong="H2974" x-morph="strongMorph:TH8686"\w* \w rester|strong="H3427" x-morph="strongMorph:TH8800"\w* à Har-\w |strong="H2022"\w*\w Hérès|strong="H2776"\w*, à \w Ajalon|strong="H357"\w* et à \w Schaalbim|strong="H8169"\w*; mais la \w main|strong="H3027"\w* de la \w maison|strong="H1004"\w* de \w Joseph|strong="H3130"\w* s'\w appesantit|strong="H3513" x-morph="strongMorph:TH8799"\w* sur eux, et ils furent assujettis à un \w tribut|strong="H4522"\w*. 
\v 36 Le \w territoire|strong="H1366"\w* des \w Amoréens|strong="H567"\w* s'étendait depuis la \w montée|strong="H4608"\w* d'\w Akrabbim|strong="H4610"\w*, depuis \w Séla|strong="H5553"\w*, et en \w dessus|strong="H4605"\w*. 

\s5
\c 2  
\p
\v 1 Un \w envoyé|strong="H4397"\w* de l'\w Éternel|strong="H3068"\w* \w monta|strong="H5927" x-morph="strongMorph:TH8799"\w* de \w Guilgal|strong="H1537"\w* à \w Bokim|strong="H1066"\w*, et \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: Je vous ai fait \w monter|strong="H5927" x-morph="strongMorph:TH8686"\w* hors d'\w Égypte|strong="H4714"\w*, et je vous ai \w amenés|strong="H935" x-morph="strongMorph:TH8686"\w* dans le \w pays|strong="H776"\w* que j'ai \w juré|strong="H7650" x-morph="strongMorph:TH8738"\w* à vos \w pères|strong="H1"\w* de vous donner. J'ai \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: \w Jamais|strong="H5769"\w* je ne \w romprai|strong="H6565" x-morph="strongMorph:TH8686"\w* mon \w alliance|strong="H1285"\w* avec vous; 
\v 2 et vous, vous ne \w traiterez|strong="H3772" x-morph="strongMorph:TH8799"\w* point \w alliance|strong="H1285"\w* avec les \w habitants|strong="H3427" x-morph="strongMorph:TH8802"\w* de ce \w pays|strong="H776"\w*, vous \w renverserez|strong="H5422" x-morph="strongMorph:TH8799"\w* leurs \w autels|strong="H4196"\w*. Mais vous n'avez point \w obéi|strong="H8085" x-morph="strongMorph:TH8804"\w* à ma \w voix|strong="H6963"\w*. Pourquoi avez-vous \w fait|strong="H6213" x-morph="strongMorph:TH8804"\w* cela? 

\s5
\v 3 J'ai \w dit|strong="H559" x-morph="strongMorph:TH8804"\w* alors: Je ne les \w chasserai|strong="H1644" x-morph="strongMorph:TH8762"\w* point \w devant|strong="H6440"\w* vous; mais ils seront à vos \w côtés|strong="H6654"\w*, et leurs \w dieux|strong="H430"\w* vous seront un \w piège|strong="H4170"\w*. 
\v 4 Lorsque l'\w envoyé|strong="H4397"\w* de l'\w Éternel|strong="H3068"\w* eut \w dit|strong="H1696" x-morph="strongMorph:TH8763"\w* ces \w paroles|strong="H1697"\w* à tous les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w*, le \w peuple|strong="H5971"\w* \w éleva|strong="H5375" x-morph="strongMorph:TH8799"\w* la \w voix|strong="H6963"\w* et \w pleura|strong="H1058" x-morph="strongMorph:TH8799"\w*. 
\v 5 Ils \w donnèrent|strong="H7121" x-morph="strongMorph:TH8799"\w* à ce \w lieu|strong="H4725"\w* le \w nom|strong="H8034"\w* de \w Bokim|strong="H1066"\w*, et ils y offrirent des \w sacrifices|strong="H2076" x-morph="strongMorph:TH8799"\w* à l'\w Éternel|strong="H3068"\w*. 

\s5
\v 6  \w Josué|strong="H3091"\w* \w renvoya|strong="H7971" x-morph="strongMorph:TH8762"\w* le \w peuple|strong="H5971"\w*, et les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w* \w allèrent|strong="H3212" x-morph="strongMorph:TH8799"\w* \w chacun|strong="H376"\w* dans son \w héritage|strong="H5159"\w* pour prendre \w possession|strong="H3423" x-morph="strongMorph:TH8800"\w* du \w pays|strong="H776"\w*. 
\v 7 Le \w peuple|strong="H5971"\w* \w servit|strong="H5647" x-morph="strongMorph:TH8799"\w* l'\w Éternel|strong="H3068"\w* pendant toute la \w vie|strong="H3117"\w* de \w Josué|strong="H3091"\w*, et pendant toute la \w vie|strong="H3117"\w* des \w anciens|strong="H2205"\w* qui \w survécurent|strong="H748" x-morph="strongMorph:TH8689"\w*\w |strong="H3117"\w*\w |strong="H310"\w* à \w Josué|strong="H3091"\w* et qui avaient \w vu|strong="H7200" x-morph="strongMorph:TH8804"\w* toutes les \w grandes|strong="H1419"\w* \w choses|strong="H4639"\w* que l'\w Éternel|strong="H3068"\w* avait \w faites|strong="H6213" x-morph="strongMorph:TH8804"\w* en faveur d'\w Israël|strong="H3478"\w*. 
\v 8  \w Josué|strong="H3091"\w*, \w fils|strong="H1121"\w* de \w Nun|strong="H5126"\w*, \w serviteur|strong="H5650"\w* de l'\w Éternel|strong="H3068"\w*, \w mourut|strong="H4191" x-morph="strongMorph:TH8799"\w* \w âgé|strong="H1121"\w* de \w cent|strong="H3967"\w* \w dix|strong="H6235"\w* \w ans|strong="H8141"\w*. 

\s5
\v 9 On l'\w ensevelit|strong="H6912" x-morph="strongMorph:TH8799"\w* dans le \w territoire|strong="H1366"\w* qu'il avait eu en \w partage|strong="H5159"\w*, à Thimnath-\w Hérès|strong="H8556"\w*, dans la \w montagne|strong="H2022"\w* d'\w Éphraïm|strong="H669"\w*, au \w nord|strong="H6828"\w* de la \w montagne|strong="H2022"\w* de \w Gaasch|strong="H1608"\w*. 
\v 10 Toute cette \w génération|strong="H1755"\w* fut \w recueillie|strong="H622" x-morph="strongMorph:TH8738"\w* auprès de ses \w pères|strong="H1"\w*, et il s'\w éleva|strong="H6965" x-morph="strongMorph:TH8799"\w* \w après|strong="H310"\w* elle une \w autre|strong="H312"\w* \w génération|strong="H1755"\w*, qui ne \w connaissait|strong="H3045" x-morph="strongMorph:TH8804"\w* point l'\w Éternel|strong="H3068"\w*, ni \w ce|strong="H4639"\w* qu'il avait \w fait|strong="H6213" x-morph="strongMorph:TH8804"\w* en faveur d'\w Israël|strong="H3478"\w*. 

\s5
\v 11 Les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w* \w firent|strong="H6213" x-morph="strongMorph:TH8799"\w* alors ce qui \w déplaît|strong="H7451"\w*\w |strong="H5869"\w* à l'\w Éternel|strong="H3068"\w*, et ils \w servirent|strong="H5647" x-morph="strongMorph:TH8799"\w* les \w Baals|strong="H1168"\w*. 
\v 12 Ils \w abandonnèrent|strong="H5800" x-morph="strongMorph:TH8799"\w* l'\w Éternel|strong="H3068"\w*, le \w Dieu|strong="H430"\w* de leurs \w pères|strong="H1"\w*, qui les avait fait \w sortir|strong="H3318" x-morph="strongMorph:TH8688"\w* du \w pays|strong="H776"\w* d'\w Égypte|strong="H4714"\w*, et ils \w allèrent|strong="H3212" x-morph="strongMorph:TH8799"\w* \w après|strong="H310"\w* d'\w autres|strong="H312"\w* \w dieux|strong="H430"\w* d'entre les \w dieux|strong="H430"\w* des \w peuples|strong="H5971"\w* qui les \w entouraient|strong="H5439"\w*; ils se \w prosternèrent|strong="H7812" x-morph="strongMorph:TH8691"\w* devant eux, et ils \w irritèrent|strong="H3707" x-morph="strongMorph:TH8686"\w* l'\w Éternel|strong="H3068"\w*. 
\v 13 Ils \w abandonnèrent|strong="H5800" x-morph="strongMorph:TH8799"\w* l'\w Éternel|strong="H3068"\w*, et ils \w servirent|strong="H5647" x-morph="strongMorph:TH8799"\w* \w Baal|strong="H1168"\w* et les \w Astartés|strong="H6252"\w*. 

\s5
\v 14 La \w colère|strong="H639"\w* de l'\w Éternel|strong="H3068"\w* s'\w enflamma|strong="H2734" x-morph="strongMorph:TH8799"\w* contre \w Israël|strong="H3478"\w*. Il les \w livra|strong="H5414" x-morph="strongMorph:TH8799"\w* entre les \w mains|strong="H3027"\w* de \w pillards|strong="H8154" x-morph="strongMorph:TH8802"\w* qui les \w pillèrent|strong="H8155" x-morph="strongMorph:TH8799"\w*, il les \w vendit|strong="H4376" x-morph="strongMorph:TH8799"\w* entre les \w mains|strong="H3027"\w* de leurs \w ennemis|strong="H341" x-morph="strongMorph:TH8802"\w* d'\w alentour|strong="H5439"\w*, et ils ne \w purent|strong="H3201" x-morph="strongMorph:TH8804"\w* \w plus|strong="H5750"\w* \w résister|strong="H5975" x-morph="strongMorph:TH8800"\w*\w |strong="H6440"\w* à leurs \w ennemis|strong="H341" x-morph="strongMorph:TH8802"\w*. 
\v 15 Partout où ils \w allaient|strong="H3318" x-morph="strongMorph:TH8804"\w*, la \w main|strong="H3027"\w* de l'\w Éternel|strong="H3068"\w* était contre eux pour leur faire du \w mal|strong="H7451"\w*, comme l'\w Éternel|strong="H3068"\w* l'avait \w dit|strong="H1696" x-morph="strongMorph:TH8765"\w*, comme l'\w Éternel|strong="H3068"\w* le leur avait \w juré|strong="H7650" x-morph="strongMorph:TH8738"\w*. Ils furent ainsi dans une \w grande|strong="H3966"\w* \w détresse|strong="H3334" x-morph="strongMorph:TH8799"\w*. 

\s5
\v 16 L'\w Éternel|strong="H3068"\w* \w suscita|strong="H6965" x-morph="strongMorph:TH8686"\w* des \w juges|strong="H8199" x-morph="strongMorph:TH8802"\w*, afin qu'ils les \w délivrassent|strong="H3467" x-morph="strongMorph:TH8686"\w* de la \w main|strong="H3027"\w* de ceux qui les \w pillaient|strong="H8154" x-morph="strongMorph:TH8802"\w*. 
\v 17 Mais ils n'\w écoutèrent|strong="H8085" x-morph="strongMorph:TH8804"\w* pas même leurs \w juges|strong="H8199" x-morph="strongMorph:TH8802"\w*, car ils se \w prostituèrent|strong="H2181" x-morph="strongMorph:TH8804"\w* \w à|strong="H310"\w* d'\w autres|strong="H312"\w* \w dieux|strong="H430"\w*, se \w prosternèrent|strong="H7812" x-morph="strongMorph:TH8691"\w* devant eux. Ils se \w détournèrent|strong="H5493" x-morph="strongMorph:TH8804"\w* \w promptement|strong="H4118"\w* de la \w voie|strong="H1870"\w* qu'avaient \w suivie|strong="H1980" x-morph="strongMorph:TH8804"\w* leurs \w pères|strong="H1"\w*, et ils n'\w obéirent|strong="H8085" x-morph="strongMorph:TH8800"\w*\w |strong="H6213" x-morph="strongMorph:TH8804"\w* point comme eux aux \w commandements|strong="H4687"\w* de l'\w Éternel|strong="H3068"\w*. 

\s5
\v 18 Lorsque l'\w Éternel|strong="H3068"\w* leur \w suscitait|strong="H6965" x-morph="strongMorph:TH8689"\w* des \w juges|strong="H8199" x-morph="strongMorph:TH8802"\w*, l'\w Éternel|strong="H3068"\w* était avec le \w juge|strong="H8199" x-morph="strongMorph:TH8802"\w*, et il les \w délivrait|strong="H3467" x-morph="strongMorph:TH8689"\w* de la \w main|strong="H3027"\w* de leurs \w ennemis|strong="H341" x-morph="strongMorph:TH8802"\w* pendant toute la \w vie|strong="H3117"\w* du \w juge|strong="H8199" x-morph="strongMorph:TH8802"\w*; car l'\w Éternel|strong="H3068"\w* avait \w pitié|strong="H5162" x-morph="strongMorph:TH8735"\w* \w de|strong="H6440"\w* leurs \w gémissements|strong="H5009"\w* contre ceux qui les \w opprimaient|strong="H3905" x-morph="strongMorph:TH8801"\w* et les \w tourmentaient|strong="H1766" x-morph="strongMorph:TH8802"\w*. 
\v 19 Mais, à la \w mort|strong="H4194"\w* du \w juge|strong="H8199" x-morph="strongMorph:TH8802"\w*, ils se \w corrompaient|strong="H7843" x-morph="strongMorph:TH8689"\w* de \w nouveau|strong="H7725" x-morph="strongMorph:TH8799"\w* plus que leurs \w pères|strong="H1"\w*, en \w allant|strong="H3212" x-morph="strongMorph:TH8800"\w* \w après|strong="H310"\w* d'\w autres|strong="H312"\w* \w dieux|strong="H430"\w* pour les \w servir|strong="H5647" x-morph="strongMorph:TH8800"\w* et se \w prosterner|strong="H7812" x-morph="strongMorph:TH8692"\w* devant eux, et ils \w persévéraient|strong="H5307" x-morph="strongMorph:TH8689"\w* dans la même \w conduite|strong="H4611"\w* et le même \w endurcissement|strong="H7186"\w*\w |strong="H1870"\w*. 

\s5
\v 20 Alors la \w colère|strong="H639"\w* de l'\w Éternel|strong="H3068"\w* s'\w enflamma|strong="H2734" x-morph="strongMorph:TH8799"\w* contre \w Israël|strong="H3478"\w*, et il \w dit|strong="H559" x-morph="strongMorph:TH8799"\w*: Puisque cette \w nation|strong="H1471"\w* a \w transgressé|strong="H5674" x-morph="strongMorph:TH8804"\w* mon \w alliance|strong="H1285"\w* que j'avais \w prescrite|strong="H6680" x-morph="strongMorph:TH8765"\w* à ses \w pères|strong="H1"\w*, et puisqu'ils n'ont point \w obéi|strong="H8085" x-morph="strongMorph:TH8804"\w* à ma \w voix|strong="H6963"\w*, 
\v 21 je ne \w chasserai|strong="H3423" x-morph="strongMorph:TH8687"\w* \w plus|strong="H3254" x-morph="strongMorph:TH8686"\w* \w devant|strong="H6440"\w* eux \w aucune|strong="H376"\w* des \w nations|strong="H1471"\w* que \w Josué|strong="H3091"\w* \w laissa|strong="H5800" x-morph="strongMorph:TH8804"\w* quand il \w mourut|strong="H4191" x-morph="strongMorph:TH8799"\w*. 
\v 22 C'est ainsi que je mettrai par elles \w Israël|strong="H3478"\w* à l'\w épreuve|strong="H5254" x-morph="strongMorph:TH8763"\w*, pour savoir s'ils prendront \w garde|strong="H8104" x-morph="strongMorph:TH8802"\w* ou non de \w suivre|strong="H3212" x-morph="strongMorph:TH8800"\w* la \w voie|strong="H1870"\w* de l'\w Éternel|strong="H3068"\w*, comme leurs \w pères|strong="H1"\w* y ont pris \w garde|strong="H8104" x-morph="strongMorph:TH8804"\w*. 
\v 23 Et l'\w Éternel|strong="H3068"\w* \w laissa|strong="H3240" x-morph="strongMorph:TH8686"\w* en repos ces \w nations|strong="H1471"\w* qu'il n'avait pas \w livrées|strong="H5414" x-morph="strongMorph:TH8804"\w* entre les \w mains|strong="H3027"\w* de \w Josué|strong="H3091"\w*, et il ne se \w hâta|strong="H4118"\w*\w |strong="H1115"\w* point de les \w chasser|strong="H3423" x-morph="strongMorph:TH8687"\w*. 

\s5
\c 3  
\p
\v 1 Voici les \w nations|strong="H1471"\w* que l'\w Éternel|strong="H3068"\w* \w laissa|strong="H3240" x-morph="strongMorph:TH8689"\w* pour \w éprouver|strong="H5254" x-morph="strongMorph:TH8763"\w* par elles \w Israël|strong="H3478"\w*, tous ceux qui n'avaient pas \w connu|strong="H3045" x-morph="strongMorph:TH8804"\w* toutes les \w guerres|strong="H4421"\w* de \w Canaan|strong="H3667"\w*. 
\v 2 Il voulait \w seulement|strong="H7535"\w* que les \w générations|strong="H1755"\w* des \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w* \w connussent|strong="H3045" x-morph="strongMorph:TH8800"\w* et \w apprissent|strong="H3925" x-morph="strongMorph:TH8763"\w* la \w guerre|strong="H4421"\w*, ceux qui ne l'avaient pas \w connue|strong="H3045" x-morph="strongMorph:TH8804"\w* \w auparavant|strong="H6440"\w*. 
\v 3 Ces nations étaient: les \w cinq|strong="H2568"\w* \w princes|strong="H5633"\w* des \w Philistins|strong="H6430"\w*, tous les \w Cananéens|strong="H3669"\w*, les \w Sidoniens|strong="H6722"\w*, et les \w Héviens|strong="H2340"\w* qui \w habitaient|strong="H3427" x-morph="strongMorph:TH8802"\w* la \w montagne|strong="H2022"\w* du \w Liban|strong="H3844"\w*, depuis la \w montagne|strong="H2022"\w* de Baal-\w Hermon|strong="H1179"\w* jusqu'à l'\w entrée|strong="H935" x-morph="strongMorph:TH8800"\w* de \w Hamath|strong="H2574"\w*. 

\s5
\v 4 Ces nations servirent à mettre \w Israël|strong="H3478"\w* à l'\w épreuve|strong="H5254" x-morph="strongMorph:TH8763"\w*, afin que l'\w Éternel|strong="H3068"\w* \w sût|strong="H3045" x-morph="strongMorph:TH8800"\w* s'ils \w obéiraient|strong="H8085" x-morph="strongMorph:TH8799"\w* aux \w commandements|strong="H4687"\w* qu'il avait \w prescrits|strong="H6680" x-morph="strongMorph:TH8765"\w* à leurs \w pères|strong="H1"\w* \w par|strong="H3027"\w* \w Moïse|strong="H4872"\w*. 
\v 5 Et les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w* \w habitèrent|strong="H3427" x-morph="strongMorph:TH8804"\w* au \w milieu|strong="H7130"\w* des \w Cananéens|strong="H3669"\w*, des \w Héthiens|strong="H2850"\w*, des \w Amoréens|strong="H567"\w*, des \w Phéréziens|strong="H6522"\w*, des \w Héviens|strong="H2340"\w* et des \w Jébusiens|strong="H2983"\w*; 
\v 6 ils \w prirent|strong="H3947" x-morph="strongMorph:TH8799"\w* leurs \w filles|strong="H1323"\w* pour \w femmes|strong="H802"\w*, ils \w donnèrent|strong="H5414" x-morph="strongMorph:TH8804"\w* à leurs \w fils|strong="H1121"\w* leurs propres \w filles|strong="H1323"\w*, et ils \w servirent|strong="H5647" x-morph="strongMorph:TH8799"\w* leurs \w dieux|strong="H430"\w*. 

\s5
\v 7 Les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w* \w firent|strong="H6213" x-morph="strongMorph:TH8799"\w* ce qui \w déplaît|strong="H7451"\w* \w à|strong="H5869"\w* l'\w Éternel|strong="H3068"\w*, ils \w oublièrent|strong="H7911" x-morph="strongMorph:TH8799"\w* l'\w Éternel|strong="H3068"\w*, et ils \w servirent|strong="H5647" x-morph="strongMorph:TH8799"\w* les \w Baals|strong="H1168"\w* et les \w idoles|strong="H842"\w*. 
\v 8 La \w colère|strong="H639"\w* de l'\w Éternel|strong="H3068"\w* s'\w enflamma|strong="H2734" x-morph="strongMorph:TH8799"\w* contre \w Israël|strong="H3478"\w*, et il les \w vendit|strong="H4376" x-morph="strongMorph:TH8799"\w* entre les \w mains|strong="H3027"\w* de Cuschan-\w Rischeathaïm|strong="H3573"\w*, \w roi|strong="H4428"\w* de \w Mésopotamie|strong="H763"\w*. Et les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w* furent \w asservis|strong="H5647" x-morph="strongMorph:TH8799"\w* \w huit|strong="H8083"\w* \w ans|strong="H8141"\w* à Cuschan-\w Rischeathaïm|strong="H3573"\w*. 

\s5
\v 9 Les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w* \w crièrent|strong="H2199" x-morph="strongMorph:TH8799"\w* à l'\w Éternel|strong="H3068"\w*, et l'\w Éternel|strong="H3068"\w* leur \w suscita|strong="H6965" x-morph="strongMorph:TH8686"\w* un \w libérateur|strong="H3467" x-morph="strongMorph:TH8688"\w* qui \w les|strong="H1121"\w*\w |strong="H3478"\w* \w délivra|strong="H3467" x-morph="strongMorph:TH8686"\w*, \w Othniel|strong="H6274"\w*, \w fils|strong="H1121"\w* de \w Kenaz|strong="H7073"\w*, \w frère|strong="H251"\w* \w cadet|strong="H6996"\w* de \w Caleb|strong="H3612"\w*. 
\v 10 L'\w esprit|strong="H7307"\w* de l'\w Éternel|strong="H3068"\w* fut sur lui. Il devint \w juge|strong="H8199" x-morph="strongMorph:TH8799"\w* en \w Israël|strong="H3478"\w*, et il \w partit|strong="H3318" x-morph="strongMorph:TH8799"\w* pour la \w guerre|strong="H4421"\w*. L'\w Éternel|strong="H3068"\w* \w livra|strong="H5414" x-morph="strongMorph:TH8799"\w* entre ses \w mains|strong="H3027"\w* Cuschan-\w Rischeathaïm|strong="H3573"\w*, \w roi|strong="H4428"\w* de \w Mésopotamie|strong="H763"\w*, et sa \w main|strong="H3027"\w* fut \w puissante|strong="H5810" x-morph="strongMorph:TH8799"\w* contre Cuschan-\w Rischeathaïm|strong="H3573"\w*. 
\v 11 Le \w pays|strong="H776"\w* fut en \w repos|strong="H8252" x-morph="strongMorph:TH8799"\w* pendant \w quarante|strong="H705"\w* \w ans|strong="H8141"\w*. Et \w Othniel|strong="H6274"\w*, \w fils|strong="H1121"\w* de \w Kenaz|strong="H7073"\w*, \w mourut|strong="H4191" x-morph="strongMorph:TH8799"\w*. 

\s5
\v 12 Les \w enfants|strong="H1121"\w* d'\w Israël|strong="H3478"\w* \w firent|strong="H6213" x-morph="strongMorph:TH8800"\w* \w encore|strong="H3254" x-morph="strongMorph:TH8686"\w* ce qui \w déplaît|strong="H7451"\w* \w à|strong="H5869"\w* l'\w Éternel|strong="H3068"\w*; et l'\w Éternel|strong="H3068"\w* \w fortifia|strong="H2388" x-morph="strongMorph:TH8762"\w* \w Églon|strong="H5700"\w*, \w roi|strong="H4428"\w* de \w Moab|strong="H4124"\w*, contre \w Israël|strong="H3478"\w*, parce qu'ils avaient \w fait|strong="H6213" x-morph="strongMorph:TH8804"\w* ce qui \w déplaît|strong="H7451"\w* \w à|strong="H5869"\w* l'\w Éternel|strong="H3068"\w*. 
